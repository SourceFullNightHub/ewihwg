                Library:MakeNotify({
                    Title = "Sitink Hub",
                    Content = "Turn On Toggle Auto Darkbeard!!!",
                    Description = "Auto Soul Guitar < Materials >",
                    Time = 5 
                })
                ToggleAutoDarkCoat:Set(true)
