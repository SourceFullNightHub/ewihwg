                Title_2.TextSize = 13.000
                Title_2.TextXAlignment = Enum.TextXAlignment.Left

                UIPadding_6.Parent = Title_2
                UIPadding_6.PaddingLeft = UDim.new(0, 7)

                CheckFrame.Name = "CheckFrame"
                CheckFrame.Parent = ToggleDisable
