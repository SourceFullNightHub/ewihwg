    while task.wait() do
        if Configs["Auto Farm"] then
            if Configs["Select Method Farm"] == "Cake Prince" then
                StatusFarm:SetTitle("Status Farm : Farm Cake Prince")
            elseif Configs["Select Method Farm"] == "Level Farm" then
                StatusFarm:SetTitle("Status Farm : Level Farm")
            elseif Configs["Select Method Farm"] == "Bone" then
                StatusFarm:SetTitle("Status Farm : Farm Bone")
            end
        else
            StatusFarm:SetTitle("Status Farm : Not Farm")
            StatusFarm:SetDesc("Status : Please Farm")
        end
    end
end)

