                CheckFrame.BorderColor3 = Color3.fromRGB(0, 0, 0)
                CheckFrame.BorderSizePixel = 0
                CheckFrame.Position = UDim2.new(0.725274742, 0, 0.21875, 0)
                CheckFrame.Size = UDim2.new(0, 39, 0, 17)

                UICorner_8.CornerRadius = UDim.new(1, 0)
                UICorner_8.Parent = CheckFrame

                Cricle.Name = "Cricle"
                Cricle.Parent = CheckFrame
                Cricle.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
                Cricle.BorderColor3 = Color3.fromRGB(0, 0, 0)
                Cricle.BorderSizePixel = 0
                Cricle.Position = UDim2.new(0, 1, 0, 0)
                Cricle.Size = UDim2.new(0, 17, 0, 17)

                UICorner_9.CornerRadius = UDim.new(1, 0)
                UICorner_9.Parent = Cricle

                Click_3.Name = "Click"
                Click_3.Parent = ToggleDisable
                Click_3.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
                Click_3.BackgroundTransparency = 1.000
                Click_3.BorderColor3 = Color3.fromRGB(0, 0, 0)
                Click_3.BorderSizePixel = 0
                Click_3.Size = UDim2.new(1, 0, 1, 0)
                Click_3.Font = Enum.Font.SourceSans
                Click_3.Text = ""
                Click_3.TextColor3 = Color3.fromRGB(0, 0, 0)
                Click_3.TextSize = 14.000

                function FuncToggle:Set(Value)
                    if Value then
                        TweenService:Create(
                            Title_2,
                            TweenInfo.new(0.2, Enum.EasingStyle.Quad, Enum.EasingDirection.Out),
                            {TextColor3 = Color3.fromRGB(255, 255, 255)}
                        ):Play()
                        TweenService:Create(
                            CheckFrame,
                            TweenInfo.new(0.2, Enum.EasingStyle.Quad, Enum.EasingDirection.Out),
                            {BackgroundColor3 = Color3.fromRGB(20, 20, 20)}
                        ):Play()
                        TweenService:Create(
                            Cricle,
                            TweenInfo.new(0.2, Enum.EasingStyle.Quad, Enum.EasingDirection.Out),
                            {BackgroundColor3 = Color3.fromRGB(125, 53, 119)}
                        ):Play()
                        TweenService:Create(
                            Cricle,
                            TweenInfo.new(0.4, Enum.EasingStyle.Quad, Enum.EasingDirection.Out),
                            {Position = UDim2.new(0, 22, 0, 0)}
                        ):Play()
                    else
                        TweenService:Create(
                            Title_2,
                            TweenInfo.new(0.2, Enum.EasingStyle.Quad, Enum.EasingDirection.Out),
                            {TextColor3 = Color3.fromRGB(170, 170, 170)}
                        ):Play()
                        TweenService:Create(
                            CheckFrame,
                            TweenInfo.new(0.2, Enum.EasingStyle.Quad, Enum.EasingDirection.Out),
                            {BackgroundColor3 = Color3.fromRGB(190, 190, 190)}
                        ):Play()
                        TweenService:Create(
                            Cricle,
                            TweenInfo.new(0.2, Enum.EasingStyle.Quad, Enum.EasingDirection.Out),
                            {BackgroundColor3 = Color3.fromRGB(255, 255, 255)}
                        ):Play()
                        TweenService:Create(
                            Cricle,
                            TweenInfo.new(0.4, Enum.EasingStyle.Quad, Enum.EasingDirection.Out),
                            {Position = UDim2.new(0, 1, 0, 0)}
                        ):Play()
                    end
                    Toggled = Value
                    cfToggle.Callback(Toggled)
                end

                Toggled = cfToggle.Default
				FuncToggle:Set(cfToggle.Default)

                Click_3.MouseButton1Click:Connect(function()
                    Toggled = not Toggled
                    FuncToggle:Set(Toggled)
                end)

                return FuncToggle
            end
            function SectionFunc:AddDropdown(cfDropdown)
                cfDropdown = cfDropdown or {}
                cfDropdown.Name = cfDropdown.Name or "Dropdown"
                cfDropdown.Options = cfDropdown.Options or {}
                cfDropdown.Default = cfDropdown.Default or ""
                cfDropdown.Callback = cfDropdown.Callback or function() end

                local Dropdown = Instance.new("Frame")
                local UICorner_17 = Instance.new("UICorner")
                local Title_6 = Instance.new("TextLabel")
                local UIPadding_11 = Instance.new("UIPadding")
                local DropMain = Instance.new("Frame")
                local UICorner_18 = Instance.new("UICorner")
                local NiggaArrow = Instance.new("ImageLabel")
                local List_5 = Instance.new("Frame")
                local Scroll = Instance.new("ScrollingFrame")
                local UIPadding_12 = Instance.new("UIPadding")
                local UIListLayout_6 = Instance.new("UIListLayout")
                local Selected = Instance.new("TextLabel")
                local UIPadding_14 = Instance.new("UIPadding")
                local Click_6 = Instance.new("TextButton")
                local DropFunc = {}

                Dropdown.Name = "Dropdown"
                Dropdown.Parent = List_2
                Dropdown.BackgroundColor3 = Color3.fromRGB(50, 50, 50)
                Dropdown.BackgroundTransparency = 1.000
                Dropdown.BorderColor3 = Color3.fromRGB(0, 0, 0)
                Dropdown.BorderSizePixel = 0
                Dropdown.ClipsDescendants = true
                Dropdown.Size = UDim2.new(1, 0, 0, 52)

                UICorner_17.CornerRadius = UDim.new(0, 3)
                UICorner_17.Parent = Dropdown

                Title_6.Name = "Title"
                Title_6.Parent = Dropdown
                Title_6.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
                Title_6.BackgroundTransparency = 1.000
                Title_6.BorderColor3 = Color3.fromRGB(0, 0, 0)
                Title_6.BorderSizePixel = 0
                Title_6.Size = UDim2.new(1, 0, 0, 20)
                Title_6.Font = Enum.Font.GothamBold
