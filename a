spawn(function()
    while wait() do
        if Bulon["Select Stats"] then
            if Bulon["Select Stats"] ~= nil and Bulon["Select Stats"] ~= "" then
                local args = {
                    [1] = "AddPoint",
                    [2] = Bulon["Select Stats"],
                    [3] = 9999
                }
                game:GetService("ReplicatedStorage").Remotes.CommF_:InvokeServer(unpack(args))
            end
        end
    end
end)

-- // Main Farm \\ --

ListFarm = {"Level Farm", "Cake Prince", "Bone"}
local DropdownMethodFarm = MainF:MakeDropdown({
    Name = "Select Method Farm",
    Description = "",
    List = ListFarm,
    Default = Bulon["Select Method Farm"],
    Callback = function(Value)
        Bulon["Select Method Farm"] = Value
        SaveSettings()
    end
})

local ToggleAutoFarm = MainF:MakeToggle({
    Name = "Auto Farm",
    Description = "",
    Default = Bulon["Auto Farm"],
    Setting = true,
    Callback = function(Value)
        Bulon["Auto Farm"] = Value
        SaveSettings()
        DisableTween(Bulon["Auto Farm"])
    end
})

local MobCastleHaunted = {
    "Reborn Skeleton",
    "Posessed Mummy",
    "Living Zombie",
    "Demonic Soul",
}

local MobCakePrince = {
    "Baking Staff",
    "Cookie Crafter",
    "Cake Guard",
    "Head Baker"
}

StackFunc = false
function AutoFarm()
    if StackFunc then
        return
    end
    if Bulon["Select Method Farm"] == "Bone" then
        if DetectMob("Reborn Skeleton") or DetectMob("Posessed Mummy") or DetectMob("Living Zombie") or DetectMob("Demonic Soul") then
            for r, v in pairs(WS.Enemies:GetChildren()) do
                if table.find(MobCastleHaunted, v.Name) and v:FindFirstChild("Humanoid") and v.Humanoid.Health > 0 and v:FindFirstChild("HumanoidRootPart") and v.Parent then
                    repeat wait()
                        KillMob(v.Name, Bulon["Select Method Farm"] ~= "Bone" or not Bulon["Auto Farm"])
                    until not v.Parent or not v:FindFirstChild("Humanoid") or v:FindFirstChild("Humanoid").Health <= 0 or not v:FindFirstChild("HumanoidRootPart") or Bulon["Select Method Farm"] ~= "Bone" or not Bulon["Auto Farm"] 
                end
            end
        else
            Tween(CFrame.new(-9516.99316, 172.017181, 6078.46533, 0, 0, -1, 0, 1, 0, 1, 0, 0))
        end
    elseif Bulon["Select Mode Farm"] == "Cake Prince" then
        if not GetGlassCakePrince() and DetectNigga("Cake Prince") then
            if DetectMob("Cake Prince") then
                v = DetectMob("Cake Prince")
                repeat wait()
                    KillMob("Cake Prince", Bulon["Select Mode Farm"] ~= "Cake Prince" or Bulon["Auto Farm"] == false or StackFunc)
                until not v.Parent or not v:FindFirstChild("Humanoid") or v:FindFirstChild("Humanoid").Health <= 0 or not v:FindFirstChild("HumanoidRootPart") or Bulon["Select Method Farm"] ~= "Cake Prince" or not Bulon["Auto Farm"]
            else
                 if GetDistance(CFrame.new(-2125.64282, 70.3262863, -12400.6777)) > 4500 then
                    Bypass(CFrame.new(-2125.64282, 70.3262863, -12400.6777))
                 elseif GetDistance(CFrame.new(-2125.64282, 70.3262863, -12400.6777)) < 4500 then
                    Tween(CFrame.new(-2125.64282, 70.3262863, -12400.6777))
                 end
            end
        elseif GetGlassCakePrince() and not DetectNigga("Cake Prince") then
            if DetectMob("Baking Staff") or DetectMob("Cookie Crafter") or DetectMob("Cake Guard") or DetectMob("Head Baker") then
                for r, v in pairs(WS.Enemies:GetChildren()) do
                    if table.find(MobCakePrince, v.Name) and v:FindFirstChild("Humanoid") and v.Humanoid.Health > 0 and v:FindFirstChild("HumanoidRootPart") and v.Parent then
                        repeat wait()
                            KillMob(v.Name, Bulon["Select Method Farm"] ~= "Bone" or not Bulon["Auto Farm"])
                        until not v.Parent or not v:FindFirstChild("Humanoid") or v:FindFirstChild("Humanoid").Health <= 0 or not v:FindFirstChild("HumanoidRootPart") or Bulon["Select Method Farm"] ~= "Cake Prince" or not Bulon["Auto Farm"] 
                    end
                end
            else
                Tween(CFrame.new(-2136.31177, 73.9832458, -12327.4492, 0.996233284, -3.53329632e-08, 0.0867136046, 4.28375344e-08, 1, -8.46835277e-08, -0.0867136046, 8.80791475e-08, 0.996233284))
            end
        end
    end
end

spawn(function()
    while wait() do
        if Bulon["Auto Farm"] then
            AutoFarm()
        end
    end
end)
