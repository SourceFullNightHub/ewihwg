        Click.Name = "Click"
        Click.Parent = FrameClick
        Click.Active = false
        Click.BackgroundTransparency = 1.000
        Click.Selectable = false
        Click.Size = UDim2.new(1, 0, 1, 0)
        Click.Image = "rbxassetid://2777727756"

        local waitbruh = false
        function NotifyFunc:Close()
            if waitbruh then
                return false
            end
            waitbruh = true
            TweenService:Create(
                NotifyReal,
                TweenInfo.new(tonumber(.2), Enum.EasingStyle.Back, Enum.EasingDirection.InOut),
                {Position = UDim2.new(0, 400, 0, 0)}
            ):Play()
            task.wait(tonumber(cfNotify.Time) / 1.2)
            NotifyMain:Destroy()
        end
        Click.MouseButton1Click:Connect(
            function()
                NotifyFunc:Close()
            end
        )
        TweenService:Create(
            NotifyReal,
            TweenInfo.new(tonumber(.2), Enum.EasingStyle.Back, Enum.EasingDirection.InOut),
            {Position = UDim2.new(0, -250, 1, -(NotifyPosHeigh) - 70)}
        ):Play()
        TimeEnd:TweenSize(UDim2.new(0, 0, 0, 1),"Out","Quad",tonumber(cfNotify.Time),true)
        task.wait(tonumber(cfNotify.Time))
        TweenService:Create(
            NotifyReal,
            TweenInfo.new(tonumber(.2), Enum.EasingStyle.Back, Enum.EasingDirection.InOut),
            {Position = UDim2.new(1, -250, 1, -(NotifyPosHeigh) - 70)}
        ):Play()
        task.wait(tonumber(2))
        NotifyFunc:Close()
    end)
    return NotifyFunc
end
