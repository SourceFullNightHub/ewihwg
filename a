            local function AdjustSectionHeight()
                List_2.Size = UDim2.new(1, 0, 0, UIListLayout_3.AbsoluteContentSize.Y + 48)
                Section.Size = UDim2.new(1, 0, 0, UIListLayout_3.AbsoluteContentSize.Y + 48)
            end
        
            UIListLayout_3:GetPropertyChangedSignal("AbsoluteContentSize"):Connect(AdjustSectionHeight)
            AdjustSectionHeight()

            local SectionFunc = {}
            function SectionFunc:AddButton(cfButton)
                cfButton = cfButton or {}
                cfButton.Name = cfButton.Name or ""
                cfButton.Callback = cfButton.Callback or function() end

                local Button = Instance.new("Frame")
                local UICorner_20 = Instance.new("UICorner")
                local Title_8 = Instance.new("TextLabel")
