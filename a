                Frame.Size = UDim2.new(0, 10, 0, 10)

                UICorner_25.CornerRadius = UDim.new(1, 0)
                UICorner_25.Parent = Frame

                local Dragging = false
				local function Round(Number, Factor)
					local Result = math.floor(Number/Factor + (math.sign(Number) * 0.5)) * Factor
					if Result < 0 then Result = Result + Factor end
					return Result
				end
				function SliderFunc:Set(Value)
					Value = math.clamp(Round(Value, 1), cfSlider.Min, cfSlider.Max)
					SliderFunc.Value = Value
					ValueB.Text = tostring(Value)
					TweenService:Create(
						Drag,
						TweenInfo.new(0.2, Enum.EasingStyle.Quad, Enum.EasingDirection.Out),
						{Size = UDim2.fromScale((Value - cfSlider.Min) / (cfSlider.Max - cfSlider.Min), 1)}
					):Play()
				end
				FrameDrag.InputBegan:Connect(function(Input)
					if Input.UserInputType == Enum.UserInputType.MouseButton1 then 
						Dragging = true 
					end 
				end)
				FrameDrag.InputEnded:Connect(function(Input) 
					if Input.UserInputType == Enum.UserInputType.MouseButton1 then 
						Dragging = false 
						cfSlider.Callback(SliderFunc.Value)
					end 
				end)
				uis.InputChanged:Connect(function(Input)
					if Dragging and Input.UserInputType == Enum.UserInputType.MouseMovement then 
						local SizeScale = math.clamp((Input.Position.X - FrameDrag.AbsolutePosition.X) / FrameDrag.AbsoluteSize.X, 0, 1)
						SliderFunc:Set(cfSlider.Min + ((cfSlider.Max - cfSlider.Min) * SizeScale)) 
					end
				end)
				ValueB:GetPropertyChangedSignal("Text"):Connect(function()
					local Valid = ValueB.Text:gsub("[^%d]", "")
					if Valid ~= "" then
						local ValidNumber = math.min(tonumber(Valid), cfSlider.Max)
						ValueB.Text = tostring(ValidNumber)
					else
						ValueB.Text = tostring(Valid)
					end
				end)
				ValueB.FocusLost:Connect(function()
					if ValueB.Text ~= "" then
						SliderFunc:Set(tonumber(ValueB.Text))
					else
						SliderFunc:Set(0)
					end
				end)
				SliderFunc:Set(tonumber(cfSlider.Default))
                return SliderFunc
            end
            function SectionFunc:AddParagraph(cfPara)
                cfPara = cfPara or {}
                cfPara.Name = cfPara.Name or "Paragraph"
                cfPara.Description = cfPara.Description or ""

                local Paragraph = Instance.new("Frame")
                local Title_10 = Instance.new("TextLabel")
                local UIPadding_16 = Instance.new("UIPadding")
                local UICorner_26 = Instance.new("UICorner")
                local Desc = Instance.new("TextLabel")
                local UIPadding_17 = Instance.new("UIPadding")
                local ParaFunc = {}

                Paragraph.Name = "Paragraph"
                Paragraph.Parent = List_2
                Paragraph.BackgroundColor3 = Color3.fromRGB(70, 70, 70)
                Paragraph.BorderColor3 = Color3.fromRGB(0, 0, 0)
                Paragraph.BorderSizePixel = 0
                Paragraph.Size = UDim2.new(1, 0, 0, 42)

                Title_10.Name = "Title"
                Title_10.Parent = Paragraph
                Title_10.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
                Title_10.BackgroundTransparency = 1.000
                Title_10.BorderColor3 = Color3.fromRGB(0, 0, 0)
                Title_10.BorderSizePixel = 0
                Title_10.Size = UDim2.new(1, 0, 0, 20)
                Title_10.Font = Enum.Font.GothamBold
                Title_10.Text = cfPara.Name
                Title_10.TextColor3 = Color3.fromRGB(255, 255, 255)
                Title_10.TextSize = 13.000
                Title_10.TextXAlignment = Enum.TextXAlignment.Left

                UIPadding_16.Parent = Title_10
                UIPadding_16.PaddingLeft = UDim.new(0, 7)

                UICorner_26.CornerRadius = UDim.new(0, 3)
                UICorner_26.Parent = Paragraph

                Desc.Name = "Desc"
                Desc.Parent = Paragraph
                Desc.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
                Desc.BackgroundTransparency = 1.000
                Desc.BorderColor3 = Color3.fromRGB(0, 0, 0)
                Desc.BorderSizePixel = 0
                Desc.Position = UDim2.new(0, 0, 0, 20)
                Desc.Size = UDim2.new(1, 0, 0, 22)
                Desc.Font = Enum.Font.GothamBold
                Desc.Text = cfPara.Description
                Desc.TextColor3 = Color3.fromRGB(210, 139, 222)
                Desc.TextSize = 13.000
                Desc.TextXAlignment = Enum.TextXAlignment.Left
                Desc.TextYAlignment = Enum.TextYAlignment.Top

                UIPadding_17.Parent = Desc
                UIPadding_17.PaddingLeft = UDim.new(0, 7)

                if cfPara.Description == nil or cfPara.Description == "" then
                    Paragraph.Size = UDim2.new(1, 0, 0, 20)
                end

                function ParaFunc:SetDesc(ccc)
                    Desc.Text = ccc
                end

                function ParaFunc:SetTitle(cccc)
                    Title_10.Text = cccc
                end
                return ParaFunc
            end
            function SectionFunc:AddSeperator(cc)
                local Seperator = Instance.new("TextLabel")

                Seperator.Name = "Seperator"
                Seperator.Parent = List_2
                Seperator.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
                Seperator.BackgroundTransparency = 1.000
                Seperator.BorderColor3 = Color3.fromRGB(0, 0, 0)
                Seperator.BorderSizePixel = 0
                Seperator.Size = UDim2.new(1, 0, 0, 20)
                Seperator.Font = Enum.Font.GothamBold
                Seperator.Text = "-- // ".. cc .. " \\ --"
                Seperator.TextColor3 = Color3.fromRGB(255, 255, 255)
                Seperator.TextSize = 13.000
                
            end
            return SectionFunc
        end
        return ItemFunc
    end
    return TabFunc
end

Library = LuxuryRemake

-- // UI MAIN \\ --

local Window = Library:AddWindow()

local Main = Window:AddTab({ Icon = "" })
local Settings = Window:AddTab({ Icon = ""  })

local MainAutoFarmSection = Main:AddSection("Left", {
    Name = "Main Auto Farm"
})

MainAutoFarmSection:AddToggle({
    Name = "Auto Farm Level",
    Default = a["Auto Farm Level"],
    Callback = function(Value)
        Save("Auto Farm Level", Value)
    end
})

MainAutoFarmSection:AddToggle({
    Name = "Double Quest",
    Default = a["Double Quest"],
    Callback = function(Value)
        Save("Double Quest", Value)
    end
})

MainAutoFarmSection:AddToggle({
    Name = "FastAttack",
    Default = a["FastAttack"],
    Callback = function(Value)
        Save("FastAttack", Value)
    end
})

function AutoFarmLevel()
    if plr.PlayerGui.Main.Quest.Visible then
        if GetMobFarm() then
            for r, v in next, Ws.Enemies:GetChildren() do
                if v.Name == GetMobFarm() then
                    if v:FindFirstChild("HumanoidRootPart") and v.Parent and v:FindFirstChild("Humanoid") and v.Humanoid.Health > 0 then
                        repeat wait()
                            KillMob(v.Name, a["Auto Farm Level"] == false)
                        until a["Auto Farm Level"] == false or plr.PlayerGui.Main.Quest.Visible == false
                    end
                end
            end
        end
    else
        ClaimQuest()
    end
end
LoadedHub = true
spawn(function()
    while wait() do
        if a["Auto Farm Level"] then
            AutoFarmLevel()
        end
    end
end)
