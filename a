                DropMain.BorderColor3 = Color3.fromRGB(0, 0, 0)
                DropMain.BorderSizePixel = 0
                DropMain.Position = UDim2.new(0, 0, 0, 20)
                DropMain.Size = UDim2.new(1, 0, 0, 120)

                UICorner_18.CornerRadius = UDim.new(0, 3)
                UICorner_18.Parent = DropMain

                NiggaArrow.Name = "Nigga Arrow"
                NiggaArrow.Parent = DropMain
                NiggaArrow.BackgroundTransparency = 1.000
                NiggaArrow.Position = UDim2.new(1, -30, 0, 0)
                NiggaArrow.Size = UDim2.new(0, 30, 0, 30)
                NiggaArrow.Image = "rbxassetid://2777862738"

                List_5.Name = "List"
                List_5.Parent = DropMain
                List_5.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
                List_5.BackgroundTransparency = 1.000
                List_5.BorderColor3 = Color3.fromRGB(0, 0, 0)
                List_5.BorderSizePixel = 0
                List_5.Position = UDim2.new(0, 0, 0, 30)
                List_5.Size = UDim2.new(1, 0, 1, -30)

                Scroll.Name = "Scroll"
                Scroll.Parent = List_5
                Scroll.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
                Scroll.BackgroundTransparency = 1.000
                Scroll.BorderColor3 = Color3.fromRGB(0, 0, 0)
                Scroll.BorderSizePixel = 0
                Scroll.Selectable = false
                Scroll.Size = UDim2.new(1, 0, 1, 0)
                Scroll.ScrollBarThickness = 1

                UIPadding_12.Parent = Scroll
                UIPadding_12.PaddingBottom = UDim.new(0, 2)
                UIPadding_12.PaddingLeft = UDim.new(0, 3)
                UIPadding_12.PaddingRight = UDim.new(0, 3)
                UIPadding_12.PaddingTop = UDim.new(0, 2)

                UIListLayout_6.Parent = Scroll
                UIListLayout_6.SortOrder = Enum.SortOrder.LayoutOrder
                UIListLayout_6.Padding = UDim.new(0, 4)

                Selected.Name = "Selected"
                Selected.Parent = DropMain
                Selected.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
                Selected.BackgroundTransparency = 1.000
                Selected.BorderColor3 = Color3.fromRGB(0, 0, 0)
                Selected.BorderSizePixel = 0
                Selected.Size = UDim2.new(1, -30, 0, 30)
                Selected.Font = Enum.Font.GothamBold
                Selected.Text = cfDropdown.Default
                Selected.TextColor3 = Color3.fromRGB(255, 255, 255)
                Selected.TextSize = 13.000
                Selected.TextStrokeColor3 = Color3.fromRGB(255, 255, 255)
                Selected.TextXAlignment = Enum.TextXAlignment.Left

                UIPadding_14.Parent = Selected
                UIPadding_14.PaddingLeft = UDim.new(0, 7)

                Click_6.Name = "Click"
                Click_6.Parent = Dropdown
                Click_6.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
                Click_6.BackgroundTransparency = 1.000
                Click_6.BorderColor3 = Color3.fromRGB(0, 0, 0)
                Click_6.BorderSizePixel = 0
                Click_6.Position = UDim2.new(0, 0, 0, 20)
                Click_6.Size = UDim2.new(1, 0, 0, 32)
                Click_6.Font = Enum.Font.SourceSans
                Click_6.Text = ""
                Click_6.TextColor3 = Color3.fromRGB(0, 0, 0)
                Click_6.TextSize = 14.000

                local Dropped = false
				function DropFunc:Add(ac)
					for i, v in next, ac do
                        local Option = Instance.new("Frame")
                        local Title_7 = Instance.new("TextButton")
                        local UIPadding_13 = Instance.new("UIPadding")
                        local UICorner_19 = Instance.new("UICorner")

                        Option.Name = "Option"
                        Option.Parent = Scroll
                        Option.BackgroundColor3 = Color3.fromRGB(75, 75, 75)
                        Option.BackgroundTransparency = 1.000
                        Option.BorderColor3 = Color3.fromRGB(0, 0, 0)
                        Option.BorderSizePixel = 0
                        Option.Size = UDim2.new(1, 0, 0, 20)

                        Title_7.Name = "Title"
                        Title_7.Parent = Option
                        Title_7.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
                        Title_7.BackgroundTransparency = 1.000
                        Title_7.BorderColor3 = Color3.fromRGB(0, 0, 0)
                        Title_7.BorderSizePixel = 0
                        Title_7.Size = UDim2.new(1, 0, 1, 0)
                        Title_7.Font = Enum.Font.GothamBold
                        Title_7.Text = tostring(v)
                        Title_7.TextColor3 = Color3.fromRGB(170, 170, 170)
                        Title_7.TextSize = 13.000
                        Title_7.TextXAlignment = Enum.TextXAlignment.Left

                        UIPadding_13.Parent = Title_7
                        UIPadding_13.PaddingLeft = UDim.new(0, 7)

                        UICorner_19.CornerRadius = UDim.new(0, 4)
                        UICorner_19.Parent = Option

                        Title_7.MouseButton1Click:Connect(function()
                            for r, v in next, Scroll:GetChildren() do
                                if v.Name == "Option" then
                                    v.BackgroundTransparency = 1.000
                                end
                            end
                            for r, v in next, Scroll:GetChildren() do
                                if v.Name == "Option" then
                                    v.Title.TextColor3 = Color3.fromRGB(170, 170, 170)
                                end
                            end
                            Title_7.TextColor3 = Color3.fromRGB(255, 255, 255)
                            Option.BackgroundTransparency = 0
                            Selected.Text = Title_7.Text
                            cfDropdown.Callback(Title_7.Text)
                        end)
					end
				end

				function DropFunc:Set(acc)
					for i, v in pairs(Scroll:GetChildren()) do
						if v.Name == "Option" then
							if v.Title.Text == acc then
								Dropped = false
								TweenService:Create(NiggaArrow, TweenInfo.new(.3, Enum.EasingStyle.Quad, Enum.EasingDirection.Out), {Rotation = -180.000}):Play()
								Selected.Text = v.Title.Text
								cfDropdown.Callback(v.Title.Text)
							end
						end
					end
				end

				function DropFunc:Refresh(Option)
					for i, v in pairs(Scroll:GetChildren()) do
						if v.Name == "Option" then
							v:Destroy()
						end
					end
					Dropped = false
					DropFunc:Add(Option)
					Selected.Text = ""
				end

				Click_6.MouseButton1Click:Connect(function()
					if Dropped == false then
						TweenService:Create(NiggaArrow, TweenInfo.new(.3, Enum.EasingStyle.Quad, Enum.EasingDirection.Out), {Rotation = -90.000}):Play()
						Dropdown:TweenSize(UDim2.new(1, 0, 0, 139),"Out","Quad",.3,true)
						Dropped = true
					else
						Dropdown:TweenSize(UDim2.new(1, 0, 0, 53),"Out","Quad",.3,true)
						TweenService:Create(NiggaArrow, TweenInfo.new(.3, Enum.EasingStyle.Quad, Enum.EasingDirection.Out), {Rotation = -180.000}):Play()
						Dropped = false
					end
				end)

				DropFunc:Refresh(cfDropdown.Options)
				cfDropdown.Callback(cfDropdown.Default)
				return DropFunc
            end
