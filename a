        end)
        CountTab = CountTab + 1
        local ItemFunc = {}
        local cctab
        function ItemFunc:AddSection(tabchoose, CfSection)
            CfSection = CfSection or {}
            CfSection.Name = CfSection.Name or ""
        
            if tabchoose == nil or tabchoose == 'Left' then
                cctab = List
            elseif tabchoose == "Right" then
                cctab = List_3
            end
        
            local Section = Instance.new("Frame")
            local UICorner_6 = Instance.new("UICorner")
            local TopTitle = Instance.new("Frame")
            local Title = Instance.new("TextLabel")
            local Line = Instance.new("Frame")
            local List_2 = Instance.new("Frame")
            local UIPadding_5 = Instance.new("UIPadding")
            local UIListLayout_3 = Instance.new("UIListLayout")
        
