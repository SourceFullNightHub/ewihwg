Bulon = {
    ["Select Weapon"] = "Melee"
}

function LoadSettings()
    if readfile and writefile and isfile and isfolder then
        if not isfolder("Sitink Hub") then
            makefolder("Sitink Hub")
        end
        if not isfolder("Sitink Hub/Blox Fruits/") then
            makefolder("Sitink Hub/Blox Fruits/")
        end
        if not isfile("Sitink Hub/Blox Fruits/" .. game.Players.LocalPlayer.Name .. ".json") then
            writefile(
                "Sitink Hub/Blox Fruits/" .. game.Players.LocalPlayer.Name .. ".json",
                game:GetService("HttpService"):JSONEncode(Bulon)
            )
        else
            local x =
                game:GetService("HttpService"):JSONDecode(
                readfile("Sitink Hub/Blox Fruits/" .. game.Players.LocalPlayer.Name .. ".json")
            )
            for y, z in pairs(x) do
                Bulon[y] = z
            end
        end
    else
        return
    end
end
function SaveSettings()
    if readfile and writefile and isfile and isfolder then
        if not isfile("Sitink Hub/Blox Fruits/" .. game.Players.LocalPlayer.Name .. ".json") then
            LoadSettings()
        else
            local x =
                game:GetService("HttpService"):JSONDecode(
                readfile("Sitink Hub/Blox Fruits/" .. game.Players.LocalPlayer.Name .. ".json")
            )
            local A = {}
            for y, z in pairs(Bulon) do
                A[y] = z
            end
            writefile(
                "Sitink Hub/Blox Fruits/" .. game.Players.LocalPlayer.Name .. ".json",
                game:GetService("HttpService"):JSONEncode(A)
            )
        end
    else
        return
    end
end
LoadSettings()

function DetectMob(Cac)
    for i, v in pairs(WS.Enemies:GetChildren()) do
        if v.Name == Cac and v:FindFirstChild("Humanoid") and v.Humanoid.Health > 0 and v.Parent then
            return v
        end
    end
end

function DetectNigga(CCCCCCCC)
    for i, v in pairs(WS.Enemies:GetChildren()) do
        if v.Name == CCCCCCCC and v:FindFirstChild("Humanoid") and v.Humanoid.Health > 0 and v.Parent then
            return v
        end
    end
    for i, v in pairs(Rc:GetChildren()) do
        if v.Name == CCCCCCCC and v:FindFirstChild("Humanoid") and v.Humanoid.Health > 0 and v.Parent then
            return v
        end
    end
end

function GetWP(Nigga)
    Concac = ""
    for i, v in pairs(plr.Backpack:GetChildren()) do
        if v:IsA("Tool") and v.ToolTip == Nigga then
            Concac = v.Name
        end
    end
    for i, v in pairs(plr.Character:GetChildren()) do
        if v:IsA("Tool") and v.ToolTip == Nigga then
            Concac = v.Name
        end
    end
    return Concac
end

function EquipWP(EvenZDaiTo)
    if Bulon["Select Weapon"] == "" then
        Bulon["Select Weapon"] = "Melee"
    end
    EvenZDaiTo = GetWP(Bulon["Select Weapon"])
    local cc = plr.Backpack:FindFirstChild(EvenZDaiTo)
    if cc then
        plr.Character.Humanoid:EquipTool(cc)
    end
end

function DjtBuso()
    if not plr.Character:FindFirstChild("HasBuso") then
        Rc.Remotes["CommF_"]:InvokeServer("Buso")
    end
end

function CheckPortalXungQuanh(cc)
    Akikisieudz = cc.Position
    if Old_World then
        Teleporter = {
            ["1"] = Vector3.new(-7894.6201171875, 5545.49169921875, -380.2467346191406),
            ["2"] = Vector3.new(-4607.82275390625, 872.5422973632812, -1667.556884765625),
            ["3"] = Vector3.new(61163.8515625, 11.759522438049316, 1819.7841796875),
            ["4"] = Vector3.new(3876.280517578125, 35.10614013671875, -1939.3201904296875)
        }
    elseif New_World then
        Teleporter = {
            ["Mansion"] = Vector3.new(-288.46246337890625, 306.130615234375, 597.9988403320312),
            ["Flamingo"] = Vector3.new(2284.912109375, 15.152046203613281, 905.48291015625),
            ["122"] = Vector3.new(923.21252441406, 126.9760055542, 32852.83203125),
            ["3032"] = Vector3.new(-6508.5581054688, 89.034996032715, -132.83953857422)
        }
    elseif Third_World then
        Teleporter = {
            ["Caslte On The Sea"] = Vector3.new(-5058.77490234375, 314.5155029296875, -3155.88330078125),
            ["Mansion"] = Vector3.new(-12463.8740234375, 374.9144592285156, -7523.77392578125),
            ["Great Tree"] = Vector3.new(28282.5703125, 14896.8505859375, 105.1042709350586),
            ["Hydra island"] = Vector3.new(5314.58203125, 25.419387817382812, -125.94227600097656),
            ["Ngu1"] = Vector3.new(-11993.580078125, 334.7812805175781, -8844.1826171875),
            ["ngu2"] = Vector3.new(5314.58203125, 25.419387817382812, -125.94227600097656),
            ["Temple Of Time"] = Vector3.new(2957.833740234375, 2286.495361328125, -7217.05078125)
        }
    end
    for i, v in pairs(Teleporter) do
        this = v
        cacmaytonhi = i
    end
    for i, v in pairs(Teleporter) do
        if (v - Akikisieudz).Magnitude <= 7000 and (plr.Character.HumanoidRootPart.Position - Akikisieudz).Magnitude >= 3000 then
            return this, cacmaytonhi
        end
    end
end

function TempleTween()
    game:GetService("ReplicatedStorage").Remotes.CommF_:InvokeServer("requestEntrance", Vector3.new(28282.5703125, 14896.8505859375, 105.1042709350586))
end

function requestEntrance(e, nigga)
    if nigga ~= "Great Tree" then
        game:GetService("ReplicatedStorage").Remotes.CommF_:InvokeServer("requestEntrance", e)
        oldcframe = plr.Character.HumanoidRootPart.CFrame
        plr.Character.HumanoidRootPart.CFrame = CFrame.new(oldcframe.Z, oldcframe.Y + 80, oldcframe.X)
        if plr.Character:FindFirstChild("TweenSmooth") then
            plr.Character:FindFirstChild("TweenSmooth").CFrame = plr.Character.HumanoidRootPart.CFrame
        end
    else
        if (Vector3.new(28282.5703125, 14896.8505859375, 105.1042709350586) - plr.Character.HumanoidRootPart.Position) >= 1000 then
            TempleTween()
        else
            if (CFrame.new(28612.7148, 14896.4893, 103.860237).Position - plr.Character.HumanoidRootPart.Position).Magnitude <= 10 then
                game.ReplicatedStorage.Remotes.CommF_:InvokeServer("RaceV4Progress", "TeleportBack")
            else
                Tween(CFrame.new(28612.7148, 14896.4893, 103.860237))
            end
        end
    end
end

function WaitHRP(q0) 
    if not q0 then return end
    return q0.Character:WaitForChild("HumanoidRootPart", 9) 
end 

function Tween(TargetCFrame)
    Distance = (TargetCFrame.Position - plr.Character.HumanoidRootPart.Position).Magnitude
    if Distance <= 250 then
        plr.Character.HumanoidRootPart.CFrame = TargetCFrame
    end
    if not plr.Character:FindFirstChild("TweenSmooth") then
        local Ncc = Instance.new("Part", plr.Character)
        Ncc.Size = Vector3.new(10,1,10)
        Ncc.Name = "TweenSmooth"
        Ncc.Anchored = true
        Ncc.Transparency = 1
        Ncc.CanCollide = false
        Ncc.CFrame = WaitHRP(plr).CFrame 
        Ncc:GetPropertyChangedSignal("CFrame"):Connect(function()
            task.wait(0.01)
            WaitHRP(plr).CFrame = Ncc.CFrame
        end)
    end
    tween = game:service"TweenService":Create(plr.Character["TweenSmooth"], TweenInfo.new(Distance / 160, Enum.EasingStyle.Quad), {CFrame = TargetCFrame})
    tween:Play()
end

spawn(function()
    while task.wait() do
        if plr.Character.Humanoid.Health <= 0 or not plr.Character:FindFirstChild("HumanoidRootPart") then
            if plr.Character:FindFirstChild("TweenSmooth") then
                plr.Character:FindFirstChild("TweenSmooth"):Destroy()
            end
        end
    end
end)
spawn(function()
    while task.wait() do
        if plr.Character:FindFirstChild("TweenSmooth") then
            if (plr.Character.HumanoidRootPart.Position - plr.Character:FindFirstChild("TweenSmooth").Position).Magnitude >= 100 then
                plr.Character:FindFirstChild("TweenSmooth"):Destroy()
            end
        end
    end
end)

function InMyNetWork(object)
    if isnetworkowner then
        return isnetworkowner(object)
    else
        if (object.Position - game.Players.LocalPlayer.Character.HumanoidRootPart.Position).Magnitude <= 350 then
            return true
        end
        return false
    end
end

function BringMob(nhulon)
    for i, v in pairs(game.Workspace.Enemies:GetChildren()) do
        if
            v.Name == nhulon and v.Parent and v:FindFirstChild("HumanoidRootPart") and v:FindFirstChild("Humanoid") and v.Humanoid.Health > 0 and
                (v.HumanoidRootPart.Position - game.Players.LocalPlayer.Character.HumanoidRootPart.Position).Magnitude <=
                    350
         then

            if InMyNetWork(v.HumanoidRootPart) then
                v.HumanoidRootPart.CFrame = BringCFrame
                v.Humanoid.JumpPower = 0
                v.Humanoid.WalkSpeed = 0
                v.HumanoidRootPart.Transparency = 1
                v.HumanoidRootPart.CanCollide = false
                v.Head.CanCollide = false
                if v.Humanoid:FindFirstChild("Animator") then
                    v.Humanoid.Animator:Destroy()
                end
                sethiddenproperty(plr, "SimulationRadius", math.huge)
                v.Humanoid:ChangeState(11)
            end
        end
    end
end

function CancelTween()
    NoClip = false
    if plr.Character:FindFirstChild("TweenSmooth") then
        plr.Character:FindFirstChild("TweenSmooth"):Destroy()
    end
    if plr.Character.Head:FindFirstChild("NoClip") then
        plr.Character.Head:FindFirstChild("NoClip"):Destroy()
    end 
    return Tween(plr.Character.HumanoidRootPart.CFrame)
end

function DisableTween(ac)
    if not ac then
        CancelTween()
        return Tween(plr.Character.HumanoidRootPart.CFrame)
    end
end

function KillMob(Ngu, Ngu2)
    for i, v in pairs(WS.Enemies:GetChildren()) do
        if v.Name == Ngu and v.Parent and v:FindFirstChild("Humanoid") and v:FindFirstChild("HumanoidRootPart") and v.Humanoid.Health > 0 then
            repeat wait()
                DjtBuso()
                EquipWP()
                Tween(v.HumanoidRootPart.CFrame * CFrame.new(0, 15, 0))
                game.Players.LocalPlayer.Character.Humanoid.Sit = false
                BringCFrame = v.HumanoidRootPart.CFrame
                BringMob(Ngu)
                NoClip = true
            until not v.Parent or not v:FindFirstChild("Humanoid") or v:FindFirstChild("Humanoid").Health <= 0 or not v:FindFirstChild("HumanoidRootPart") or Ngu2
            NoClip = false
            CancelTween()
        end
    end
end

function CheckQuestVisible()
    if plr.PlayerGui.Main.Quest.Visible == true then
        return true
    end
    return false
end

function CheckTitleQuest(cc)
    for i, v in pairs(plr.PlayerGui.Main.Quest.Container.QuestTitle.Title.Text:GetChildren()) do
        if string.find(v.Text, cc) then
            return true
        end
    end
    return false
end

function StackFunc()
    
end

function Bypass(cac)
    local vai, cut = CheckPortalXungQuanh(cac)
    if vai then
        CancelTween()
        requestEntrance(vai, cut)
    else
        if (cac.Position - vai).Magnitude >= 4500 then
            repeat wait()
                if plr.Character:FindFirstChild("TweenSmooth") then
                    plr.Character:FindFirstChild("TweenSmooth").CFrame = cac
                end
                game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = cac
                game.Players.LocalPlayer.Character.Head:Destroy()
                game:GetService("Players").LocalPlayer.Character:WaitForChild("Humanoid"):ChangeState(15)
                wait(2)
                game.Players.LocalPlayer.Character.PrimaryPart.CFrame = cac
                wait(3)
            until (cac.Position - plr.Character.HumanoidRootPart.Position).Magnitude < 4500 or plr.Character:FindFirstChild("Humanoid").Health <= 0
        end
    end
end

function GetDistance(hehe)
    if typeof(hehe) == "CFrame" then
        return (hehe.Position - plr.Character.HumanoidRootPart.Position).Magnitude
    elseif typeof(hehe) == "Vector3" then
        return (hehe - plr.Character.HumanoidRootPart.Position).Magnitude
    end
end

function GetGlassCakePrince()
    if game:GetService("Workspace").Map.CakeLoaf.BigMirror.Other.Transparency == 1 then
        return true
    end
    return false
end

function CheckData(cccc)
    if cccc == "Beli" then
        return plr.Data.Beli.Value
    elseif cccc == "Fragments" then
        return plr.Data.Fragments.Value
    elseif cccc == "Level" then
        return plr.Data.Level.Value
    end
end

spawn(function()
    while wait() do
        if NoClip == true then
            if plr.Character:FindFirstChild("Head") then
                if not plr.Character.Head:FindFirstChild("NoClip") then
                    local BodyClip = Instance.new("BodyVelocity", plr.Character.Head)
                    BodyClip.Name = "NoClip"
                    BodyClip.Velocity = Vector3.new(0, 0, 0)
                    BodyClip.MaxForce = Vector3.new(9999999, 9999999, 9999999)
                    BodyClip.P = 15000
                end
                for i, v in pairs(plr.Character:GetDescendants()) do
                    if v:IsA("BasePart") then
                        v.CanCollide = false
                    end
                end
            end
        else
            if plr.Character:FindFirstChild("Head") then
                if plr.Character.Head:FindFirstChild("NoClip") then
                    plr.Character.Head:FindFirstChild("NoClip"):Destroy()
                end
            end
        end
    end
end)

