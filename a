                local UICorner_8 = Instance.new("UICorner")
                local Cricle = Instance.new("Frame")
                local UICorner_9 = Instance.new("UICorner")
                local Click_3 = Instance.new("TextButton")
                local FuncToggle = {}

                ToggleDisable.Name = "Toggle Disable"
                ToggleDisable.Parent = List_2
                ToggleDisable.BackgroundColor3 = Color3.fromRGB(65, 65, 65)
                ToggleDisable.BorderColor3 = Color3.fromRGB(0, 0, 0)
