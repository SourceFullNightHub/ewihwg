local plr = game.Players.LocalPlayer
function TweenObject(Tenbien, TweenCFrame,obj,ts)
    if not ts then ts = 350 end
    if not Tenbien:FindFirstChild("SmoothPart") then
        local PartObject = Instance.new("Part")
        PartObject.Parent = game:GetService("Workspace").Boats:FindFirstChild("PirateBrigade") -- Create part
        PartObject.Size = Vector3.new(10,1,10)
        PartObject.Name = "SmoothPart"
        PartObject.Anchored = true
        PartObject.Transparency = 1
        PartObject.CanCollide = false
        PartObject.CFrame = obj.CFrame 
        PartObject:GetPropertyChangedSignal("CFrame"):Connect(function()
            task.wait(0.01)
            obj.CFrame = PartObject.CFrame
        end)
    end
    local tween_s = game:GetService("TweenService")
    local info = TweenInfo.new((TweenCFrame.Position - obj.Position).Magnitude / ts, Enum.EasingStyle.Linear)
    tween = tween_s:Create(Tenbien["PartObject"] ,info ,{CFrame = TweenCFrame})
    tween:Play() 
end

function GetYourShip()
    for i, v in pairs(game.Workspace.Boats:GetChildren()) do
        if v:FindFirstChild("Owner") then
            if v:FindFirstChild("Owner").Value == game.Players.LocalPlayer.Name then
                return v
            end
        end
    end
end

TweenObject(game:GetService("Workspace").Boats:FindFirstChild("PirateBrigade"), CFrame.new(-44842.3945, 10.7790766, 16911.3477), game:GetService("Workspace").Boats:FindFirstChild("PirateBrigade").VehicleSeat)
