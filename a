local MobCakeIsland = {
    "Baking Staff",
    "Head Baker",
    "Cake Guard",
    "Cookie Crafter"
}
local MobHauntedCastle = {
    "Reborn Skeleton",
    "Demonic Soul",
    "Living Zombie",
    "Posessed Mummy",
}

function CheckGlassCakeIsland()
    if game:GetService("Workspace").Map.CakeLoaf.BigMirror.Other.Transparency == 1 then
        return true
    end
    return false
end

IsAutoFarm = true
function AutoFarm()
    if not IsAutoFarm then
        return
    end
    if Configs["Select Method Farm"] == "Cake Prince" then
        if not CheckGlassCakeIsland() and DetectMobAllFolder("Cake Prince") then
            if DetectMob("Cake Prince") then
                v = DetectMob("Cake Prince")
                if v:FindFirstChild("Humanoid") and v.Humanoid.Health > 0 and v.Parent and v:FindFirstChild("HumanoidRootPart") then
                    repeat wait()
                        StatusFarm:SetDesc("Killing Mob ".. v.Name)
                        KillMob("Cake Prince", Configs["Auto Farm"])
                    until not CheckGlassCakeIsland() or not Configs["Auto Farm"] or Configs["Select Method Farm"] ~= "Cake Prince" or not v.Parent or not v:FindFirstChild("Humanoid") or v.Humanoid.Health <= 0 or not v:FindFirstChild("HumanoidRootPart") 
                end
            else
                NoClip = true
                Tween(CFrame.new(-2125.64282, 70.3262863, -12400.6777))
            end
        elseif CheckGlassCakeIsland() and not DetectMobAllFolder("Cake Prince") then
            if DetectMob("Baking Staff") or DetectMob("Cookie Crafter") or DetectMob("Cake Guard") or DetectMob("Head Baker") then                                     
                for r, v in pairs(WS.Enemies:GetChildren()) do
                    if table.find(MobCakeIsland, v.Name) and v:FindFirstChild("Humanoid") and v.Humanoid.Health > 0 and v.Parent and v:FindFirstChild("HumanoidRootPart") then
                        repeat wait()
                            StatusFarm:SetDesc("Killing Mob ".. v.Name)
                            KillMob(v.Name, Configs["Auto Farm"])
                        until CheckGlassCakeIsland() or not Configs["Auto Farm"] or Configs["Select Method Farm"] ~= "Cake Prince" or not v.Parent or not v:FindFirstChild("Humanoid") or v.Humanoid.Health <= 0 or not v:FindFirstChild("HumanoidRootPart")
                    end
                end
            else
                for i, v in pairs(GetMobSpawnList(GetMobName("Cake Guard"))) do
                    repeat wait()
                        StatusFarm:SetDesc("Waitting Mob...")
                        NoClip = true
                        Tween(v.CFrame * CFrame.new(2, 20, 2))
                    until not CheckGlassCakeIsland() or not Configs["Auto Farm"] or Configs["Select Method Farm"] ~= "Cake Prince" or DetectMob("Cake Guard") or CheckDistancebyCFrame(v) <= 100
                end
            end
        end
    elseif Configs["Select Method Farm"] == "Bone" then
        if DetectMob("Reborn Skeleton") or DetectMob("Demonic Soul") or DetectMob("Living Zombie") or DetectMob("Posessed Mummy") then                                     
            for r, v in pairs(WS.Enemies:GetChildren()) do
                if table.find(MobHauntedCastle, v.Name) and v:FindFirstChild("Humanoid") and v.Humanoid.Health > 0 and v.Parent and v:FindFirstChild("HumanoidRootPart") then
                    repeat wait()
                        StatusFarm:SetDesc("Killing Mob ".. v.Name)
                        KillMob(v.Name, Configs["Auto Farm"])
                    until not Configs["Auto Farm"] or Configs["Select Method Farm"] ~= "Bone" or not v.Parent or not v:FindFirstChild("Humanoid") or v.Humanoid.Health <= 0 or not v:FindFirstChild("HumanoidRootPart")
                end
            end
        else
            for i, v in pairs(GetMobSpawnList(GetMobName("Demonic Soul"))) do
                repeat wait()
                    StatusFarm:SetDesc("Waitting Mob...")
                    NoClip = true
                    Tween(v.CFrame * CFrame.new(2, 20, 2))
                until not Configs["Auto Farm"] or Configs["Select Method Farm"] ~= "Bone" or DetectMob("Demonic Soul") or CheckDistancebyCFrame(v) <= 100
            end
        end
    elseif Configs["Select Method Farm"] == "Level Farm" then
        if plr.PlayerGui.Main.Quest.Visible == true then
            if MobLevel1OrMobLevel2() then
                for r, v in pairs(game.Workspace.Enemies:GetChildren()) do
                    if v.Name == MobLevel1OrMobLevel2() then
                        if v:FindFirstChild("Humanoid") and v.Humanoid.Health > 0 and v.Parent and v:FindFirstChild("HumanoidRootPart") then
                            repeat wait()
                                StatusFarm:SetDesc("Killing Mob ".. v.Name)
                                KillMob(v.Name, Configs["Auto Farm"])
                            until not Configs["Auto Farm"] or Configs["Select Method Farm"] ~= "Level Farm" or not v.Parent or not v:FindFirstChild("Humanoid") or v.Humanoid.Health <= 0 or not v:FindFirstChild("HumanoidRootPart")
                        end
                    end
                end
            else
                for i, v in pairs(GetMobSpawnList(GetMob())) do
                    repeat wait()
                        StatusFarm:SetDesc("Waitting Mob ".. CheckNameMobDoubleQuest())
                        NoClip = true
                        Tween(v.CFrame * CFrame.new(2, 20, 2))
                    until not Configs["Auto Farm"] or Configs["Select Method Farm"] ~= "Level Farm" or MobLevel1OrMobLevel2() or CheckDistancebyCFrame(v) <= 100
                end
            end
        else
            StatusFarm:SetDesc("Get Quest Level Farm")
            GetQuest()
        end
    end
end

spawn(function()
    while wait() do
        if Configs["Auto Farm"] then
            AutoFarm()
        end
    end
end)

local ReplicatedStorage = game:GetService("ReplicatedStorage")
local task = task or require(game:GetService("ReplicatedStorage"):WaitForChild("task"))
task.spawn(function()
    local hookFunction = hookfunction or hookfunc or function(...) end
    local Container = ReplicatedStorage:WaitForChild("Effect"):WaitForChild("Container")
    local CameraShaker = require(ReplicatedStorage:WaitForChild("Util"):WaitForChild("CameraShaker"))
    local Death = require(Container:WaitForChild("Death"))
    local Respawn = require(Container:WaitForChild("Respawn"))
    hookFunction(Death, function() 
        return nil 
    end)
    hookFunction(Respawn, function() 
        return nil 
    end)
    game:GetService("ReplicatedStorage").Assets.GUI.DamageCounter.Enabled = false
    CameraShaker:Stop()
end)
