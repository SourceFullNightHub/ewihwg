            UICorner_12.Parent = Slider

            UIStroke_7.Color = Color3.fromRGB(100, 100, 100)
            UIStroke_7.Parent = Slider

            Desc_3.Name = "Desc"
            Desc_3.Parent = Slider
            Desc_3.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
            Desc_3.BackgroundTransparency = 1.000
            Desc_3.BorderColor3 = Color3.fromRGB(0, 0, 0)
            Desc_3.BorderSizePixel = 0
            Desc_3.Position = UDim2.new(0.00287356321, 0, 0.548151672, 0)
            Desc_3.Size = UDim2.new(0, 348, 0, 43)
            Desc_3.Font = Enum.Font.SourceSansBold
            Desc_3.Text = Options.Description
            Desc_3.TextColor3 = Color3.fromRGB(144, 144, 144)
            Desc_3.TextSize = 14.000
            Desc_3.TextWrapped = true
            Desc_3.TextXAlignment = Enum.TextXAlignment.Left
            Desc_3.TextYAlignment = Enum.TextYAlignment.Top

            UIPadding_14.Parent = Desc_3
            UIPadding_14.PaddingLeft = UDim.new(0, 7)

            SliderBack.Name = "SliderBack"
            SliderBack.Parent = Slider
            SliderBack.BackgroundColor3 = Color3.fromRGB(95, 95, 95)
            SliderBack.BorderColor3 = Color3.fromRGB(0, 0, 0)
            SliderBack.BorderSizePixel = 0
            SliderBack.Position = UDim2.new(0.0201149434, 0, 0.355769217, 0)
            SliderBack.Size = UDim2.new(0, 334, 0, 8)

            UICorner_13.Parent = SliderBack

            UIStroke_8.Parent = SliderBack

            Draggable.Name = "Draggable"
            Draggable.Parent = SliderBack
            Draggable.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
            Draggable.BorderColor3 = Color3.fromRGB(0, 0, 0)
            Draggable.BorderSizePixel = 0
            Draggable.Size = UDim2.new(0, 167, 0, 8)

            UICorner_14.Parent = Draggable

            Content_4.Name = "Content"
            Content_4.Parent = Slider
            Content_4.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
            Content_4.BackgroundTransparency = 1.000
            Content_4.BorderColor3 = Color3.fromRGB(0, 0, 0)
            Content_4.BorderSizePixel = 0
            Content_4.ClipsDescendants = true
            Content_4.Position = UDim2.new(0, 0, -7.33595641e-07, 0)
            Content_4.Size = UDim2.new(1, 0, 0.278846622, 0)
            Content_4.Font = Enum.Font.Unknown
            Content_4.Text = Options.Name
            Content_4.TextColor3 = Color3.fromRGB(255, 255, 255)
            Content_4.TextSize = 14.000
            Content_4.TextXAlignment = Enum.TextXAlignment.Left

            UIPadding_15.Parent = Content_4
            UIPadding_15.PaddingLeft = UDim.new(0, 7)

            Value.Name = "Value"
            Value.Parent = Content_4
            Value.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
            Value.BackgroundTransparency = 1.000
            Value.BorderColor3 = Color3.fromRGB(0, 0, 0)
            Value.BorderSizePixel = 0
            Value.Position = UDim2.new(0.90476191, 0, 0.122463182, 0)
            Value.Size = UDim2.new(0, 25, 0, 25)
            Value.Font = Enum.Font.Unknown
            Value.Text = tostring(Options.Default and math.floor((Options.Default / Options.Max) * (Options.Max - Options.Min) + Options.Min) or 1)
            Value.TextColor3 = Color3.fromRGB(255, 255, 255)


            function SliderNhulon:SetValue(v)
                if v == nil then
                    local percentage = math.clamp((mouse.X - SliderBack.AbsolutePosition.X) / (SliderBack.AbsoluteSize.X), 0, 1)
				    local value = math.floor(((Options.Max - Options.Min) * percentage) + Options.Min)

                    Value.Text = tostring(value)
                    Draggable.Size = UDim2.fromScale(percentage, 1)
                else
                    Value.Text = tostring(v)
                    local value = math.floor(((v - Options.Min) / (Options.Max - Options.Min)), 1)
                end
                Options.CallBack(SliderNhulon:GetValue())
            end

            function SliderNhulon:GetValue()
                return tonumber(Value.Text)
            end

            Draggable.MouseEnter:Connect(function()
                SliderNhulon.Hover = true

                TweenService:Create(Draggable, TweenInfo.new(0.2, Enum.EasingStyle.Quad, Enum.EasingDirection.Out), {BackgroundColor3 = Color3.fromRGB(200, 200, 200)}):Play()     
                TweenService:Create(UIStroke_8, TweenInfo.new(0.2, Enum.EasingStyle.Quad, Enum.EasingDirection.Out), {Color = Color3.fromRGB(255, 255, 255)}):Play()     
                TweenService:Create(UIStroke_7, TweenInfo.new(0.2, Enum.EasingStyle.Quad, Enum.EasingDirection.Out), {Color = Color3.fromRGB(255, 255, 255)}):Play()     
            end)

            Draggable.MouseLeave:Connect(function()
                SliderNhulon.Hover = false

                TweenService:Create(UIStroke_8, TweenInfo.new(0.2, Enum.EasingStyle.Quad, Enum.EasingDirection.Out), {Color = Color3.fromRGB(144, 144, 144)}):Play()     
                TweenService:Create(UIStroke_7, TweenInfo.new(0.2, Enum.EasingStyle.Quad, Enum.EasingDirection.Out), {Color = Color3.fromRGB(144, 144, 144)}):Play()     
                TweenService:Create(Draggable, TweenInfo.new(0.2, Enum.EasingStyle.Quad, Enum.EasingDirection.Out), {BackgroundColor3 = Color3.fromRGB(106, 106, 106)}):Play()     
            end)

            uis.InputBegan:Connect(function(input, gpe)
                if gpe then return end

                if input.UserInputType == Enum.UserInputType.MouseButton1 and SliderNhulon.Hover then
                    SliderNhulon.MouseDown = true
                    TweenService:Create(Draggable, TweenInfo.new(0.2, Enum.EasingStyle.Quad, Enum.EasingDirection.Out), {BackgroundColor3 = Color3.fromRGB(200, 200, 200)}):Play()     
                    TweenService:Create(UIStroke_7, TweenInfo.new(0.2, Enum.EasingStyle.Quad, Enum.EasingDirection.Out), {Color = Color3.fromRGB(255, 255, 255)}):Play()     
                    TweenService:Create(UIStroke_8, TweenInfo.new(0.2, Enum.EasingStyle.Quad, Enum.EasingDirection.Out), {Color = Color3.fromRGB(255, 255, 255)}):Play()     
                
                    if not SliderNhulon.Connection then
                        SliderNhulon.Connection = Runservice.RenderStepped:Connect(function()
                            SliderNhulon:SetValue()
                        end)
                    end
                end
            end)

            uis.InputEnded:Connect(function(input, gpe)
                if gpe then return end
                
                if input.UserInputType == Enum.UserInputType.MouseButton1 then
                    SliderNhulon.MouseDown = false

                    if SliderNhulon.Hover then
                        TweenService:Create(UIStroke_7, TweenInfo.new(0.2, Enum.EasingStyle.Quad, Enum.EasingDirection.Out), {Color = Color3.fromRGB(144, 144, 144)}):Play()     
                        TweenService:Create(UIStroke_8, TweenInfo.new(0.2, Enum.EasingStyle.Quad, Enum.EasingDirection.Out), {Color = Color3.fromRGB(144, 144, 144)}):Play()     
                        TweenService:Create(Draggable, TweenInfo.new(0.2, Enum.EasingStyle.Quad, Enum.EasingDirection.Out), {BackgroundColor3 = Color3.fromRGB(106, 106, 106)}):Play()
                    else
                        TweenService:Create(UIStroke_7, TweenInfo.new(0.2, Enum.EasingStyle.Quad, Enum.EasingDirection.Out), {Color = Color3.fromRGB(144, 144, 144)}):Play()     
                        TweenService:Create(UIStroke_8, TweenInfo.new(0.2, Enum.EasingStyle.Quad, Enum.EasingDirection.Out), {Color = Color3.fromRGB(144, 144, 144)}):Play()     
                        TweenService:Create(Draggable, TweenInfo.new(0.2, Enum.EasingStyle.Quad, Enum.EasingDirection.Out), {BackgroundColor3 = Color3.fromRGB(106, 106, 106)}):Play()     
                    end

                    if SliderNhulon.Connection then SliderNhulon.Connection:Disconnect() end
                    SliderNhulon.Connection = nil
                    end                 
                end)

            return SliderNhulon
        end

