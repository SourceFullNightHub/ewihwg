    Description = "Join Job ID Your Put In TextBox",
    CallBack = function()
        game:GetService("TeleportService"):TeleportToPlaceInstance(game.placeId, _G.S.Job, plr)
    end
})

S:CreateButton({
    Name = "Hop Servers",
    Description = "Change Servers",
    CallBack = function()
        HopServer()
    end
})

E:CreateSection({ Text = "Sea Event" })

E:CreateToggle({
    Name = "Start Sea Event",
    Description = "Auto Teleport To Tiki and Buy Ship",
    Default = _G.S.StartSea,
    CallBack = function(Value)
        _G.S.StartSea = Value
        SaveSettings()
    end
})

spawn(function()
    while wait() do
        if _G.S.StartSea and CheckShip() then
            if game.Players.LocalPlayer.Character:WaitForChild("Humanoid").Sit == false then
                if not CheckEnemies("Shark") and not CheckEnemies("Terrorshark") and not CheckEnemies("Fish Crew Member") and not CheckSeaBeast() and not CheckGhoshShip() and not CheckPirateBoat() then
                    table.foreach(GetYourShip():GetDescendants(), function(a, child)
                        if child:IsA("BasePart") or child:IsA("MeshPart") or child:IsA("Part") then
                            child.CanCollide = false
                        end
                    end)
                    TPP(GetYourShip():FindFirstChild("VehicleSeat").CFrame * CFrame.new(0, 1, 0))
                end
            end
        end
    end
end)

spawn(function()
    while wait() do
        if _G.S.StartSea then
            if not game:GetService("Workspace").Boats:FindFirstChild("PirateBrigade") then
                buyb = TPP(CFrame.new(-16927.451171875, 9.0863618850708, 433.8642883300781))
                if (CFrame.new(-16927.451171875, 9.0863618850708, 433.8642883300781).Position - game.Players.LocalPlayer.Character.HumanoidRootPart.Position).magnitude <= 10 then
                    if buyb then 
                        buyb:Stop() 
                    end
                    local args = {
                        [1] = "BuyBoat",
                        [2] = "PirateBrigade"
                    }
                    game:GetService("ReplicatedStorage").Remotes.CommF_:InvokeServer(unpack(args))
                end
            end
        end
    end
end)

spawn(function()
    while wait() do
        if _G.S.StartSea then
            if CheckShip() then
                if game.Players.LocalPlayer.Character:WaitForChild("Humanoid").Sit == true then
                    repeat wait()
                        if (CFrame.new(-17013.80078125, 10.962434768676758, 438.0169982910156).Position - game.Players.LocalPlayer.Character.HumanoidRootPart.Position).magnitude <= 10 then
                            TPB(CFrame.new(-44842.3945, 10.7790766, 16911.3477))
                        elseif (CFrame.new(-44842.3945, 10.7790766, 16911.3477).Position - game.Players.LocalPlayer.Character.HumanoidRootPart.Position).magnitude <= 10 then
                            TPB(CFrame.new(-37952.49609375, 10.96342945098877, -1324.12109375))
                        end
                    until CheckGhoshShip() or CheckPirateBoat() or CheckSeaBeast() or CheckEnemies("Shark") or CheckEnemies("Terrorshark") or CheckEnemies("Piranha") or CheckEnemies("Fish Crew Member") or plr.Character.Humanoid.Sit == false or _G.S.StartSea == false
                end
            end
        end
    end
end)

spawn(function()
    while wait() do
        if _G.S.StartSea then
            if CheckEnemies("Shark") or CheckEnemies("Terrorshark") or CheckEnemies("Piranha") or CheckEnemies("Fish Crew Member") or CheckGhoshShip() or CheckSeaBeast() or CheckPirateBoat() then
                plr.Character.Humanoid.Sit = false
            end
        end
    end
end)

E:CreateToggle({
    Name = "Safe Farm",
    Description = "If Low Health tween to Y",
    Default = _G.S.Y,
    CallBack = function(Value)
        _G.S.Y = Value
        SaveSettings()
    end
})

spawn(function()
    while task.wait() do
        if plr.Character.Humanoid.Health <= 3999 then
            CFSafe = CFrame.new(20, 80, 20)
        elseif plr.Character.Humanoid.Health >= 4000 then
            CFSafe = CFrame.new(2, 20, 2)
        end
    end
end)

local FarmmingSea = E:CreateSection({Text = 'Farmming Sea Event'})

E:CreateToggle({
    Name = "Auto Shark",
    Description = "Auto Kill Shark",
    Default = _G.S.KillShark,
    CallBack = function(Value)
        _G.S.KillShark = Value
        SaveSettings()
    end
})

spawn(function()
    while task.wait() do
        if _G.S.KillShark and Sea3 then
            if CheckEnemies("Shark") then
                KillSeaEvent("Shark")
            end
        end
    end
end)

