                    Time = 1,
                })
                ToggleDitCyborh:Set(false)
            elseif plr.Data.Level.Value >= 300 and not CheckInventory("Cool Shades") then
                if CheckBoss("Cyborg") then
                    KillBoss("Cyborg", _G.S.BopdaiCyborg)
