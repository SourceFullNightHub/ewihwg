                            elseif not _G.S.ClaimQuestBQ then
                                if CheckGlassCake() and not CheckEnemies("Cake Prince") then
                                    if CheckEnemies("Baking Staff") or CheckEnemies("Cookie Crafter") or CheckEnemies("Cake Guard") or CheckEnemies("Head Baker") then
                                        KillMob("Baking Staff", _G.S.StFarm)
                                        KillMob("Cookie Crafter", _G.S.StFarm)
                                        KillMob("Cake Guard", _G.S.StFarm)
                                        KillMob("Head Baker", _G.S.StFarm)
                                    else
                                        if _G.S.BypassTele then
                                            if CheckDistancelonhon(CFrame.new(-2091.911865234375, 70.00884246826172, -12142.8359375), 2500) then
                                                Bypass(CFrame.new(-2091.911865234375, 70.00884246826172, -12142.8359375))
                                            elseif CheckDistancebehon(CFrame.new(-2091.911865234375, 70.00884246826172, -12142.8359375), 2500) then
                                                Tween(CFrame.new(-2091.911865234375, 70.00884246826172, -12142.8359375))
                                            end
                                        else
                                            Tween(CFrame.new(-2091.911865234375, 70.00884246826172, -12142.8359375))
                                        end
                                    end
                                end
                            end
                        end
                    end
                elseif _G.S.SelectMethodFarm == "Bone" and Sea3 then
                    if _G.S.ClaimQuestBQ then
                        if not CheckQuestVisible() or not CheckTextQuest("Posessed Mummy") then
                            TakeQuest(CFrame.new(-9516.99316, 172.017181, 6078.46533, 0, 0, -1, 0, 1, 0, 1, 0, 0), "HauntedQuest2", 2)
                        elseif CheckQuestVisible() and CheckTextQuest("Posessed Mummy") then
                            if CheckEnemies("Reborn Skeleton") or CheckEnemies("Living Zombie") or CheckEnemies("Demonic Soul") or CheckEnemies("Posessed Mummy") then
                                KillMob("Posessed Mummy", _G.S.StFarm)
                                KillMob("Reborn Skeleton", _G.S.StFarm)
                                KillMob("Living Zombie", _G.S.StFarm)
                                KillMob("Demonic Soul", _G.S.StFarm)
                            else
                                if _G.S.BypassTele then
                                    if CheckDistancelonhon(CFrame.new(-9515.75, 174.8521728515625, 6079.40625), 2500) then
                                        Bypass(CFrame.new(-9515.75, 174.8521728515625, 6079.40625))
                                    elseif CheckDistancebehon(CFrame.new(-9515.75, 174.8521728515625, 6079.40625), 2500) then
                                        Tween(CFrame.new(-9515.75, 174.8521728515625, 6079.40625))
                                    end
                                else
                                    Tween(CFrame.new(-9515.75, 174.8521728515625, 6079.40625))
                                end
                            end
                        end
                    elseif not _G.S.ClaimQuestBQ then
                        if CheckEnemies("Reborn Skeleton") or CheckEnemies("Living Zombie") or CheckEnemies("Demonic Soul") or CheckEnemies("Posessed Mummy") then
                            KillMob("Posessed Mummy", _G.S.StFarm)
                            KillMob("Reborn Skeleton", _G.S.StFarm)
                            KillMob("Living Zombie", _G.S.StFarm)
                            KillMob("Demonic Soul", _G.S.StFarm)
                        else
                            if _G.S.BypassTele then
                                if CheckDistancelonhon(CFrame.new(-9515.75, 174.8521728515625, 6079.40625), 2500) then
                                    Bypass(CFrame.new(-9515.75, 174.8521728515625, 6079.40625))
                                elseif CheckDistancebehon(CFrame.new(-9515.75, 174.8521728515625, 6079.40625), 2500) then
                                    Tween(CFrame.new(-9515.75, 174.8521728515625, 6079.40625))
                                end
                            else
                                Tween(CFrame.new(-9515.75, 174.8521728515625, 6079.40625))
                            end
                        end
                    end
                end
            end
    end
end)

spawn(function()
    pcall(function()
        while wait() do
            if _G.S.StFarm then
                if game:GetService("Players").LocalPlayer.PlayerGui.Main.PvpDisabled.Visible == true then
                    game:GetService("ReplicatedStorage").Remotes.CommF_:InvokeServer("EnablePvp")
                end
            end
        end
    end)
end)

H:CreateSection({ Text = "Mastery Farmming" })

H:CreateDropdown({
    Name = "Select Mastery Farm",
    Description = "Select Gun or Fruits to Farm mastery",
    Options = {"Fruit", "Gun"},
    Default = _G.S.SelectMastery,
    CallBack = function(Value)
        _G.S.SelectMastery = Value
        SaveSettings()
    end
})

H:CreateToggle({
    Name = "Auto Mastery",
    Description = "nil",
    Default = _G.S.AutoFarmMastery,
    CallBack = function(Value)
        _G.S.AutoFarmMastery = Value
        SaveSettings()
        if _G.S.AutoFarmMastery == false then
            SpamSkill = false
        end
    end
})

spawn(function()
    while task.wait() do
        if _G.S.AutoFarmMastery then
            pcall(function()
                S()
                if CheckEnemies(Mob1) then
                    for i, v in pairs(WS.Enemies:GetChildren()) do
                        if v.Name == Mob1 then
                            if v.Humanoid.Health > 0 and v:FindFirstChild("HumanoidRootPart") and v.Parent then
                                HealthMin = v.Humanoid.MaxHealth * 25/100
                                repeat task.wait()
                                    if _G.S.SelectMastery == "Fruit" then
                                        if v.Humanoid.Health < HealthMin then
                                            EnableBuso()
                                            EquipTool(plr.Data.DevilFruit.Value)
                                            v.HumanoidRootPart.Size = Vector3.new(30, 30, 30)
                                            v.Head.CanCollide = false
                                            v.HumanoidRootPart.CanCollide = false
                                            v.Humanoid.WalkSpeed = 0
                                            SpamSkillDF = true
                                            SkillAim = true
                                            AimPosition = v.HumanoidRootPart.Position
                                        else 
                                            EnableBuso()
                                            EquipWeapon()
                                            v.HumanoidRootPart.Size = Vector3.new(30, 30, 30)
                                            v.Head.CanCollide = false
                                            v.HumanoidRootPart.CanCollide = false
                                            v.Humanoid.WalkSpeed = 0
                                            SpamSkillDF = false
                                            SkillAim = false
                                            AimPosition = nil
                                        end
                                    elseif _G.S.SelectMastery == "Gun" then
                                        if v.Humanoid.Health < HealthMin then
                                            EnableBuso()
                                            EquipTool(Gun)
                                            v.HumanoidRootPart.Size = Vector3.new(30, 30, 30)
                                            v.Head.CanCollide = false
                                            v.HumanoidRootPart.CanCollide = false
                                            v.Humanoid.WalkSpeed = 0
                                            SpamSkillGun = true
                                            SkillAim = true
                                            AimPosition = v.HumanoidRootPart.Position
                                        else 
                                            EnableBuso()
                                            EquipWeapon()
                                            v.HumanoidRootPart.Size = Vector3.new(30, 30, 30)
                                            v.Head.CanCollide = false
                                            v.HumanoidRootPart.CanCollide = false
                                            v.Humanoid.WalkSpeed = 0
                                            SpamSkillGun = false
                                            SkillAim = false
                                            AimPosition = nil
                                        end
                                    end
                                    Click()
                                    Tween(v.HumanoidRootPart.CFrame * CFrame.new(2, 20, 2))
                                    BringMob(Mob1)
                                until not v.Parent or not v:FindFirstChild("HumanoidRootPart") or not _G.S.AutoFarmMastery
                                SpamSkillGun = false
                                SpamSkillDF = false
                                SkillAim = false
                                AimPosition = nil
                            end
                        end
                    end
                else
                    for i,v in pairs(game.Workspace["_WorldOrigin"].EnemySpawns:GetChildren()) do
                        if string.find(v.Name, Mob1) then
                            Tween(v.CFrame * CFrame.new(0,15,0))
                        end
                    end
                end
            end)
        end
    end
end)

H:CreateSection({ Text = "Chest Farmming" })

local ChestTween = H:CreateToggle({
    Name = "Auto Chest <Tween>",
    Description = "Tween To Chest In Workspace",
    Default = _G.S.AutoChestTween,
    CallBack = function(Value)
        _G.S.AutoChestTween = Value
        SaveSettings()
    end
})

local ChestInstance = H:CreateToggle({
    Name = "Auto Chest <Instance>",
    Description = "Instance To Chest In Workspace",
    Default = _G.S.AutoChestInstance,
    CallBack = function(Value)
        _G.S.AutoChestInstance = Value
        SaveSettings()
    end
})

spawn(function()
    while task.wait() do
        if _G.S.AutoChestTween or _G.S.AutoChestInstance then
            v = GetNearChest()
            repeat task.wait()
                if _G.S.AutoChestTween then
                    Tween(v.CFrame)
                elseif _G.S.AutoChestInstance then
                    InstanceTween(v.CFrame)
                    game.ReplicatedStorage.Remotes.CommF_:InvokeServer("SetTeam","Pirates")
                end
                if (v.Position - plr.Character.HumanoidRootPart.Position).Magnitude <= 10 then
                    SendKeyEvents("W")
                end
            until not v.Parent or not _G.S.AutoChestTween and not _G.S.AutoChestInstance
        end
    end
end)

H:CreateToggle({
    Name = "Tween Safe Zone If Have Fist Of Darkness or God's Chalice",
    Description = "nil",
    Default = _G.S.SafeChest,
    CallBack = function(Value)
        _G.S.SafeChest = Value
        SaveSettings()
    end
})

spawn(function()
    while task.wait() do
        if _G.S.SafeChest then
            if plr.Backpack:FindFirstChild("God's Chalice") or plr.Character:FindFirstChild("God's Chalice") or plr.Backpack:FindFirstChild("Fist Of Darkness") or plr.Character:FindFirstChild("Fist Of Darkness") then
                _G.S.AutoChestInstance = false
                _G.S.AutoChestTween = false
                CancelTween()
                InstanceTween(plr.Character.HumanoidRootPart.CFrame)
                wait(.1)
                game:GetService("ReplicatedStorage").Remotes.CommF_:InvokeServer("requestEntrance",Vector3.new(-12549.7236, 337.511505, -7470.36328))
            end
        end
    end
end)

H:CreateSection({ Text = "Boss Farmming" })

if Sea1 then
    TableBoss = {"The Gorilla King","Bobby","Yeti","Mob Leader","Vice Admiral","Warden","Chief Warden","Swan","Magma Admiral","Fishman Lord","Wysper","Thunder God","Cyborg","Saber Expert"}
elseif Sea2 then
    TableBoss = {"Diamond","Jeremy","Fajita","Don Swan","Smoke Admiral","Cursed Captain","Darkbeard","Order","Awakened Ice Admiral","Tide Keeper"}
elseif Sea3 then
    TableBoss = {"Stone","Island Empress","Kilo Admiral","Captain Elephant","Beautiful Pirate","rip_indra True Form","Longma","Soul Reaper","Cake Queen"}
end

local BossStatus = H:CreateLabel({Text = "Boss Status : nil"})

spawn(function()
    while task.wait() do
        pcall(function()
            if CheckBoss(_G.Selectboss) then
                BossStatus:Set("Boss Status : Spawned")
            else
                BossStatus:Set("Boss Status : Not Spawned")
            end
        end)
    end
end)

H:CreateDropdown({
    Name = "Select Boss",
    Description = "Select Boss To FARM",
    Options = TableBoss,
    Default = "",
    CallBack = function(Value)
        _G.Selectboss = Value
    end
})

H:CreateToggle({
    Name = "Take Quest Boss",
    Description = "Take Quest Boss Your Selected",
    Default = _G.S.TakeQuestBoss,
    CallBack = function(Value)
        _G.S.TakeQuestBoss = Value
        SaveSettings()
    end
})

H:CreateToggle({
    Name = "Auto Boss",
    Description = "Auto Kill Boss Your Select If Spawned",
    Default = false,
    CallBack = function(Value)
        _G.AutoBoss = Value
    end
})

spawn(function()
    while task.wait() do
        GetBoss()
        if _G.AutoBoss and _G.S.TakeQuestBoss and NQBoss ~= nil then
            if not CheckQuestVisible() or not CheckTextQuest(Boss) then
                TakeQuest(CQBoss, NQBoss, LQBoss)
            elseif CheckQuestVisible() and CheckTextQuest(Boss) then
                if CheckEnemies(_G.Selectboss) then
                    KillMob(_G.Selectboss, _G.AutoBoss)
                else
                    if _G.S.BypassTele then
                        Byp(CFBoss)
                    else
                        Tween(CFBoss)
                    end
                end
            end
        elseif _G.AutoBoss and not _G.S.TakeQuestBoss or _G.AutoBoss and _G.S.TakeQuestBoss and NQBoss == nil then
            GetBoss()
            if CheckEnemies(_G.Selectboss) then
                KillMob(_G.Selectboss, _G.AutoBoss)
            else
                if _G.S.BypassTele then
                    Byp(CFBoss)
                else
                    Tween(CFBoss)
                end
            end
        end
    end
end)

H:CreateSection({ Text = "Others Farmming" })

H:CreateToggle({
    Name = "Auto Pirates Raid",
    Description = "Auto Kill All Pirates Raid In Castle On The Sea(function Only Sea 3)",
    Default = _G.S.PiratesRaid,
    CallBack = function(Value)
        _G.S.PiratesRaid = Value
        SaveSettings()
    end
})

