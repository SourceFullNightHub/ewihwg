T:CreateSection({ Text = "Players" })

T:CreateToggle({
    Name = "Walk On Water",
    Description = "Walk On Water",
    Default = true,
    CallBack = function(Value)
        if Value then
            game:GetService("Workspace").Map["WaterBase-Plane"].Size = Vector3.new(1000,112,1000)
        else
            game:GetService("Workspace").Map["WaterBase-Plane"].Size = Vector3.new(1000,80,1000)
        end
    end
})

T:CreateToggle({
    Name = "Auto Buso",
    Description = "Auto Turn On Buso Haki",
    Default = true,
    CallBack = function(Value)
        getgenv().Buso = Value
        SaveSettings()
    end
})

spawn(function()
    while task.wait() do
        if getgenv().Buso then
            if not plr.Character:FindFirstChild("HasBuso") then
                game:GetService("ReplicatedStorage").Remotes.CommF_:InvokeServer("Buso")
            end
        end
    end
end)

T:CreateToggle({
    Name = "Run Speed",
    Description = "Boots Speed For You",
    Default = true,
    CallBack = function(Value)
        SpeedRun = Value
        if Value then
            if game:GetService("Players").LocalPlayer.Character.HumanoidRootPart:FindFirstChild("Night Hub Ngon Vai") then
                game:GetService("Players").LocalPlayer.Character.HumanoidRootPart:FindFirstChild("Night Hub Ngon Vai"):Destroy()
            end
        end
    end
})

T:CreateToggle({
    Name = "Remove Notifications",
    Description = "Remove All Notifications In Game",
    Default = _G.S.xoanotify,
    CallBack = function(Value)
        _G.S.xoanotify = Value
        SaveSettings()
    end
})

spawn(function()
    while task.wait() do
        if _G.S.xoanotify then
            game.Players.LocalPlayer.PlayerGui.Notifications.Enabled = false
        else
            game.Players.LocalPlayer.PlayerGui.Notifications.Enabled = true
        end
    end
end)

T:CreateToggle({
    Name = "Remove Damage Text",
    Description = "Remove All Damage Text If Your Attack Mob",
    Default = _G.S.xoadamage,
    CallBack = function(Value)
        _G.S.xoadamage = Value
        SaveSettings()
    end
})
spawn(function()
    while task.wait() do
        if _G.S.xoadamage then
            game:GetService("ReplicatedStorage").Assets.GUI.DamageCounter.Enabled = false
        else
            game:GetService("ReplicatedStorage").Assets.GUI.DamageCounter.Enabled = true
        end
    end
end)
