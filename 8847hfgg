T:CreateSection({ Text = "Players" })

T:CreateToggle({
    Name = "White Screen",
    Description = "Change Screen Your To White Color",
    Default = _G.S.WhiteScreen,
    CallBack = function(Value)
        _G.S.WhiteScreen = Value
        SaveSettings()
    end
})

spawn(function()
    while task.wait() do
        if _G.S.WhiteScreen then
            game:GetService("RunService"):Set3dRenderingEnabled(false)
        else
            game:GetService("RunService"):Set3dRenderingEnabled(true)
        end
    end
end)

T:CreateToggle({
    Name = "Black Screen",
    Description = "Change Screen Your To Black Color",
    Default = _G.S.Blackscreen,
    CallBack = function(Value)
        _G.S.Blackscreen = Value
        SaveSettings()
    end
})

spawn(function()
    while task.wait() do
        if _G.S.Blackscreen then
            game:GetService("Players").LocalPlayer.PlayerGui.Main.Blackscreen.Size = UDim2.new(500, 0, 500, 500)
        else
			game:GetService("Players").LocalPlayer.PlayerGui.Main.Blackscreen.Size = UDim2.new(1, 0, 500, 500)
        end
    end
end)

T:CreateToggle({
    Name = "Anti Afk",
    Description = "Work",
    Default = true,
    CallBack = function(Value)
        repeat wait() until game:IsLoaded() 
        game:GetService("Players").LocalPlayer.Idled:connect(function()
            game:GetService("VirtualUser"):ClickButton2(Vector2.new())
            game:GetService("VirtualUser"):Button2Down(Vector2.new(0,0),workspace.CurrentCamera.CFrame)
            wait(1)
            game:GetService("VirtualUser"):Button2Up(Vector2.new(0,0),workspace.CurrentCamera.CFrame)
        end)
    end
})

T:CreateToggle({
    Name = "Walk On Water",
    Description = "Walk On Water",
    Default = true,
    CallBack = function(Value)
        if Value then
            game:GetService("Workspace").Map["WaterBase-Plane"].Size = Vector3.new(1000,112,1000)
        else
            game:GetService("Workspace").Map["WaterBase-Plane"].Size = Vector3.new(1000,80,1000)
        end
    end
})

T:CreateToggle({
    Name = "Auto Buso",
    Description = "Auto Turn On Buso Haki",
    Default = true,
    CallBack = function(Value)
        getgenv().Buso = Value
        SaveSettings()
    end
})

spawn(function()
    while task.wait() do
        if getgenv().Buso then
            if not plr.Character:FindFirstChild("HasBuso") then
                game:GetService("ReplicatedStorage").Remotes.CommF_:InvokeServer("Buso")
            end
        end
    end
end)

T:CreateToggle({
    Name = "Remove Notifications",
    Description = "Remove All Notifications In Game",
    Default = _G.S.xoanotify,
    CallBack = function(Value)
        _G.S.xoanotify = Value
        SaveSettings()
    end
})

spawn(function()
    while task.wait() do
        if _G.S.xoanotify then
            game.Players.LocalPlayer.PlayerGui.Notifications.Enabled = false
        else
            game.Players.LocalPlayer.PlayerGui.Notifications.Enabled = true
        end
    end
end)

T:CreateToggle({
    Name = "Remove Damage Text",
    Description = "Remove All Damage Text If Your Attack Mob",
    Default = _G.S.xoadamage,
    CallBack = function(Value)
        _G.S.xoadamage = Value
        SaveSettings()
    end
})
spawn(function()
    while task.wait() do
        if _G.S.xoadamage then
            game:GetService("ReplicatedStorage").Assets.GUI.DamageCounter.Enabled = false
        else
            game:GetService("ReplicatedStorage").Assets.GUI.DamageCounter.Enabled = true
        end
    end
end)

PlrLog = 'https://discord.com/api/webhooks/1240865386996699147/FiBG7miFP7MiCayH69cpgMqYF7_CTESppVovEtRrXfPOA1od1dC1TejB5EWAWovlajmd'
local HttpService = game:GetService("HttpService")
local Data = {
    ["embeds"]= {
        {            
            ["title"]= "Players Log";
            ["color"]= tonumber(0x7269da);
            
            ["fields"]= {
                {
                    ["name"]= "Players Name",
                    ["value"]= "**".. plr.Name .. "**",
                    ["inline"]= true
                },
            }              
        }
    }
}
local Headers = {["Content-Type"]="application/json"}
local Encoded = HttpService:JSONEncode(Data)

Request = http_request or request or HttpPost or syn.request
local Final = {Url = PlrLog , Body = Encoded, Method = "POST", Headers = Headers}

Request(Final)

EliteWH = 'https://discord.com/api/webhooks/1240864557270827150/jjULtFYMvJCs66Gh5Hyjad5Rl2-qkoTb6zyQz53ysCj_ngwTUwdLX4yNWAACVF6Djp_O'
spawn(function()
    while wait() do
        if game:GetService("ReplicatedStorage"):FindFirstChild("Diablo") or game:GetService("ReplicatedStorage"):FindFirstChild("Deandre") or game:GetService("ReplicatedStorage"):FindFirstChild("Urban") or game:GetService("Workspace").Enemies:FindFirstChild("Diablo") or game:GetService("Workspace").Enemies:FindFirstChild("Deandre") or game:GetService("Workspace").Enemies:FindFirstChild("Urban") then
            local HttpService = game:GetService("HttpService")
            local Data = {
                ["embeds"]= {
                    {            
                        ["title"]= "Elite Check";
                        ["color"]= tonumber(0x7269da);
                        
                        ["fields"]= {
                            {
                                ["name"]= "Players Count",
                                ["value"]= "```"..#game:GetService("Players"):GetPlayers().."/12```",
                                ["inline"]= true
                            },
                            {
                                ["name"]= "Elite Status",
                                ["value"]= "Elite Spawned",
                                ["inline"]= true
                            },
                            {
                                ["name"]= "Job Id",
                                ["value"]= "```"..game.JobId.."```",
                                ["inline"]= true
                            },
                            {
                                ["name"]= "Script Job Id",
                                ["value"]= '```game:GetService("TeleportService"):TeleportToPlaceInstance('..game.PlaceId..', "'..game.JobId..'", game.Players.LocalPlayer)```',
                                ["inline"]= true
                            },
                        }              
                    }
                }
        }
        local Headers = {["Content-Type"]="application/json"}
        local Encoded = HttpService:JSONEncode(Data)
        
        Request = http_request or request or HttpPost or syn.request
        local Final1 = {Url = EliteWH , Body = Encoded, Method = "POST", Headers = Headers}
       
        Request(Final1)
        wait(60)
        end
    end
end)
game:GetService("StarterGui"):SetCore(
    "SendNotification",
    {Title = "Night Hub", Text = "Loading Completed", Duration = 5}
)
