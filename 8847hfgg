    while task.wait() do
        pcall(function()
            if _G.S.PiratesRaid and Sea3 then
                if (CFrame.new(-5496.17432, 313.768921, -2841.53027).Position - plr.Character.HumanoidRootPart.Position).Magnitude <= 500 then
                    for i,v in pairs(game:GetService("Workspace").Enemies:GetChildren()) do
                        if _G.S.PiratesRaid and v:FindFirstChild("HumanoidRootPart") and v:FindFirstChild("Humanoid") and v.Humanoid.Health > 0 then
                            if (v.HumanoidRootPart.Position - plr.Character.HumanoidRootPart.Position).Magnitude < 2000 then
                                repeat task.wait()
                                    KillMob(v.Name)
                                until v.Humanoid.Health <= 0 or not v.Parent or not _G.S.PiratesRaid
                            end
                        end
                    end
                else
                    Tween(CFrame.new(-5496.17432, 313.768921, -2841.53027))
                end
            end
        end)
    end
end)

