E:CreateToggle({
    Name = "Auto Piranha + Fish Crew Member",
    Description = "Auto Kill Piranha and Fish Crew Member",
    Default = _G.S.KillFishAndPirnha,
    CallBack = function(Value)
        _G.S.KillFishAndPirnha = Value
        SaveSettings()
    end
})

spawn(function()
    while task.wait() do
        if _G.S.KillFishAndPirnha and Sea3 then
            if CheckEnemies("Piranha") or CheckEnemies("Fish Crew Member") then
                KillSeaEvent("Piranha")
                KillSeaEvent("Fish Crew Member")
            end
        end
    end
end)

E:CreateToggle({
    Name = "Auto Ghosh Ship",
    Description = "Auto Ghosh Ship",
    Default = _G.S.AutoGhoshShip,
    CallBack = function(Value)
        _G.S.AutoGhoshShip = Value
        SaveSettings()
    end
})

spawn(function()
    while task.wait() do
        if _G.S.AutoGhoshShip and Sea3 then
            pcall(function()
                if CheckGhoshShip() then
                    if (CheckGhoshShip().Engine.Position - plr.Character.HumanoidRootPart.Position).Magnitude <= 3000 then
                        repeat task.wait()
                            plr.Character.Humanoid.Sit = false
                            Tween(CheckGhoshShip().Engine.CFrame * CFrame.new(0, -20, 0))
                            if (CheckGhoshShip().Engine.Position - plr.Character.HumanoidRootPart.Position).Magnitude <= 2500 then
                                SpamSkill = true
                                AimSkill = plr.Character.HumanoidRootPart.CFrame * CFrame.new(0, -5, 0)
                            end
                        until not v.Parent or not CheckGhoshShip()
                        SpamSkill = false
                    end
                end
            end)
        end
    end
end)

E:CreateToggle({
    Name = "Auto Ship",
    Description = "Auto Ship",
    Default = _G.S.AutoShip,
    CallBack = function(Value)
        _G.S.AutoShip = Value
        SaveSettings()
    end
})

spawn(function()
    while task.wait() do
        if _G.S.AutoGhoshShip and Sea3 then
            pcall(function()
                if CheckPirateBoat() then
                    if (CheckPirateBoat().Engine.Position - plr.Character.HumanoidRootPart.Position).Magnitude <= 3000 then
                        repeat task.wait()
                            plr.Character.Humanoid.Sit = false
                            if (CheckPirateBoat().Engine.Position - plr.Character.HumanoidRootPart.Position).Magnitude <= 2500 then
                                SpamSkill = true
                                AimSkill = plr.Character.HumanoidRootPart.CFrame * CFrame.new(0, -5, 0)
                            end
                        until not v.Parent or not CheckPirateBoat()
                        SpamSkill = false
                    end
                end
            end)
        end
    end
end)

E:CreateToggle({
    Name = "Auto SeaBeast",
    Description = "Auto Kill SeaBeast",
    Default = _G.S.AutoSeaBeast,
    CallBack = function(Value)
        _G.S.AutoSeaBeast = Value
        SaveSettings()
    end
})

spawn(function()
    while task.wait() do
        pcall(function()
            if _G.S.AutoSeaBeast and Sea3 then
                if CheckSeaBeast() then
                    local v = CheckSeaBeast()
                    if v:FindFirstChild("HumanoidRootPart") then
                        if (v.HumanoidRootPart.Position - plr.Character.HumanoidRootPart.Position).Magnitude <= 3000 then
                            repeat task.wait()
                                TeleportSeabeast(v)
                                if (v.HumanoidRootPart.Position - plr.Character.HumanoidRootPart.Position).Magnitude <= 2500 then
                                    SpamSkill = true
                                end
                            until not CheckSeaBeast() or not v.Parent or not v:FindFirstChild("HumanoidRootPart")
                            SpamSkill = false
                        end
                    end
                end
            end
        end)
    end
end)

T:CreateSection({ Text = "Settings FastAttack & Bring Mob & Bypass" })

T:CreateToggle({
    Name = "Enable FastAttack",
    Description = "Attack Fast And No CoolDown",
    Default = _G.S.FastAttack,
    CallBack = function(Value)
        _G.S.FastAttack = Value
        SaveSettings()
    end
})

T:CreateDropdown({
    Name = "FastAttack Cooldown",
    Description = "Select CoolDown Delay Attack",
    List = { "0", "0.0015", "0.00175", "0.01", "0.0175", "0.05", "0.075", "0.1", "0.15", "0.155", "0.16", "0.165", "0.17", "0.175", "0.18", "0.185" },
    Default = _G.S.FastAttackDelay,
    CallBack = function(Value)
        _G.S.FastAttackDelay = Value
        SaveSettings()
    end
})

spawn(function()
    while task.wait() do
        if _G.S.FastAttack then
            pcall(function()
                repeat
                    task.wait(_G.S.FastAttackDelay)
                    AttackNoCD()
                until not _G.S.FastAttack
            end)
        end
    end
end)

