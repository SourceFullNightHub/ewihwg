PlayerList = {}
for i, v in ipairs(pl:GetPlayers()) do
    table.insert(PlayerList, v.Name)
end

local PlayerListDropdown = L:CreateDropdown({
    Name = "Select Players",
    Description = "Select Players In Servers",
    List = PlayerList,
    Default = "",
    CallBack = function(Value)
        SelectPlayer = Value
    end
})

L:CreateButton({
    Name = "Refresh Players List",
    Description = "I Doesn't think",
    CallBack = function()
        NewPlayerList = {}
        for i,v in pairs(game:GetService("Players"):GetChildren()) do
            table.insert(NewPlayerList, v.Name)
        end
        PlayerListDropdown:Refresh(NewPlayerList)
    end
})

L:CreateToggle({
    Name = "Teleport To Players",
    Description = "Not",
    Default = false,
    CallBack = function(Value)
        TweenPly = Value
	    pcall(function()
            if TweenPly then
                repeat Tween(game:GetService("Players")[SelectPlayer].Character.HumanoidRootPart.CFrame) task.wait() until not TweenPly CancelTween()
            end
        end)
    end
})

L:CreateToggle({
    Name = "Kill Players[Selected]",
    Description = "Auto Kill Players Your Selected",
    Default = false,
    CallBack = function(Value)
        _G.KillPlayer = Value
    end
})

spawn(function()
    while task.wait() do
        if _G.KillPlayer then
            if SelectPlayer ~= nil and game.Players:FindFirstChild(SelectPlayer) and game.Players:FindFirstChild(SelectPlayer).Character.Humanoid.Health > 0 then
                repeat task.wait()
                    EquipWeapon()
                    EnableBuso()
                    game.Players:FindFirstChild(SelectPlayer).Character.HumanoidRootPart.CanCollide = false
                    Tween(game.Players:FindFirstChild(SelectPlayer).Character.HumanoidRootPart.CFrame * CFrame.new(0,5,0))
                    Click()
                until game.Players:FindFirstChild(SelectPlayer).Character.Humanoid.Health <= 0 or not game.Players:FindFirstChild(SelectPlayer) or not  _G.KillPlayer
            end
        end
    end
end)

L:CreateToggle({
    Name = "Auto Kill Nearest Players",
    Description = "Mups",
    Default = false,
    CallBack = function(Value)
        _G.NearestKill = Value
    end
})

spawn(function()
    while wait() do
        if _G.NearestKill then
            for i, v in pairs(Chaster:GetChildren()) do
                if v.Name ~= plr.Name and v.Humanoid.Health > 0 and v.Parent and v:FindFirstChild("HumanoidRootPart") then
                    if (v.HumanoidRootPart.Position - plr.Character.HumanoidRootPart.Position).Magnitude <= 500 then
                        repeat task.wait()
                            v.HumanoidRootPart.Size = Vector3.new(30, 30, 30)
                            Tween(v.HumanoidRootPart.CFrame * CFrame.new(0, 0, 2))
                            Click()
                        until not v.Parent or v.Humanoid.Health <= 0 or not v:FindFirstChild("Humanoid") or _G.NearestKill == false
                    end
                end
            end
        end
    end
end)

E:CreateSection({ Text = "Sea Event" })

E:CreateToggle({
    Name = "Start Sea Event",
