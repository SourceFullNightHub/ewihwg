        IconTab_2.BorderColor3 = Color3.fromRGB(0, 0, 0)
        IconTab_2.ImageColor3 = Color3.fromRGB(144, 144, 144)
        IconTab_2.BorderSizePixel = 0
        IconTab_2.Position = UDim2.new(-0.30252099, 0, 0.0666666701, 0)
        IconTab_2.Size = UDim2.new(0, 25, 0, 25)
        IconTab_2.Image = Options.Icon

        Tab1.Name = "Tab 1"
        Tab1.Parent = ChannelContainer
        Tab1.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
        Tab1.BackgroundTransparency = 1.000
        Tab1.BorderColor3 = Color3.fromRGB(0, 0, 0)
        Tab1.BorderSizePixel = 0
        Tab1.Selectable = false
        Tab1.Size = UDim2.new(1, 0, 1, 0)
        Tab1.CanvasSize = UDim2.new(0, 0, 3, 5)
        Tab1.ScrollBarThickness = 0
        Tab1.Visible = false
        spawn(function()
            while wait() do
                Tab1.CanvasSize = UDim2.new(0,0,0,UIListLayout_2.AbsoluteContentSize.Y + 70)
            end
        end)

        UIPadding_7.Parent = Tab1
        UIPadding_7.PaddingBottom = UDim.new(0, 8)
        UIPadding_7.PaddingLeft = UDim.new(0, 8)
        UIPadding_7.PaddingRight = UDim.new(0, 8)
        UIPadding_7.PaddingTop = UDim.new(0, 8)

        UIListLayout_2.Parent = Tab1
        UIListLayout_2.SortOrder = Enum.SortOrder.LayoutOrder
        UIListLayout_2.Padding = UDim.new(0, 10)

        NameTab.Name = "Name Tab"
        NameTab.Parent = Tab1
        NameTab.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
        NameTab.BackgroundTransparency = 1.000
        NameTab.BorderColor3 = Color3.fromRGB(0, 0, 0)
        NameTab.BorderSizePixel = 0
        NameTab.Size = UDim2.new(1, 0, 0, 19)
        NameTab.Font = Enum.Font.SourceSansBold
        NameTab.Text = Options.Name
        NameTab.TextColor3 = Color3.fromRGB(255, 255, 255)
        NameTab.TextSize = 18.000

        ButtonDisabledTab.MouseEnter:Connect(function()
            if ChannelName ~= ButtonDisabledTab.Name then
                ButtonDisabledTab.TextColor3 = Color3.fromRGB(255,255,255)
                IconTab_2.ImageColor3 = Color3.fromRGB(255, 255, 255)
            end
        end)
        
        ButtonDisabledTab.MouseLeave:Connect(function()
            if ChannelName ~= ButtonDisabledTab.Name then
                ButtonDisabledTab.TextColor3 = Color3.fromRGB(114, 118, 125)
                IconTab_2.ImageColor3 = Color3.fromRGB(144, 144, 144)
            end
        end)   

        ButtonDisabledTab.MouseButton1Click:Connect(function()
            for i, v in next, ChannelContainer:GetChildren() do
                if v.Name == "Tab 1" then
                    v.Visible = false
                end
                Tab1.Visible = true
            end
            for i, v in next, ScrollingTab:GetChildren() do
                if v.ClassName == "TextButton" then
                    v.TextColor3 = Color3.fromRGB(144, 144, 144)
                    v.BackgroundTransparency = 1.000
                end
            end
            ButtonDisabledTab.TextColor3 = Color3.fromRGB(255,255,255)
            ButtonDisabledTab.BackgroundTransparency = 0.9
            ChannelName = ButtonDisabledTab.Name
        end)
        
        if default == false then
            default = true
            ButtonDisabledTab.TextColor3 = Color3.fromRGB(144,144,144)
            ChannelName = ButtonDisabledTab.Name
            ChannelContainer.Visible = true
            Tab1.Visible = false
        end

