A:CreateToggle({
    Name = "Auto Fully Raid Law",
    Description = "Auto Buy Chips, Start Raid And Summon Order Boss",
    Default = _G.S.Memaythangbeo,
    CallBack = function(Value)
        _G.S.Memaythangbeo = Value
        SaveSettings()
    end
})

spawn(function()
    while task.wait() do
        if _G.S.Memaythangbeo and Sea2 then
            if not CheckBoss("Order") then
                if not CheckBackpack("Microchip") then
                    BuyChipLaw()
                elseif CheckBackpack("Microchip") then
                    fireclickdetector(WS.Map.CircleIsland.RaidSummon.Button.Main.ClickDetector)
                end
            elseif CheckBoss("Order") then
                KillMob("Order")
            end
        end
    end
end)

S:CreateSection({ Text = "Status" })

function CheckKatakuriMob()
    if string.len(game:GetService("ReplicatedStorage").Remotes.CommF_:InvokeServer("CakePrinceSpawner")) == 88 then
        return "Killed : "..string.sub(game:GetService("ReplicatedStorage").Remotes.CommF_:InvokeServer("CakePrinceSpawner"),39,41)
    elseif string.len(game:GetService("ReplicatedStorage").Remotes.CommF_:InvokeServer("CakePrinceSpawner")) == 87 then
        return "Killed : "..string.sub(game:GetService("ReplicatedStorage").Remotes.CommF_:InvokeServer("CakePrinceSpawner"),39,40)
    elseif string.len(game:GetService("ReplicatedStorage").Remotes.CommF_:InvokeServer("CakePrinceSpawner")) == 86 then
        return "Killed : "..string.sub(game:GetService("ReplicatedStorage").Remotes.CommF_:InvokeServer("CakePrinceSpawner"),39,39)
    else
        return "Cake Prince Spawned"
    end
end

function CheckElite()
    if game:GetService("ReplicatedStorage"):FindFirstChild("Diablo") or game:GetService("ReplicatedStorage"):FindFirstChild("Deandre") or game:GetService("ReplicatedStorage"):FindFirstChild("Urban") or game:GetService("Workspace").Enemies:FindFirstChild("Diablo") or game:GetService("Workspace").Enemies:FindFirstChild("Deandre") or game:GetService("Workspace").Enemies:FindFirstChild("Urban") then
        return "Elite : Spawned"
    else
        return "Elite : Not Spawn"
    end
end

function MoonTextureId()
    if Sea1 then
        return game:GetService("Lighting").FantasySky.MoonTextureId
    elseif Sea2 then
        return game:GetService("Lighting").FantasySky.MoonTextureId
    elseif Sea3 then
        return game:GetService("Lighting").Sky.MoonTextureId
    end
end  

function CheckMoon()
    OiOi = MoonTextureId()
    if OiOi == "http://www.roblox.com/asset/?id=9709149431" then
        return 'Full Moon'
    elseif OiOi == "http://www.roblox.com/asset/?id=9709149052" then
        return "Next Night"
    elseif OiOi == "http://www.roblox.com/asset/?id=9709143733" or OiOi == "http://www.roblox.com/asset/?id=9709150401" or OiOi == "http://www.roblox.com/asset/?id=9709149680" then
        return "Bad Moon"
    else
        return "Not Moon"
    end
end

function CheckKitsuneIsland()
    if WS.Map:FindFirstChild("KitsuneIsland") then
        return "Kitsune Island : Spawned"
    else
        return "Kitsune Island : Not Spawned"
    end
end

function CheckMirageStatus()
    if WS.Map:FindFirstChild("MysticIsland") then
        return "Mirage : Spawned"
    else
        return "Mirage : Not Spawned"
    end
end

function CheckAcientStatus()
    Valk = CheckInventory("Valkyrie Helm")
    Mirror = CheckInventory("Mirror Fractal")
    if not game.Players.LocalPlayer.Character:FindFirstChild("RaceTransformed") then
        p = "You Not Have V4 Race"
        return p
    end
    local v227 = nil
    local v228 = nil
    local v229 = nil
    v229, v228, v227 = game.ReplicatedStorage.Remotes.CommF_:InvokeServer("UpgradeRace", "Check")
    if v229 == 1 then
        return "Required Train More"
    elseif v229 == 2 or v229 == 4 or v229 == 7 then
        return "Can Buy Gear With " .. v227 .. " Fragments"
    elseif v229 == 3 then
        return "Required Train More"
    elseif v229 == 5 then
        return "You Are Done Your Race."
    elseif v229 == 6 then
        return v228 - 2 .. "/3"
    end
    if v229 ~= 8 then
        if v229 == 0 then
            return "Ready For Trial"
        else
            return "memaybeo"
        end
    end
    return 10 - v228 .. " training sessions."
end

function CheckRaceStatus()
    if not game.ReplicatedStorage.Remotes.CommF_:InvokeServer("CheckTempleDoor") then
        if not Valk or Mirror then
            a = "Your Race : " .. CheckRace() .. " | " .. "Your Need Mirror or Valkyrie To Pull Lever"
        end
        return a
    elseif game.ReplicatedStorage.Remotes.CommF_:InvokeServer("CheckTempleDoor") then
        a = "Your Race : " .. CheckRace() .. " | " .. "Pull Lever Completed"
    end
end

function CheckTime()
    GameTime = "Error"
    local c = game.Lighting
    local ao = c.ClockTime
    if ao >= 16 or ao < 5 then
        GameTime = "Night"
    else
        GameTime = "Day"
    end
    return GameTime
end

function UpdateTime()
    local GameTime = math.floor(workspace.DistributedGameTime+0.5)
    local Hour = math.floor(GameTime/(60^2))%24
    local Minute = math.floor(GameTime/(60^1))%60
    local Second = math.floor(GameTime/(60^0))%60
    return "Client Time "..Hour.. "H "..Minute.."M  "..Second .. "S"
end
    
function CheckStatusFarm()
    if _G.S.SelectMethodFarm == "Level" and _G.S.StFarm then
        return "Farm Level"
    elseif _G.S.SelectMethodFarm == "Cake Prince" and _G.S.StFarm then
        return "Farm Cake Prince"
    elseif _G.S.SelectMethodFarm == "Bone" and _G.S.StFarm then
        return "Farm Bone"
    else
        return "Not Farm"
    end
end

local TimeClient = S:CreateLabel({Text = "Client Time : 0H 0M 0S"})

local StatusFarm = S:CreateLabel({Text = "Status Farm : nil"})

local CakePrinceCount = S:CreateLabel({Text = "Cake Prince : Not Spawn"})

local Elite = S:CreateLabel({Text = "Elite : Not Spawn"})

local Moon = S:CreateLabel({Text = "Moon : Not Moon"})

local Mirage = S:CreateLabel({Text = "Mirage : Not Spawned"})

local KitsuneStt = S:CreateLabel({Text = "Kitsune Island : Not Spawned"})

local AcientStatus = S:CreateLabel({Text = "Acient Status : "})

local RaceStatus = S:CreateLabel({Text = "Race Status : "})

spawn(function()
    while task.wait() do
        TimeClient:Set(UpdateTime())
    end
end)

spawn(function()
    while task.wait() do
        pcall(function()
            StatusFarm:Set("Status Farm : ".. CheckStatusFarm() .. "")
            CakePrinceCount:Set("".. CheckKatakuriMob() .. "")
            Elite:Set("".. CheckElite() .. "")
            Moon:Set("".. CheckMoon() .. " | ".. CheckTime() .. "")
            Mirage:Set("".. CheckMirageStatus() .. "")
            KitsuneStt:Set("".. CheckKitsuneIsland() .. "")
            AcientStatus:Set("".. CheckAcientStatus() .. "")
            RaceStatus:Set("".. CheckRaceStatus() .. "")
        end)
    end
end)

