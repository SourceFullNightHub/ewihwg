        UIPadding_13.PaddingLeft = UDim.new(0, 40)
    
        IconNotify.Name = "Icon Notify"
        IconNotify.Parent = Title_6
        IconNotify.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
        IconNotify.BackgroundTransparency = 1.000
        IconNotify.BorderColor3 = Color3.fromRGB(0, 0, 0)
        IconNotify.BorderSizePixel = 0
        IconNotify.Position = UDim2.new(-0.128596231, 0, 0.0909090936, 0)
        IconNotify.Size = UDim2.new(0, 27, 0, 27)
        IconNotify.Image = Options.Icon
    
        UICorner_15.Parent = Notify
    
        TweenService:Create(Notify, TweenInfo.new(0.5, Enum.EasingStyle.Quint), {Position = UDim2.new(0.737, 0, 0.797, 0)}):Play()
    
        wait(Options.Time - 0.88)
        TweenService:Create(IconNotify, TweenInfo.new(0.4, Enum.EasingStyle.Quint), {ImageTransparency = 1}):Play()
        TweenService:Create(Notify, TweenInfo.new(0.8, Enum.EasingStyle.Quint), {BackgroundTransparency = 0.6}):Play()
        wait(0.3)
        TweenService:Create(UIStroke_8, TweenInfo.new(0.6, Enum.EasingStyle.Quint), {Transparency = 0.9}):Play()
        TweenService:Create(Title_6, TweenInfo.new(0.6, Enum.EasingStyle.Quint), {TextTransparency = 0.4}):Play()
        TweenService:Create(Coginoidi, TweenInfo.new(0.6, Enum.EasingStyle.Quint), {TextTransparency = 0.5}):Play()
        wait(0.05)
    
        Notify:TweenPosition(UDim2.new(20, 0, 0, 0),'In','Quint',0.8,true)
        wait(1.35)
        Notify:Destroy()
    end

    local UITab = {}

    function UITab:CreateTab(Options)
        Options = Options or {}
        Options.Name = Options.Name or "Tab 1"
        Options.Icon = Options.Icon or "rbxassetid://17184558565"

        local ButtonDisabledTab = Instance.new("TextButton")
        local UIPadding_5 = Instance.new("UIPadding")
        local IconTab_2 = Instance.new("ImageLabel")
        local Tab1 = Instance.new("ScrollingFrame")
        local UIPadding_7 = Instance.new("UIPadding")
        local UIListLayout_2 = Instance.new("UIListLayout")
        local NameTab = Instance.new("TextLabel")
        local ChannelName = ""
        defaut = false
        
        ButtonDisabledTab.Name = "ButtonDisabledTab"
        ButtonDisabledTab.Parent = ScrollingTab
        ButtonDisabledTab.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
        ButtonDisabledTab.BackgroundTransparency = 1.000
        ButtonDisabledTab.BorderColor3 = Color3.fromRGB(0, 0, 0)
        ButtonDisabledTab.BorderSizePixel = 0
        ButtonDisabledTab.Size = UDim2.new(1, 0, 0, 30)
        ButtonDisabledTab.Font = Enum.Font.Unknown
        ButtonDisabledTab.Text = Options.Name
        ButtonDisabledTab.TextColor3 = Color3.fromRGB(255, 255, 255)
        ButtonDisabledTab.TextSize = 14.000
        ButtonDisabledTab.TextXAlignment = Enum.TextXAlignment.Left

        UIPadding_5.Parent = ButtonDisabledTab
        UIPadding_5.PaddingLeft = UDim.new(0, 40)

        IconTab_2.Name = "Icon Tab"
        IconTab_2.Parent = ButtonDisabledTab
        IconTab_2.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
        IconTab_2.BackgroundTransparency = 1.000
        IconTab_2.BorderColor3 = Color3.fromRGB(0, 0, 0)
        IconTab_2.ImageColor3 = Color3.fromRGB(144, 144, 144)
        IconTab_2.BorderSizePixel = 0
        IconTab_2.Position = UDim2.new(-0.30252099, 0, 0.0666666701, 0)
        IconTab_2.Size = UDim2.new(0, 25, 0, 25)
        IconTab_2.Image = Options.Icon

        Tab1.Name = "Tab 1"
        Tab1.Parent = ChannelContainer
        Tab1.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
        Tab1.BackgroundTransparency = 1.000
        Tab1.BorderColor3 = Color3.fromRGB(0, 0, 0)
        Tab1.BorderSizePixel = 0
        Tab1.Selectable = false
        Tab1.Size = UDim2.new(1, 0, 1, 0)
        Tab1.CanvasSize = UDim2.new(0, 0, 3, 5)
        Tab1.ScrollBarThickness = 0
        Tab1.Visible = false
        spawn(function()
            while wait() do
                Tab1.CanvasSize = UDim2.new(0,0,0,UIListLayout_2.AbsoluteContentSize.Y + 70)
            end
        end)

        UIPadding_7.Parent = Tab1
        UIPadding_7.PaddingBottom = UDim.new(0, 8)
        UIPadding_7.PaddingLeft = UDim.new(0, 8)
        UIPadding_7.PaddingRight = UDim.new(0, 8)
        UIPadding_7.PaddingTop = UDim.new(0, 8)

        UIListLayout_2.Parent = Tab1
        UIListLayout_2.SortOrder = Enum.SortOrder.LayoutOrder
        UIListLayout_2.Padding = UDim.new(0, 10)

        NameTab.Name = "Name Tab"
        NameTab.Parent = Tab1
        NameTab.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
        NameTab.BackgroundTransparency = 1.000
        NameTab.BorderColor3 = Color3.fromRGB(0, 0, 0)
        NameTab.BorderSizePixel = 0
        NameTab.Size = UDim2.new(1, 0, 0, 19)
        NameTab.Font = Enum.Font.SourceSansBold
        NameTab.Text = Options.Name
        NameTab.TextColor3 = Color3.fromRGB(255, 255, 255)
        NameTab.TextSize = 18.000

        ButtonDisabledTab.MouseEnter:Connect(function()
            if ChannelName ~= ButtonDisabledTab.Name then
                ButtonDisabledTab.TextColor3 = Color3.fromRGB(255,255,255)
                IconTab_2.ImageColor3 = Color3.fromRGB(255, 255, 255)
            end
        end)
        
        ButtonDisabledTab.MouseLeave:Connect(function()
            if ChannelName ~= ButtonDisabledTab.Name then
                ButtonDisabledTab.TextColor3 = Color3.fromRGB(114, 118, 125)
                IconTab_2.ImageColor3 = Color3.fromRGB(144, 144, 144)
            end
        end)   

        ButtonDisabledTab.MouseButton1Click:Connect(function()
            for i, v in next, ChannelContainer:GetChildren() do
                if v.Name == "Tab 1" then
                    v.Visible = false
                end
                Tab1.Visible = true
            end
            for i, v in next, ScrollingTab:GetChildren() do
                if v.ClassName == "TextButton" then
                    v.TextColor3 = Color3.fromRGB(144, 144, 144)
                    v.BackgroundTransparency = 1.000
                end
            end
            ButtonDisabledTab.TextColor3 = Color3.fromRGB(255,255,255)
            ButtonDisabledTab.BackgroundTransparency = 0.9
            ChannelName = ButtonDisabledTab.Name
        end)
        
        if default == false then
            default = true
            ButtonDisabledTab.TextColor3 = Color3.fromRGB(144,144,144)
            ChannelName = ButtonDisabledTab.Name
            ChannelContainer.Visible = true
            Tab1.Visible = false
        end

