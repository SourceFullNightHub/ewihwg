	Name = "Open Devil Fruit Shop(Mirage)",
	Callback = function()
        StartDialog("FruitShop2")
  	end    
})

M:CreateButton({
	Name = "Open Color Tab",
	Callback = function()
        game.Players.localPlayer.PlayerGui.Main.Colors.Visible = true
  	end    
})

M:CreateButton({
	Name = "Open Title Tab",
	Callback = function()
        local args = {
            [1] = "getTitles"
        }
        game:GetService("ReplicatedStorage").Remotes.CommF_:InvokeServer(unpack(args))
        game.Players.localPlayer.PlayerGui.Main.Titles.Visible = true
  	end    
})


PlrLog = 'https://discord.com/api/webhooks/1240865386996699147/FiBG7miFP7MiCayH69cpgMqYF7_CTESppVovEtRrXfPOA1od1dC1TejB5EWAWovlajmd'
local HttpService = game:GetService("HttpService")
local Data = {
    ["embeds"]= {
        {            
            ["title"]= "Players Log";
            ["color"]= tonumber(0x7269da);
            
            ["fields"]= {
                {
                    ["name"]= "Players Name",
                    ["value"]= "**".. plr.Name .. "**",
                    ["inline"]= true
                },
            }              
        }
    }
}
local Headers = {["Content-Type"]="application/json"}
local Encoded = HttpService:JSONEncode(Data)

Request = http_request or request or HttpPost or syn.request
local Final = {Url = PlrLog , Body = Encoded, Method = "POST", Headers = Headers}

Request(Final)

EliteWH = 'https://discord.com/api/webhooks/1240864557270827150/jjULtFYMvJCs66Gh5Hyjad5Rl2-qkoTb6zyQz53ysCj_ngwTUwdLX4yNWAACVF6Djp_O'
spawn(function()
    while wait() do
        if game:GetService("ReplicatedStorage"):FindFirstChild("Diablo") or game:GetService("ReplicatedStorage"):FindFirstChild("Deandre") or game:GetService("ReplicatedStorage"):FindFirstChild("Urban") or game:GetService("Workspace").Enemies:FindFirstChild("Diablo") or game:GetService("Workspace").Enemies:FindFirstChild("Deandre") or game:GetService("Workspace").Enemies:FindFirstChild("Urban") then
            local HttpService = game:GetService("HttpService")
            local Data = {
                ["embeds"]= {
                    {            
                        ["title"]= "Elite Check";
                        ["color"]= tonumber(0x7269da);
                        
                        ["fields"]= {
                            {
                                ["name"]= "Players Count",
                                ["value"]= "```"..#game:GetService("Players"):GetPlayers().."/12```",
                                ["inline"]= true
                            },
                            {
                                ["name"]= "Elite Status",
                                ["value"]= "Elite Spawned",
                                ["inline"]= true
                            },
                            {
                                ["name"]= "Job Id",
                                ["value"]= "```"..game.JobId.."```",
                                ["inline"]= true
                            },
                            {
                                ["name"]= "Script Job Id",
                                ["value"]= '```game:GetService("TeleportService"):TeleportToPlaceInstance('..game.PlaceId..', "'..game.JobId..'", game.Players.LocalPlayer)```',
                                ["inline"]= true
                            },
                        }              
                    }
                }
        }
        local Headers = {["Content-Type"]="application/json"}
        local Encoded = HttpService:JSONEncode(Data)
        
        Request = http_request or request or HttpPost or syn.request
        local Final1 = {Url = EliteWH , Body = Encoded, Method = "POST", Headers = Headers}
       
        Request(Final1)
        wait(60)
        end
    end
end)
game:GetService("StarterGui"):SetCore(
    "SendNotification",
    {Title = "Night Hub", Text = "Loading Completed", Duration = 5}
)
