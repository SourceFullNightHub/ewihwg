    while wait() do
        if _G.Settings.AutoElite and Sea3 then
            if Your.PlayerGui.Main.Quest.Visible == false then
                game:GetService("ReplicatedStorage").Remotes.CommF_:InvokeServer("EliteHunter")
            elseif Your.PlayerGui.Main.Quest.Visible == true then
                if CheckEnemies("Diablo") or CheckEnemies("Deandre") or CheckEnemies("Urban") then
                    for i, v in pairs(game.Workspace.Enemies:GetChildren()) do
                        if v.Name == "Diablo" or v.Name == "Urban" or v.Name == "Deandre" then
                            if v.Humanoid.Health > 0 and v.Parent and v:FindFirstChild("HumanoidRootPart") then
                                repeat wait()
                                    EnableBuso()
                                    EquipTool(ToolFarm)
                                    v.Humanoid.WalkSpeed = 0
                                    v.HumanoidRootPart.Size = Vector3.new(1, 1, 1)
                                    v.HumanoidRootPart.CanCollide = false
                                    v.Head.CanCollide = false
                                    Tween(v.HumanoidRootPart.CFrame * CFrame.new(2, 20, 2))
                                until v.Humanoid.Health <= 0 or not v.Parent or not v:FindFirstChild("HumanoidRootPart") or _G.Settings.AutoElite == false
                            end
                        end
                    end
                else
                    for i, v in pairs(game:GetService("ReplicatedStorage"):GetChildren()) do
                        if v.Name == "Deandre" or v.Name == "Urban" or v.Name == "Diablo" then
                            Tween(v.HumanoidRootPart.CFrame * CFrame.new(2, 20 , 2))
                        end
                    end
                end
            end
        end
    end
end)

