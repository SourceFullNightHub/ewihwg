        _G.S.DitStats = Value
        SaveSettings()
    end
})

spawn(function()
    while task.wait() do
        if _G.S.DitStats then
            local args = {
                [1] = "AddPoint",
                [2] = _G.S.DitStats, 
                [3] = 9999
                }
            game:GetService("ReplicatedStorage").Remotes.CommF_:InvokeServer(unpack(args))
        end
    end
end)

H:CreateToggle({
    Name = "Auto Awakening Race V4",
    Description = "Auto Awakening Race V4 If Can",
    Default = _G.S.AutoAwakeningRace,
    CallBack = function(Value)
        _G.S.AutoAwakeningRace = Value
        SaveSettings()
    end
})

spawn(function()
    while task.wait() do
        if _G.S.AutoAwakeningRace then
            pcall(function()
                if plr.Character:FindFirstChild("RaceEnergy") and plr.Character.RaceEnergy.Value >= 1 and not plr.Character.RaceTransformed.Value then
                    game:service("VirtualInputManager"):SendKeyEvent(true, "Y", false, game)
                    task.wait()
                    game:service("VirtualInputManager"):SendKeyEvent(false, "Y", false, game)
                end
            end)
        end
    end
end)

H:CreateSection({ Text = "Select Method Farm" })

H:CreateDropdown({
    Name = "Select Method Farm",
    Description = "Select Method Farm | EX : Cake Prince or Level or Bone",
    List = { "Level", "Cake Prince", "Bone"},
    Default = _G.S.SelectMethodFarm,
    CallBack = function(Value)
        _G.S.SelectMethodFarm = Value
        SaveSettings()
    end
})

H:CreateToggle({
    Name = "Start Farm",
    Description = "Start Farm Method Your Selected",
    Default = _G.S.StFarm,
    CallBack = function(Value)
        _G.S.StFarm = Value
        SaveSettings()
    end
})

spawn(function()
    while task.wait() do
        if _G.S.StFarm then
            pcall(function()
                if _G.S.SelectMethodFarm == "" or _G.S.SelectMethodFarm == nil then
                    _G.S.SelectMethodFarm = "Level"
                elseif _G.S.SelectMethodFarm == "Level" then
                    if not CheckQuestVisible() or not CheckTextQuest(Mob) then
                        TakeQuestFarmLevel()
                    elseif CheckQuestVisible() and CheckTextQuest(Mob) then
                        if CheckEnemies(Mob) then
                            KillMob(Mob, _G.S.StFarm)
                        else
                            for i,v in pairs(WS["_WorldOrigin"].EnemySpawns:GetChildren()) do
                                if v.Name == Mob then
                                    repeat wait()
                                        Tween(v.CFrame * CFrame.new(2, 20, 2))
                                    until WS.Enemies:FindFirstChild(Mob) or not _G.S.StFarm
                                end
                            end
                        end
                    end
                elseif _G.S.SelectMethodFarm == "Cake Prince" then
                    if CheckEnemies("Cake Prince") and not CheckGlassCake() then
                        for i, v in pairs(WS.Enemies:GetChildren()) do
                            if v.Name == "Cake Prince" and v.Humanoid.Health > 0 and v.Parent and v:FindFirstChild("HumanoidRootPart") then
                                repeat wait()
                                    EnableBuso()
                                    EquipWeapon()
                                    plr.Character.Humanoid.Sit = false
                                    v.HumanoidRootPart.Size = Vector3.new(30, 30, 30)
                                    Tween(v.HumanoidRootPart.CFrame * RandomCFrame)
                                    game:GetService'VirtualUser':CaptureController()
                                    game:GetService'VirtualUser':Button1Down(Vector2.new(1280, 672))
                                until not v.Parent or v.Humanoid.Health <= 0 or not v:FindFirstChild("HumanoidRootPart") or CheckGlassCake()
                            end
                        end
                    else
                        if game.ReplicatedStorage:FindFirstChild("Cake Prince") then
                            Tween(CFrame.new(-2125.64282, 70.3262863, -12400.6777))
                        else
                            if CheckGlassCake() and not CheckEnemies("Cake Prince") then
                                if CheckEnemies("Baking Staff") or CheckEnemies("Cookie Crafter") or CheckEnemies("Cake Guard") or CheckEnemies("Head Baker") then
                                    KillMob("Baking Staff", _G.S.StFarm)
                                    KillMob("Cookie Crafter", _G.S.StFarm)
                                    KillMob("Cake Guard", _G.S.StFarm)
                                    KillMob("Head Baker", _G.S.StFarm)
                                else
                                    if _G.S.BypassTele then
                                        if CheckDistancelonhon(CFrame.new(-2091.911865234375, 70.00884246826172, -12142.8359375), 2500) then
                                            Bypass(CFrame.new(-2091.911865234375, 70.00884246826172, -12142.8359375))
                                        elseif CheckDistancebehon(CFrame.new(-2091.911865234375, 70.00884246826172, -12142.8359375), 2500) then
                                            Tween(CFrame.new(-2091.911865234375, 70.00884246826172, -12142.8359375))
                                        end
                                    else
                                        Tween(CFrame.new(-2091.911865234375, 70.00884246826172, -12142.8359375))
                                    end
                                end
                            end
                        end
                    end
                elseif _G.S.SelectMethodFarm == "Bone" then
                    if CheckEnemies("Reborn Skeleton") or CheckEnemies("Living Zombie") or CheckEnemies("Demonic Soul") or CheckEnemies("Posessed Mummy") then
                        KillMob("Posessed Mummy", _G.S.StFarm)
                        KillMob("Reborn Skeleton", _G.S.StFarm)
                        KillMob("Living Zombie", _G.S.StFarm)
                        KillMob("Demonic Soul", _G.S.StFarm)
                    else
                        if _G.S.BypassTele then
                            if CheckDistancelonhon(CFrame.new(-9515.75, 174.8521728515625, 6079.40625), 2500) then
                                Bypass(CFrame.new(-9515.75, 174.8521728515625, 6079.40625))
                            elseif CheckDistancebehon(CFrame.new(-9515.75, 174.8521728515625, 6079.40625), 2500) then
                                Tween(CFrame.new(-9515.75, 174.8521728515625, 6079.40625))
                            end
                        else
                            Tween(CFrame.new(-9515.75, 174.8521728515625, 6079.40625))
                        end
                    end
                end
            end)
        end
    end
end)

H:CreateSection({ Text = "Mastery Farmming" })

H:CreateDropdown({
    Name = "Select Mastery Farm",
    Description = "Select Gun or Fruits to Farm mastery",
    List = {"Fruit", "Gun"},
    Default = _G.S.SelectMastery,
    CallBack = function(Value)
        _G.S.SelectMastery = Value
        SaveSettings()
    end
})

H:CreateToggle({
    Name = "Auto Mastery",
    Description = "nil",
    Default = _G.S.AutoFarmMastery,
    CallBack = function(Value)
        _G.S.AutoFarmMastery = Value
        SaveSettings()
        if _G.S.AutoFarmMastery == false then
            SpamSkill = false
        end
    end
})

spawn(function()
    while wait() do
        if _G.S.AutoFarmMastery then
            pcall(function()
                GetQuest()
                if CheckEnemies(Mob) then
                    for i, v in pairs(WS.Enemies:GetChildren()) do
                        if v.Name == Mob then
                            if v.Humanoid.Health > 0 and v:FindFirstChild("HumanoidRootPart") and v.Parent then
                                HealthMin = v.Humanoid.MaxHealth * 25/100
                                repeat wait()
                                    if _G.S.SelectMastery == "Fruit" then
                                        if v.Humanoid.Health < HealthMin then
                                            EnableBuso()
                                            EquipTool(plr.Data.DevilFruit.Value)
                                            v.HumanoidRootPart.Size = Vector3.new(30, 30, 30)
                                            v.Head.CanCollide = false
                                            v.HumanoidRootPart.CanCollide = false
                                            v.Humanoid.WalkSpeed = 0
                                            SpamSkillDF = true
                                            SkillAim = true
                                            AimPosition = v.HumanoidRootPart.Position
                                            BringMob(Mob)
                                        else 
                                            EnableBuso()
                                            EquipWeapon()
                                            v.HumanoidRootPart.Size = Vector3.new(30, 30, 30)
                                            v.Head.CanCollide = false
                                            v.HumanoidRootPart.CanCollide = false
                                            v.Humanoid.WalkSpeed = 0
                                            SpamSkillDF = false
                                            SkillAim = false
                                            AimPosition = nil
                                            Click()
                                            BringMob(Mob)
                                        end
                                    elseif _G.S.SelectMastery == "Gun" then
                                        if v.Humanoid.Health < HealthMin then
                                            EnableBuso()
                                            EquipTool(Gun)
                                            v.HumanoidRootPart.Size = Vector3.new(30, 30, 30)
                                            v.Head.CanCollide = false
                                            v.HumanoidRootPart.CanCollide = false
                                            v.Humanoid.WalkSpeed = 0
                                            SpamSkillGun = true
                                            SkillAim = true
                                            AimPosition = v.HumanoidRootPart.Position
                                            Click()
                                            BringMob(Mob)
                                        else 
                                            EnableBuso()
                                            EquipWeapon()
                                            v.HumanoidRootPart.Size = Vector3.new(30, 30, 30)
                                            v.Head.CanCollide = false
                                            v.HumanoidRootPart.CanCollide = false
                                            v.Humanoid.WalkSpeed = 0
                                            SpamSkillGun = false
                                            SkillAim = false
                                            AimPosition = nil
                                            Click()
                                            BringMob(Mob)
                                        end
                                    end
                                    Tween(v.HumanoidRootPart.CFrame * CFrame.new(2, 20, 2))
                                until not v.Parent or not v:FindFirstChild("HumanoidRootPart") or not _G.S.AutoFarmMastery
                                SpamSkillGun = false
                                SpamSkillDF = false
                                SkillAim = false
                                AimPosition = nil
                            end
                        end
                    end
                else
                    for i,v in pairs(game:GetService("Workspace")["_WorldOrigin"].EnemySpawns:GetChildren()) do
                        if v.Name == Mob then
                            repeat wait()
                                Tween(v.CFrame * CFrame.new(2, 20, 2))
                            until WS.Enemies:FindFirstChild(Mob) or not _G.S.AutoFarmMastery
                        end
                    end
                end
            end)
        end
    end
end)

H:CreateSection({ Text = "Chest Farmming" })

local ChestTween = H:CreateToggle({
    Name = "Auto Chest <Tween>",
    Description = "Tween To Chest In Workspace",
    Default = _G.S.AutoChestTween,
    CallBack = function(Value)
        _G.S.AutoChestTween = Value
        SaveSettings()
    end
})

local ChestInstance = H:CreateToggle({
    Name = "Auto Chest <Instance>",
    Description = "Instance To Chest In Workspace",
    Default = _G.S.AutoChestInstance,
    CallBack = function(Value)
        _G.S.AutoChestInstance = Value
        SaveSettings()
    end
})

spawn(function()
    while wait() do
        if _G.S.AutoChestTween or _G.S.AutoChestInstance then
            v = GetNearChest()
            repeat wait()
                if _G.S.AutoChestTween then
                    Tween(v.CFrame)
                elseif _G.S.AutoChestInstance then
                    InstanceTween(v.CFrame)
                    game.ReplicatedStorage.Remotes.CommF_:InvokeServer("SetTeam","Pirates")
                end
                if (v.Position - plr.Character.HumanoidRootPart.Position).Magnitude <= 10 then
                    SendKey("Q")
                end
            until not v.Parent or not _G.S.AutoChestTween and not _G.S.AutoChestInstance
        end
    end
end)

H:CreateToggle({
    Name = "Tween Safe Zone If Have Fist Of Darkness or God's Chalice",
    Description = "nil",
    Default = _G.S.SafeChest,
    CallBack = function(Value)
        _G.S.SafeChest = Value
        SaveSettings()
    end
})

spawn(function()
    while task.wait() do
        if plr.Backpack:FindFirstChild("God's Chalice") or plr.Character:FindFirstChild("God's Chalice") or plr.Backpack:FindFirstChild("Fist Of Darkness") or plr.Character:FindFirstChild("Fist Of Darkness") then
            ChestInstance:Set(false)
            ChestTween(false)
            CancelTween()
            InstanceTween(plr.Character.HumanoidRootPart.CFrame)
            wait(.1)
            game:GetService("ReplicatedStorage").Remotes.CommF_:InvokeServer("requestEntrance",Vector3.new(-12549.7236, 337.511505, -7470.36328))
        end
    end
end)

