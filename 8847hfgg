SE:AddParagraph("Sea Event","Sea Event has many functions such as Auto Shark, Auto Terrorshark, Auto Piranha, Auto Fish Crew Member and some others such as Auto Find Kitsune Island and Auto Collect Azure")

SE:AddSlider({
	Name = "Speed Boat Value",
	Min = 250,
	Max = 350,
	Default = _G.Settings.SpeedValue,
	Color = Color3.fromRGB(255,255,255),
	Increment = 1,
	ValueName = "Value",
	Callback = function(Value)
		_G.Settings.SpeedValue = Value
	end    
})

SE:AddToggle({
	Name = "Enable Speed Boats",
	Default = _G.Settings.EnableSpeedBoat,
	Save = true,
	Flag = "Enable Speed Boats",
	Callback = function(Value)
		_G.Settings.EnableSpeedBoat = Value
        SaveSettings()
	end    
})

spawn(function()
    while wait() do
        if _G.Settings.EnableSpeedBoat then
            for i, v in pairs(game.Workspace.Boats:GetChildren()) do
                if v:FindFirstChild("Owner").Value == game.Players.LocalPlayer then
                    v:FindFirstChild("VehicleSeat").MaxSpeed = _G.Settings.SpeedValue
                end
            end
        end
    end
end)

SE:AddToggle({
	Name = "Start Sea Event",
	Default = _G.Settings.StartSeaEvent,
	Save = true,
	Flag = "Start Sea Event",
	Callback = function(Value)
		_G.Settings.StartSeaEvent = Value
        SaveSettings()
	end    
})

spawn(function()
    while wait() do
        if _G.Settings.StartSeaEvent then
            if not CheckYourBoat("PirateGrandBrigade") then
                if not CheckEnemies("Shark") and not CheckEnemies("Terrorshark") and not CheckEnemies("Piranha") and not CheckEnemies("Fish Crew Member") then
                    BuyBoats = TeleToShip(CFrame.new(-16927.451171875, 9.0863618850708, 433.8642883300781))
                    if (CFrame.new(-16927.451171875, 9.0863618850708, 433.8642883300781).Position - game.Players.LocalPlayer.Character.HumanoidRootPart.Position).magnitude <= 10 then
                        if BuyBoats then 
                            BuyBoats:Stop() 
                        end
                        local args = {
                            [1] = "BuyBoat",
                            [2] = "PirateGrandBrigade"
                        }
                        game:GetService("ReplicatedStorage").Remotes.CommF_:InvokeServer(unpack(args))
                    end
                end
            elseif CheckYourBoat("PirateGrandBrigade") then
                if game.Players.LocalPlayer.Character:WaitForChild("Humanoid").Sit == false then
                    if not CheckEnemies("Shark") and not CheckEnemies("Terrorshark") and not CheckEnemies("Piranha") and not CheckEnemies("Fish Crew Member") then
                        for i, v in pairs(game.Workspace.Boats:GetChildren()) do
                            if v:FindFirstChild("Owner") then
                                if v:FindFirstChild("Owner").Value == game.Players.LocalPlayer then
                                    table.foreach(v:GetDescendants(), function(a, child)
                                        if child:IsA("BasePart") or child:IsA("MeshPart") or child:IsA("Part") then
                                            child.CanCollide = false
                                        end
                                    end)
                                    TeleToShip(v:FindFirstChild("VehicleSeat").CFrame * CFrame.new(0, 1, 0))
                                end
                            end
                        end
                    end
                else
                    repeat wait()
                        if (CFrame.new(-17013.80078125, 10.962434768676758, 438.0169982910156).Position - game.Players.LocalPlayer.Character.HumanoidRootPart.Position).magnitude <= 10 then
                            TweenBoats(CFrame.new(-44842.3945, 10.7790766, 16911.3477))
                        end
                    until game:GetService("Workspace").Enemies:FindFirstChild("Shark") or game:GetService("Workspace").Enemies:FindFirstChild("Terrorshark") or game:GetService("Workspace").Enemies:FindFirstChild("Piranha") or game:GetService("Workspace").Enemies:FindFirstChild("Fish Crew Member") or _G.Settings.StartSeaEvent == false
                    Your.Character.Humanoid.Sit = false
                end
            end
        end
    end
end)

spawn(function()
    while wait() do
        if _G.Settings.StartSeaEvent then
            if CheckEnemies("Shark") or CheckEnemies("Terrorshark") or CheckEnemies("Piranha") or CheckEnemies("Fish Crew Member") then
                Your.Character.Humanoid.Sit = false
            end
        end
    end
end)

