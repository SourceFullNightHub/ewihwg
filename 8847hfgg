function StartRaid()
    if Sea2 then
        return fireclickdetector(game:GetService("Workspace").Map.CircleIsland.RaidSummon2.Button.Main.ClickDetector)
    elseif Sea3 then
        return fireclickdetector(game:GetService("Workspace").Map["Boat Castle"].RaidSummon2.Button.Main.ClickDetector)
    end
end

A:CreateButton({
    Name = "Start Raid",
    Description = "Nah",
    CallBack = function()
        StartRaid()
    end
})

A:CreateToggle({
    Name = "Auto Raid",
    Description = "Auto Kill Aura Mob and Next Islands",
    Default = _G.S.AutoRaid,
    CallBack = function(Value)
        _G.S.AutoRaid = Value
        SaveSettings()
    end
})

spawn(function()
    while task.wait() do
        pcall(function()
            if _G.S.AutoRaid and plr.PlayerGui.Main.Timer.Visible == true then
                if WS["_WorldOrigin"].Locations:FindFirstChild("Island 5") then
                    if (WS["_WorldOrigin"].Locations:FindFirstChild("Island 5").Position - plr.Character.HumanoidRootPart.Position).Magnitude <= 4500 then
                        Tween(WS["_WorldOrigin"].Locations:FindFirstChild("Island 5").CFrame * CFrame.new(2, 20 ,2))
                    end
                elseif game:GetService("Workspace")["_WorldOrigin"].Locations:FindFirstChild("Island 4") then
                    if (WS["_WorldOrigin"].Locations:FindFirstChild("Island 4").Position - plr.Character.HumanoidRootPart.Position).Magnitude <= 4500 then
                        Tween(WS["_WorldOrigin"].Locations:FindFirstChild("Island 4").CFrame *CFrame.new(2, 60 ,2))
                    end
                elseif game:GetService("Workspace")["_WorldOrigin"].Locations:FindFirstChild("Island 3") then
                    if (WS["_WorldOrigin"].Locations:FindFirstChild("Island 3").Position - plr.Character.HumanoidRootPart.Position).Magnitude <= 4500 then
                        Tween(WS["_WorldOrigin"].Locations:FindFirstChild("Island 3").CFrame *CFrame.new(2, 20 ,2))
                    end
                elseif game:GetService("Workspace")["_WorldOrigin"].Locations:FindFirstChild("Island 2") then
                    if (WS["_WorldOrigin"].Locations:FindFirstChild("Island 2").Position - plr.Character.HumanoidRootPart.Position).Magnitude <= 4500 then
                        Tween(WS["_WorldOrigin"].Locations:FindFirstChild("Island 2").CFrame *CFrame.new(2, 20 ,2))
                    end
                elseif game:GetService("Workspace")["_WorldOrigin"].Locations:FindFirstChild("Island 1") then
                    if (WS["_WorldOrigin"].Locations:FindFirstChild("Island 1").Position - plr.Character.HumanoidRootPart.Position).Magnitude <= 4500 then
                        Tween(WS["_WorldOrigin"].Locations:FindFirstChild("Island 1").CFrame *CFrame.new(2, 20 ,2))
                    end
                end
            end
        end)
    end
end)

spawn(function()
    while task.wait() do
        if _G.S.AutoRaid then
            pcall(function()
                for i, v in pairs(game:GetService("Workspace").Enemies:GetChildren()) do
                    if v.Humanoid.Health > 0 and v:FindFirstChild("HumanoidRootPart") and v.Parent then
                        repeat task.wait()
                            v.Head:Destroy()
                            v.HumanoidRootPart.CanCollide = false
                            v.Humanoid.Health = 0
                        until not _G.S.AutoRaid  or not v.Parent or v.Humanoid.Health <= 0
                    end
                end
            end)
        end
    end
end)

A:CreateToggle({
    Name = "Auto Fully Raid",
    Description = "Auto Buy Chip, Start Raid and Kill Aura Mob & NextIsland",
    Default = _G.S.AutoFullyRaid,
    CallBack = function(Value)
        _G.S.AutoFullyRaid = Value
        SaveSettings()
    end
})

spawn(function()
    while wait() do
        pcall(function()
            if _G.S.AutoFullyRaid then
                if not CheckBackpack("Microchip") then 
                    game:GetService("ReplicatedStorage").Remotes.CommF_:InvokeServer("RaidsNpc", "Select", _G.S.chiplit)
                elseif CheckBackpack("Microchip") then
                    if not plr.PlayerGui.Main.Timer.Visible == true then
                        StartRaid()
                    elseif plr.PlayerGui.Main.Timer.Visible == true then
                        for i, v in pairs(game:GetService("Workspace").Enemies:GetChildren()) do
                            if v.Humanoid.Health > 0 and v:FindFirstChild("HumanoidRootPart") and v.Parent then
                                repeat task.wait()
                                    v.Head:Destroy()
                                    v.HumanoidRootPart.CanCollide = false
                                    v.Humanoid.Health = 0
                                until not _G.S.AutoRaid  or not v.Parent or v.Humanoid.Health <= 0
                            end
                        end
                        if WS["_WorldOrigin"].Locations:FindFirstChild("Island 5") then
                            if (WS["_WorldOrigin"].Locations:FindFirstChild("Island 5").Position - plr.Character.HumanoidRootPart.Position).Magnitude <= 4500 then
                                Tween(WS["_WorldOrigin"].Locations:FindFirstChild("Island 5").CFrame * CFrame.new(2, 20 ,2))
                            end
                        elseif game:GetService("Workspace")["_WorldOrigin"].Locations:FindFirstChild("Island 4") then
                            if (WS["_WorldOrigin"].Locations:FindFirstChild("Island 4").Position - plr.Character.HumanoidRootPart.Position).Magnitude <= 4500 then
                                Tween(WS["_WorldOrigin"].Locations:FindFirstChild("Island 4").CFrame *CFrame.new(2, 60 ,2))
                            end
                        elseif game:GetService("Workspace")["_WorldOrigin"].Locations:FindFirstChild("Island 3") then
                            if (WS["_WorldOrigin"].Locations:FindFirstChild("Island 3").Position - plr.Character.HumanoidRootPart.Position).Magnitude <= 4500 then
                                Tween(WS["_WorldOrigin"].Locations:FindFirstChild("Island 3").CFrame *CFrame.new(2, 20 ,2))
                            end
                        elseif game:GetService("Workspace")["_WorldOrigin"].Locations:FindFirstChild("Island 2") then
                            if (WS["_WorldOrigin"].Locations:FindFirstChild("Island 2").Position - plr.Character.HumanoidRootPart.Position).Magnitude <= 4500 then
                                Tween(WS["_WorldOrigin"].Locations:FindFirstChild("Island 2").CFrame *CFrame.new(2, 20 ,2))
                            end
                        elseif game:GetService("Workspace")["_WorldOrigin"].Locations:FindFirstChild("Island 1") then
                            if (WS["_WorldOrigin"].Locations:FindFirstChild("Island 1").Position - plr.Character.HumanoidRootPart.Position).Magnitude <= 4500 then
                                Tween(WS["_WorldOrigin"].Locations:FindFirstChild("Island 1").CFrame *CFrame.new(2, 20 ,2))
                            end
                        end
                    end
                end
            end
        end)
    end
end)
                        

A:CreateSection({ Text = "Law Boss Raid" })

function BuyChipLaw()
    if CheckFragment(1000) then
        if not CheckBackpack("Microchip") then
            local args = {
                [1] = "BlackbeardReward", 
                [2] = "Microchip", 
                [3] = "2"
            }
            Rc.Remotes.CommF_:InvokeServer(unpack(args))
        end
    end
end

A:CreateButton({
    Name = "Buy Chip Law Raids",
    Description = "nah",
    CallBack = function()
        BuyChipLaw()
    end
})

