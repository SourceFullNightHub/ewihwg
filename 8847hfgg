    Name = "Auto Craft Shark Anchor",
    Description = "",
    Default = _G.S.AutoSharkAnchor,
    CallBack = function(Value)
        _G.S.AutoSharkAnchor = Value
        SaveSettings()
    end
})

spawn(function()
    while task.wait() do
        pcall(function()
            if _G.S.AutoSharkAnchor and Sea3 then
                if GetItemCount("Monster Magnet", 1) then
                    notify("Script", "Tự bật Sea Event với Auto Kill Terror Shark đi, tao đéo rảnh", 5)
                    wait(6)
                elseif GetItemCount("Terror Eyes", 2) and GetItemCount("Electric Wing", 8) and GetItemCount("Fool's Gold", 20) and GetItemCount("Shark Tooth", 10) then
                    game:GetService("ReplicatedStorage").Remotes.CommF_:InvokeServer("CraftItem", "Check", "SharkAnchor")
                    wait(.1)
                    game:GetService("ReplicatedStorage").Remotes.CommF_:InvokeServer("CraftItem", "Craft", "SharkAnchor")
                elseif not GetItemCount("Monster Magnet", 1) or not GetItemCount("Terror Eyes", 2) and not GetItemCount("Electric Wing", 8) and not GetItemCount("Fool's Gold", 20) and not GetItemCount("Shark Tooth", 10) then
                    notify("SCRIPT", "Tự kiếm nguyên liệu đi, buồn ngủ quá", 5)
                    wait(6)
                end
            end
        end)
    end
end)

