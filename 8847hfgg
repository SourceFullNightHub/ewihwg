                _G.S.FastAttackDelay = 0.05
            elseif _G.S.FastAttackDelay == "0.075" then
                _G.S.FastAttackDelay = 0.075
            elseif _G.S.FastAttackDelay == "0.1" then
                _G.S.FastAttackDelay = 0.1
            elseif _G.S.FastAttackDelay == "0.15" then
                _G.S.FastAttackDelay = 0.15
            elseif _G.S.FastAttackDelay == "0.155" then
                _G.S.FastAttackDelay = 0.155
            elseif _G.S.FastAttackDelay == "0.16" then
                _G.S.FastAttackDelay = 0.16
            elseif _G.S.FastAttackDelay == "0.165" then
                _G.S.FastAttackDelay = 0.165
            elseif _G.S.FastAttackDelay == "0.17" then
