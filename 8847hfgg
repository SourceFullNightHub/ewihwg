            TextLabel_2.TextXAlignment = Enum.TextXAlignment.Left

            UIPadding_18.Parent = TextLabel_2
            UIPadding_18.PaddingLeft = UDim.new(0, 7)

            IconFrame.Name = "IconFrame"
