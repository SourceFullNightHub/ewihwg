I:CreateSection({ Text = "Sea 1 Item & Quest" })

local SaberTakenToggle = I:CreateToggle({
    Name = "Auto Saber",
    Description = "Auto Taken Saber(function Only Sea 1)",
    Default = _G.S.TakenSaber,
    CallBack = function(Value)
        _G.S.TakenSaber = Value
        SaveSettings()
    end
})

local SaberHop = I:CreateToggle({
    Name = "Auto Saber Hop",
    Description = "",
    Default = _G.S.AutoSaberHop,
    CallBack = function(Value)
        _G.S.AutoSaberHop = Value
        SaveSettings()
    end
})

spawn(function()
    while task.wait() do
        if _G.S.TakenSaber and Sea1 then
            pcall(function()
                if plr.Data.Level.Value >= 200 and CheckInventory("Saber") then
                    SaberTakenToggle:Set(false)
                    SaberHop:Set(false)
                    Library:MakeNotify({
                        Name = "Night Hub",
                        Content = "Your Are Have Saber",
                        Time = 1,
                    })
                    wait(5)
                elseif plr.Data.Level.Value >= 200 and not CheckInventory("Saber") then
                    if not CheckSaberDoor() then
                        if game:GetService("Workspace").Map.Jungle.QuestPlates.Door.CanCollide then
                            if CheckButtonJungle() then
                                Tween(CheckButtonJungle().Button.CFrame)
                                Library:MakeNotify({
                                    Name = "Night Hub",
                                    Content = "Go activate the buttons In Jungle",
                                    Time = 1,
                                })
                                wait(5)
                            end
                        else
                            if not CheckCup() then
                                if not plr.Backpack:FindFirstChild("Torch") and not plr.Character:FindFirstChild("Torch") then
                                    Tween(game:GetService("Workspace").Map.Jungle.Torch.CFrame)
                                    Library:MakeNotify({
                                        Name = "Night Hub",
                                        Content = "Get Torch",
                                        Time = 1,
                                    })
                                    wait(0.3)
                                    EquipTool("Torch")
                                    wait(5)
                                else
                                    notify("Night Hub", "Teleport To Door In Desert", 5)
                                    Tween(game:GetService("Workspace").Map.Desert.Burn.Fire.CFrame)
                                end
                            else
                                if game.ReplicatedStorage.Remotes.CommF_:InvokeServer("ProQuestProgress", "RichSon") ~= 0 and game:GetService("ReplicatedStorage").Remotes.CommF_:InvokeServer("ProQuestProgress","RichSon") ~= 1 then
                                    print("lon")
                                    if not plr.Backpack:FindFirstChild("Cup") and not plr.Character:FindFirstChild("Cup") then
                                        game:GetService("ReplicatedStorage").Remotes.CommF_:InvokeServer("ProQuestProgress","GetCup")
                                        Library:MakeNotify({
                                            Name = "Night Hub",
                                            Content = "Task To NPC In Pirates Village",
                                            Time = 1,
                                        })
                                        wait(0.3)
                                        EquipTool("Cup")
                                        wait(0.5)
                                        Library:MakeNotify({
                                            Name = "Night Hub",
                                            Content = "Give the Cup to the thirsty old man",
                                            Time = 1,
                                        })
                                        wait(0.3)
                                        game:GetService("ReplicatedStorage").Remotes.CommF_:InvokeServer("ProQuestProgress","FillCup",plr.Character.Cup)
                                        wait(0.5)
                                        Library:MakeNotify({
                                            Name = "Night Hub",
                                            Content = "Take Quest Kill Mob Leader",
                                            Time = 1,
                                        })
                                        wait(0.3)
                                        game:GetService("ReplicatedStorage").Remotes.CommF_:InvokeServer("ProQuestProgress","SickMan")
                                        wait(5)
                                    end
                                else
                                    if game:GetService("ReplicatedStorage").Remotes.CommF_:InvokeServer("ProQuestProgress","RichSon") == nil then
                                        game:GetService("ReplicatedStorage").Remotes.CommF_:InvokeServer("ProQuestProgress","RichSon")
                                    elseif game:GetService("ReplicatedStorage").Remotes.CommF_:InvokeServer("ProQuestProgress","RichSon") == 0 then
                                        if CheckEnemies("Mob Leader") then
                                            KillMob("Mob Leader", _G.S.TakenSaber)
                                        else
                                            Tween(CFrame.new(-2967.59521, -4.91089821, 5328.70703))
                                        end
                                    elseif game:GetService("ReplicatedStorage").Remotes.CommF_:InvokeServer("ProQuestProgress","RichSon") == 1 then
                                        game:GetService("ReplicatedStorage").Remotes.CommF_:InvokeServer("ProQuestProgress","RichSon")
                                        wait(0.1)
                                        Library:MakeNotify({
                                            Name = "Night Hub",
                                            Content = "Equip Relic",
                                            Time = 1,
                                        })
                                        EquipTool("Relic")
                                        wait(0.2)
                                        Tween(CFrame.new(-1404.91504, 29.9773273, 3.80598116, 0.876514494, 5.66906877e-09, 0.481375456, 2.53851997e-08, 1, -5.79995607e-08, -0.481375456, 6.30572643e-08, 0.876514494))
                                        wait(5)
                                    end
                                end
                            end
                        end
                    else
                        if CheckEnemies("Saber Expert") then
                            Library:MakeNotify({
                                Name = "Night Hub",
                                Content = "Kill Saber Expert",
                                Time = 1,
                            })
                            KillBoss("Saber Expert", _G.S.TakenSaber)
                            wait(5)
                        else
                            Tween(CFrame.new(-1458.89502, 29.8870335, -50.633564))
                            if game.ReplicatedStorage:FindFirstChild("Saber Expert") then
                                notify("Night Hub", "Wait Boss...", 1)
                                Tween(v.HumanoidRootPart.CFrame * CFrame.new(2, 20, 2))
                                wait(5)
                            else
                                if _G.S.AutoSaberHop == true then
                                    notify("Night Hub", "Hop Servers Because Not Boss", 1)
                                    wait(_G.S.DelayHop)
                                    HopServer()
                                end
                            end
                        end
                    end
                end
            end)
        end
    end
end)

