            IconDropdown.Parent = IconFrame
            IconDropdown.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
            IconDropdown.BackgroundTransparency = 1.000
            IconDropdown.BorderColor3 = Color3.fromRGB(0, 0, 0)
            IconDropdown.BorderSizePixel = 0
