E:CreateToggle({
    Name = "Auto Ship",
    Description = "Auto Ghosh Ship",
    Default = _G.S.AutoGhoshShip,
    CallBack = function(Value)
        _G.S.AutoGhoshShip = Value
        SaveSettings()
    end
})

spawn(function()
    while task.wait() do
        pcall(function()
            if _G.S.AutoGhoshShip then
                if GetShipTarget() then
                    if not CheckEnemies("Shark") and not CheckEnemies("Terrorshark") and not CheckEnemies("Fish Crew Member") and not CheckEnemies("Piranha") then
                        if (GetShipTarget().Engine.Position - plr.Character.HumanoidRootPart.Position).Magnitude <= 3000 then
                            repeat task.wait()
                                plr.Character.Humanoid.Sit = false
                                spawn(Tween(GetShipTarget().Engine.CFrame * CFrame.new(0, -20, 0)), 1)
                                if (GetShipTarget().Engine.Position - plr.Character.HumanoidRootPart.Position).Magnitude <= 2500 then
                                    SpamSkill = true
                                    AimPosition = v.Engine.Position
                                    SkillAim = true
                                end
                            until not v.Parent or not GetShipTarget()
                            SpamSkill = false
                            AimPosition = nil
                            SkillAim = false
                        end
                    elseif CheckEnemies("Shark") or CheckEnemies("Terrorshark") or CheckEnemies("Fish Crew Member") or CheckEnemies("Piranha") then
                        KillSeaEvent("Shark")
                        KillSeaEvent("Terrorshark")
                        KillSeaEvent("Fish Crew Member")
                        KillSeaEvent("Piranha")
                    end
                end
            end
        end)
    end
end)

local ToggleSeaBeast = E:CreateToggle({
    Name = "Auto SeaBeast",
    Description = "Auto Kill SeaBeast",
    Default = _G.S.AutoSeaBeast,
    CallBack = function(Value)
        _G.S.AutoSeaBeast = Value
        SaveSettings()
    end
})

spawn(function()
    while task.wait() do
        if _G.S.AutoSeaBeast then
            if CheckSeaBeast() then
                if not CheckEnemies("Shark") and not CheckEnemies("Terrorshark") and not CheckEnemies("Fish Crew Member") and not CheckEnemies("Piranha") then
                    local v = CheckSeaBeast()
                    if v:FindFirstChild("HumanoidRootPart") then
                        if (v.HumanoidRootPart.Position - plr.Character.HumanoidRootPart.Position).Magnitude <= 3000 then
                            repeat task.wait()
                                plr.Character.Humanoid.Sit = falsse
                                TeleportSeabeast(v)
                                if (v.HumanoidRootPart.Position - plr.Character.HumanoidRootPart.Position).Magnitude <= 2500 then
                                    SpamSkill = true
                                    SkillAim = true
                                    AimPosition = v.HumanoidRootPart.Position
                                end
                            until not CheckSeaBeast() or not v.Parent or not v:FindFirstChild("HumanoidRootPart")
                            SpamSkill = false
                            SkillAim = false
                            AimPosition = nil
                        end
                    end
                elseif CheckEnemies("Shark") or CheckEnemies("Terrorshark") or CheckEnemies("Fish Crew Member") or CheckEnemies("Piranha") then
                    KillSeaEvent("Shark")
                    KillSeaEvent("Terrorshark")
                    KillSeaEvent("Fish Crew Member")
                    KillSeaEvent("Piranha")
                end
            end
        end
    end
end)

R:CreateSection({ Text = "Race Upgraded" })

R:CreateToggle({
    Name = "Auto Race V2 - V3",
    Description = "Beta",
    Default = _G.S.AutoRaceV2V3,
    CallBack = function(Value)
        _G.S.AutoRaceV2V3 = Value
        SaveSettings()
    end
})

ChestCount = 0
spawn(function()
    while wait() do
        if _G.S.AutoRaceV2V3 and CheckSea(2) then
            if string.find(CheckRace(), "V1") then
                if not game:GetService("Players").LocalPlayer.Data.Race:FindFirstChild("Evolved") then
                    if game:GetService("ReplicatedStorage").Remotes.CommF_:InvokeServer("Alchemist","1") == 0 then
                        game:GetService("ReplicatedStorage").Remotes.CommF_:InvokeServer("Alchemist","2")
                    elseif game:GetService("ReplicatedStorage").Remotes.CommF_:InvokeServer("Alchemist","1") == 1 then
                        pcall(function()
                            if not plr.Backpack:FindFirstChild("Flower 1") and not plr.Character:FindFirstChild("Flower 1") then
                                Tween(game:GetService("Workspace").Flower1.CFrame)
                            elseif not plr.Backpack:FindFirstChild("Flower 2") and not plr.Character:FindFirstChild("Flower 2") then
                                Tween(game:GetService("Workspace").Flower2.CFrame)
                            elseif not game:GetService("Players").LocalPlayer.Backpack:FindFirstChild("Flower 3") and not game:GetService("Players").LocalPlayer.Character:FindFirstChild("Flower 3") then
                                if WS.Enemies:FindFirstChild("Ship Deckhand") or WS.Enemies:FindFirstChild("Ship Engineer") or WS.Enemies:FindFirstChild("Ship Steward") or WS.Enemies:FindFirstChild("Ship Officer") then
                                    for i,v in pairs(WS.Enemies:GetChildren()) do
                                        if v.Name == "Ship Deckhand" or v.Name == "Ship Engineer" or v.Name == "Ship Steward" or v.Name == "Ship Officer" then
                                            repeat task.wait()
                                                EnableBuso()
                                                EquipWeapon()
                                                Tween(v.HumanoidRootPart.CFrame * CFrame.new(0,20,0))
                                                v.HumanoidRootPart.CanCollide = false
                                                Click()
                                                BringMob("Ship Deckhand")
                                                BringMob("Ship Engineer")
                                                BringMob("Ship Steward")
                                                BringMob("Ship Officer")
                                            until plr.Backpack:FindFirstChild("Flower 3") or not v.Parent or v.Humanoid.Health <= 0 or _G.S.AutoRaceV2V3
                                        end
                                    end
                                else
                                    Tween(CFrame.new(1212.0111083984375, 150.79205322265625, 33059.24609375))
                                end
                            end
                        end)
                    elseif game:GetService("ReplicatedStorage").Remotes.CommF_:InvokeServer("Alchemist","1") == 2 then
                        game:GetService("ReplicatedStorage").Remotes.CommF_:InvokeServer("Alchemist","3")
                    end
                end
            elseif string.find(CheckRace(), "V2") then
                Race = CheckRace()
                if not string.find(Race, "V3") and not string.find(Race, "V4") then
                    if game:GetService("ReplicatedStorage").Remotes.CommF_:InvokeServer("BartiloQuestProgress", "Bartilo") == 3 then
                        if Race == "Human V2" then
                            ClaimQuestV3()
                            if CheckBoss("Jeremy") and CheckBoss("Diamond") and CheckBoss("Fajita") then
                                KillBoss("Jeremy", _G.S.AutoRaceV2V3)
                                KillBoss("Diamond", _G.S.AutoRaceV2V3)
                                KillBoss("Fajita", _G.S.AutoRaceV2V3)
                            elseif not CheckBoss("Jeremy") or not CheckBoss("Diamond") or not CheckBoss("Fajita") then
                                notify("Script", "HopServer to find Boss", 5)
                                wait(_G.S.DelayHop)
                                HopServer()
                                wait(5)
                            end
                        elseif Race == "Mink V2" then
                            ClaimQuestV3()
                            v = GetNearChest()
                            repeat wait()
                                Tween(v.CFrame)
                                ChestCount = ChestCount + 1
                                if (v.Position - plr.Character.HumanoidRootPart.Position).Magnitude <= 10 then
                                    SendKey("Q")
                                end
                                if ChestCount >= 30 then
                                    notify("Script Notify", "Race Status: " ..tostring(game.ReplicatedStorage.Remotes.CommF_:InvokeServer("Wenlocktoad", "info")), 5)
                                    wait(5)
                                end
                            until not v.Parent or not _G.S.AutoRaceV2V3
                        elseif Race == "Fishman V2" then
                            ClaimQuestV3()
                            repeat wait()
                                if not GetYourShip() then
                                    Tween(CFrame.new(-5325.12646, 4.7167592, -714.438171, -0.245173886, 4.1912454e-08, -0.969479144, -2.48438443e-08, 1, 4.95147496e-08, 0.969479144, 3.62253125e-08, -0.245173886))
                                    if CheckDistancebehon(CFrame.new(-5325.12646, 4.7167592, -714.438171, -0.245173886, 4.1912454e-08, -0.969479144, -2.48438443e-08, 1, 4.95147496e-08, 0.969479144, 3.62253125e-08, -0.245173886), 10) then
                                        local args = {
                                            [1] = "BuyBoat",
                                            [2] = "PirateBrigade"
                                        }
                                        game:GetService("ReplicatedStorage").Remotes.CommF_:InvokeServer(unpack(args))
                                    end
                                elseif GetYourShip() then
                                    v = GetYourShip()
                                    if plr.Character.Humanoid.Sit == false then
                                        TPP(v:FindFirstChild('VehicleSeat').CFrame * CFrame.new(0, 1, 0))
                                    else
                                        if (v:FindFirstChild('VehicleSeat').Position - CFrame.new(-5325.12646, 4.7167592, -714.438171).Position).Magnitude <= 10 then
                                            TPB(CFrame.new(-4872.52979, -0.34796223, 750.928406))
                                        end
                                    end
                                end
                                ToggleSeaBeast:Set(true)
                            until Race ~= "Fishman V2" or not _G.S.AutoRaceV2V3
                            ToggleSeaBeast:Set(false)
                        elseif Race == "Skypiea V2" then
                            ClaimQuestV3()
                            SkypieaPlayers = {}
                            for i, v in pairs(game.Players:GetChildren()) do
                                pcall(function()
                                    if v.Name ~= plr.Name and game.Workspace.Characters:FindFirstChild(v.Name) and v:FindFirstChild("Humanoid") and v:FindFirstChild("Data") and v.Data:FindFirstChild("Race") and v.Data.Race.Value == "Skypiea" then
                                        table.insert(SkypieaPlayers, v)
                                    end
                                end)
                            end
                            if #SkypieaPlayers > 0 then
                                for r, v in pairs(SkypieaPlayers) do
                                    repeat
                                        wait()
                                        v.HumanoidRootPart.Size = Vector3.new(30, 30, 30)
                                        Tweento(v.HumanoidRootPart.CFrame * CFrame.new(0, 0, 3))
                                        Click()
                                        EnableBuso()
                                        EquipWeapon()
                                    until not v.Parent or not plr.Data.Race.Value == 'Skypiea V2' or v.Humanoid.Health <= 0
                                end
                            else
                                HopServer(9)
                                HopServer(10)
                            end
                        end
                    end
                end
            end
        end
    end
end)

