        TweenService:Create(UIStroke_8, TweenInfo.new(0.6, Enum.EasingStyle.Quint), {Transparency = 0.9}):Play()
        TweenService:Create(Title_6, TweenInfo.new(0.6, Enum.EasingStyle.Quint), {TextTransparency = 0.4}):Play()
        TweenService:Create(Coginoidi, TweenInfo.new(0.6, Enum.EasingStyle.Quint), {TextTransparency = 0.5}):Play()
        wait(0.05)
    
        Notify:TweenPosition(UDim2.new(20, 0, 0, 0),'In','Quint',0.8,true)
        wait(1.35)
        Notify:Destroy()
    end

    local UITab = {}

    function UITab:CreateTab(Options)
        Options = Options or {}
        Options.Name = Options.Name or "Tab 1"
        Options.Icon = Options.Icon or "rbxassetid://17184558565"

        local ButtonDisabledTab = Instance.new("TextButton")
