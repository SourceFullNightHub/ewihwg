spawn(function()
    while wait() do
        if game.Players.localPlayer.Data.Points.Value >= _G.Settings.Stats then
            if _G.Settings.Melee then
                local args = {
                [1] = "AddPoint",
                [2] = "Melee",
                [3] = _G.Settings.Stats
            }
            game:GetService("ReplicatedStorage").Remotes.CommF_:InvokeServer(unpack(args))
        elseif _G.Settings.Defense then
            local args = {
                [1] = "AddPoint",
                [2] = "Defense",
                [3] = _G.Settings.Stats
                }
                game:GetService("ReplicatedStorage").Remotes.CommF_:InvokeServer(unpack(args))
            elseif _G.Settings.Sword then
                local args = {
                    [1] = "AddPoint",
                    [2] = "Sword",
                    [3] = _G.Settings.Stats
                }
                game:GetService("ReplicatedStorage").Remotes.CommF_:InvokeServer(unpack(args))
            elseif _G.Settings.Gun then
                local args = {
                    [1] = "AddPoint",
                    [2] = "Gun",
                    [3] = _G.Settings.Stats
                }
                game:GetService("ReplicatedStorage").Remotes.CommF_:InvokeServer(unpack(args))
            elseif _G.Settings.DF then
                local args = {
                    [1] = "AddPoint",
                    [2] = "Blox Fruit",
                    [3] = _G.Settings.Stats
                }
                game:GetService("ReplicatedStorage").Remotes.CommF_:InvokeServer(unpack(args))
            end
        end
    end
end)

