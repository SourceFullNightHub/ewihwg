        Coginoidi.TextSize = 14.000
        Coginoidi.TextWrapped = true
        Coginoidi.TextXAlignment = Enum.TextXAlignment.Left
    
        UIPadding_12.Parent = Coginoidi
        UIPadding_12.PaddingBottom = UDim.new(0, 16)
        UIPadding_12.PaddingLeft = UDim.new(0, 12)
    
        UIStroke_8.Color = Color3.fromRGB(255, 255, 255)
        UIStroke_8.Parent = Notify
    
        Title_6.Name = "Title"
        Title_6.Parent = Notify
        Title_6.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
