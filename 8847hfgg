            TextLabel_2.Parent = Content_5
            TextLabel_2.BackgroundColor3 = Color3.fromRGB(50, 50, 50)
            TextLabel_2.BorderColor3 = Color3.fromRGB(0, 0, 0)
            TextLabel_2.BorderSizePixel = 0
            TextLabel_2.Position = UDim2.new(0.00184722175, 0, 0.906360686, 0)
            TextLabel_2.Size = UDim2.new(0, 283, 0, 27)
            TextLabel_2.Font = Enum.Font.SourceSansBold
            TextLabel_2.Text = Options.Default
            TextLabel_2.TextColor3 = Color3.fromRGB(255, 255, 255)
            TextLabel_2.TextSize = 14.000
            TextLabel_2.TextXAlignment = Enum.TextXAlignment.Left

            UIPadding_18.Parent = TextLabel_2
            UIPadding_18.PaddingLeft = UDim.new(0, 7)

            IconFrame.Name = "IconFrame"
            IconFrame.Parent = Content_5
            IconFrame.BackgroundColor3 = Color3.fromRGB(34, 34, 34)
            IconFrame.BorderColor3 = Color3.fromRGB(0, 0, 0)
            IconFrame.BorderSizePixel = 0
            IconFrame.Position = UDim2.new(0.890086234, 0, 0.926372707, 0)
            IconFrame.Size = UDim2.new(0, 25, 0, 25)

            IconDropdown.Name = "Icon Dropdown"
            IconDropdown.Parent = IconFrame
            IconDropdown.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
            IconDropdown.BackgroundTransparency = 1.000
            IconDropdown.BorderColor3 = Color3.fromRGB(0, 0, 0)
            IconDropdown.BorderSizePixel = 0
            IconDropdown.Position = UDim2.new(0.200000003, 0, 0.200000003, 0)
            IconDropdown.Size = UDim2.new(0, 15, 0, 15)
            IconDropdown.Image = "rbxassetid://17325676878"

            ListFrame.Name = "ListFrame"
            ListFrame.Parent = Dropdown
            ListFrame.BackgroundColor3 = Color3.fromRGB(30, 30, 30)
            ListFrame.BorderColor3 = Color3.fromRGB(0, 0, 0)
            ListFrame.BorderSizePixel = 0
            ListFrame.Position = UDim2.new(0, 0, 0.521263361, 0)
            ListFrame.Size = UDim2.new(0, 349, 0, 125)
            ListFrame.Visible = false

            UICorner_16.Parent = ListFrame

            List.Name = "List"
            List.Parent = ListFrame
            List.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
            List.BackgroundTransparency = 1.000
            List.BorderColor3 = Color3.fromRGB(0, 0, 0)
            List.BorderSizePixel = 0
            List.Selectable = false
            List.Size = UDim2.new(1, 0, 1, 0)
            List.CanvasSize = UDim2.new(0, 0, 0, 0)
            List.ScrollBarThickness = 0
            spawn(function()
                while wait() do
                    List.CanvasSize = UDim2.new(0, 0, 0, UIListLayout_3.AbsoluteContentSize.Y)
                end
            end)

            UIPadding_19.Parent = List
            UIPadding_19.PaddingBottom = UDim.new(0, 6)
            UIPadding_19.PaddingLeft = UDim.new(0, 6)
            UIPadding_19.PaddingRight = UDim.new(0, 6)
            UIPadding_19.PaddingTop = UDim.new(0, 6)

            UIListLayout_3.Parent = List
            UIListLayout_3.SortOrder = Enum.SortOrder.LayoutOrder
            UIListLayout_3.Padding = UDim.new(0, 8)

            Options.CallBack(TextLabel_2.Text)

            for i, v in next, Options.List do
                local Option = Instance.new("TextButton")

                Option.Name = "Option"
                Option.Parent = List
                Option.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
                Option.BackgroundTransparency = 1.000
                Option.BorderColor3 = Color3.fromRGB(0, 0, 0)
                Option.BorderSizePixel = 0
                Option.Size = UDim2.new(1, 0, 0, 30)
                Option.Font = Enum.Font.SourceSansBold
                Option.Text = tostring(v)
                Option.TextColor3 = Color3.fromRGB(144, 144, 144)
                Option.TextSize = 14.000

                Option.MouseEnter:Connect(function()
                    TweenService:Create(
                        Option,
                        TweenInfo.new(0.3,Enum.EasingStyle.Quad,Enum.EasingDirection.Out),
                        {
                            TextTransparency = 0
                        }
                    ):Play()
                end)
    
                Option.MouseLeave:Connect(function()
                    TweenService:Create(
                        Option,
                        TweenInfo.new(0.3,Enum.EasingStyle.Quad,Enum.EasingDirection.Out),
                        {
                            TextTransparency = 0.5
                        }
                    ):Play()
                end)
    
                Option.MouseButton1Click:Connect(function()
                    Dropped = false
                    ListFrame.Visible = false
                    Dropdown:TweenSize(UDim2.new(0, 348, 0, 137),"Out","Quad",0.3,true)
                    TweenService:Create(
                        ListFrame,
                        TweenInfo.new(0.1 ,Enum.EasingStyle.Quad,Enum.EasingDirection.Out),
                        {
                            Visible = false
                        }
                    ):Play()
                    pcall(Option.Text)
                    Options.CallBack(Option.Text)
                    TextLabel_2.Text = Option.Text
                end)
            end

            Content_5.MouseButton1Click:Connect(function()
                if Dropped == false then
                    Dropped = true
                    Dropdown:TweenSize(UDim2.new(0, 348, 0.182, 137),"Out","Quad",0.3,true)
                    TweenService:Create(
                        ListFrame,
                        TweenInfo.new(0.2,Enum.EasingStyle.Quad,Enum.EasingDirection.Out),
                        {
                            Visible = true
                        }
                    ):Play()
                else
                    Dropped = false
                    Dropdown:TweenSize(UDim2.new(0, 348, 0, 137),"Out","Quad",0.3,true)
                    TweenService:Create(
                        ListFrame,
                        TweenInfo.new(0.1 ,Enum.EasingStyle.Quad,Enum.EasingDirection.Out),
                        {
                            Visible = false
                        }
                    ):Play()
                end
            end)

        end

        function TabContent:CreateLabel(Options)
            Options = Options or {}
            Options.Text = Options.Text or ""

            local Label = Instance.new("Frame")
            local UICorner_8 = Instance.new("UICorner")
            local UIStroke_4 = Instance.new("UIStroke")
            local Content = Instance.new("TextLabel")
            local UIPadding_9 = Instance.new("UIPadding")
            local LabelUpdate = {}

            Label.Name = "Label"
            Label.Parent = Tab1
            Label.BackgroundColor3 = Color3.fromRGB(71, 71, 71)
            Label.BorderColor3 = Color3.fromRGB(0, 0, 0)
            Label.BorderSizePixel = 0
            Label.Position = UDim2.new(0, 0, 0.224264711, 0)
            Label.Size = UDim2.new(0, 348, 0, 38)

            UICorner_8.CornerRadius = UDim.new(0, 4)
            UICorner_8.Parent = Label

            UIStroke_4.Color = Color3.fromRGB(100, 100, 100)
            UIStroke_4.Parent = Label

            Content.Name = "Content"
            Content.Parent = Label
            Content.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
            Content.BackgroundTransparency = 1.000
            Content.BorderColor3 = Color3.fromRGB(0, 0, 0)
            Content.BorderSizePixel = 0
            Content.ClipsDescendants = true
            Content.Size = UDim2.new(1, 0, 1, 0)
            Content.Font = Enum.Font.Unknown
            Content.Text = Options.Text
            Content.TextColor3 = Color3.fromRGB(255, 255, 255)
            Content.TextSize = 14.000
            Content.TextWrapped = true
            Content.TextXAlignment = Enum.TextXAlignment.Left
            
            UIPadding_9.Parent = Content
            UIPadding_9.PaddingLeft = UDim.new(0, 7)

            function LabelUpdate:Set(v)
                Options.Text = v
                LabelUpdate:update()
            end

            function LabelUpdate:update()
                Content.Text = Options.Text

                Content.Size = UDim2.new(Content.Size.X.Scale, Content.Size.X.Offset, 0, math.huge)
                Content.Size = UDim2.new(Content.Size.X.Scale, Content.Size.X.Offset, 0, Content.TextBounds.Y)
                Label.Size = UDim2.new(Content.Size.X.Scale, Content.Size.X.Offset, 0, Content.TextBounds.Y + 12)
            end

            LabelUpdate:update()
            return LabelUpdate
            
        end

        return TabContent

    end

    return UITab
end

