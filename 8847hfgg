A:CreateToggle({
    Name = "Auto Raid",
    Description = "Auto Kill Aura Mob And Auto Next Island",
    Default = _G.S.DonateForme,
    CallBack = function(Value)
        _G.S.DonateForme = Value
        SaveSettings()
    end
})

spawn(function()
    while wait() do
        if _G.S.DonateForme then
            if plr.PlayerGui.Main.Timer.Visible == true then
                for i, v in pairs(game.Workspace.Enemies:GetChildren()) do
                    if (v:WaitForChild("HumanoidRootPart").Position - plr.Character.HumanoidRootPart.Position).Magnitude <= 1000 then
                        repeat task.wait()
                            v.Humanoid.Health = 0
                            v.Humanoid.WalkSpeed = 0
                            v.HumanoidRootPart.CanCollide = false
                            v.Head.CanCollide = false
                        until v.Humanoid.Health <= 0 or not v.Parent
                    end
                end
                if NextIsland() then
                    Tween(NextIsland().CFrame * CFrame.new(0, 60, 0))
                end
            end
        end
    end
end)

S:CreateSection({ Text = "Status" })

local CakePrinceCount = S:CreateParagraph({Title = "Cake Prince : Not Spawn", Content = ""})

spawn(function()
    while task.wait() do
        pcall(function()
            if string.len(game:GetService("ReplicatedStorage").Remotes.CommF_:InvokeServer("CakePrinceSpawner")) == 88 then
                CakePrinceCount:Set({Title = "Killed : "..string.sub(game:GetService("ReplicatedStorage").Remotes.CommF_:InvokeServer("CakePrinceSpawner"),39,41), Content = ""})
            elseif string.len(game:GetService("ReplicatedStorage").Remotes.CommF_:InvokeServer("CakePrinceSpawner")) == 87 then
                CakePrinceCount:Set({Title = "Killed : "..string.sub(game:GetService("ReplicatedStorage").Remotes.CommF_:InvokeServer("CakePrinceSpawner"),39,40), Content = ""})
            elseif string.len(game:GetService("ReplicatedStorage").Remotes.CommF_:InvokeServer("CakePrinceSpawner")) == 86 then
                CakePrinceCount:Set({Title = "Killed : "..string.sub(game:GetService("ReplicatedStorage").Remotes.CommF_:InvokeServer("CakePrinceSpawner"),39,39), Content = ""})
            else
                CakePrinceCount:Set({Title = "Cake Prince : Spawned", Content = ""})
            end
        end)
    end
end)

local Elite = S:CreateParagraph({Title = "Elite : Not Spawn", Content = ""})

spawn(function()
    while wait() do
        if game:GetService("ReplicatedStorage"):FindFirstChild("Diablo") or game:GetService("ReplicatedStorage"):FindFirstChild("Deandre") or game:GetService("ReplicatedStorage"):FindFirstChild("Urban") or game:GetService("Workspace").Enemies:FindFirstChild("Diablo") or game:GetService("Workspace").Enemies:FindFirstChild("Deandre") or game:GetService("Workspace").Enemies:FindFirstChild("Urban") then
            Elite:Set({Title = "Elite : Spawned", Content = ""})
        else
            Elite:Set({Title = "Elite : Not Spawn", Content = ""})
        end
    end
end)

function MoonTextureId()
    if Sea1 then
        return game:GetService("Lighting").FantasySky.MoonTextureId
    elseif Sea2 then
        return game:GetService("Lighting").FantasySky.MoonTextureId
    elseif Sea3 then
        return game:GetService("Lighting").Sky.MoonTextureId
    end
end  

function CheckMoon()
    OiOi = MoonTextureId()
    if OiOi == "http://www.roblox.com/asset/?id=9709149431" then
        return 'Full Moon'
    elseif OiOi == "http://www.roblox.com/asset/?id=9709149052" then
        return "Next Night"
    elseif OiOi == "http://www.roblox.com/asset/?id=9709143733" or OiOi == "http://www.roblox.com/asset/?id=9709150401" or OiOi == "http://www.roblox.com/asset/?id=9709149680" then
        return "Bad Moon"
    else
        return "Not Moon"
    end
end

local Moon = S:CreateParagraph({Title = "Moon : Not Moon", Content = "Check Status Moon : Not Moon"})

spawn(function()
    while wait() do
        pcall(function()
            Moon:Set({Title = "Moon : "..CheckMoon().."", Content = ""})
        end)
    end
end)

