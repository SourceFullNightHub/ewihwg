                elseif _G.S.SelectIsland == "Tiki Outpost" then
                    Tween(CFrame.new(-16207.501953125, 9.0863618850708, 475.1490783691406))
		        end
		    until not _G.S.TweentoIsland
            CancelTween()
		end
    end
})

