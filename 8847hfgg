            Content_3.MouseButton1Click:Connect(function()
                if Toggled == false then

                    TweenService:Create(CheckerFrame, TweenInfo.new(.2, Enum.EasingStyle.Quad, Enum.EasingDirection.Out), {
                        BackgroundColor3 = Color3.fromRGB(75, 117, 255)
                    }):Play()
                    
                    TweenService:Create(ImageLabel_2, TweenInfo.new(.3, Enum.EasingStyle.Quad, Enum.EasingDirection.Out), {
                        ImageTransparency = 0.5
                    }):Play()
                    
                    ImageLabel_2.Visible = true
                    wait(0.01)
                    
                    TweenService:Create(ImageLabel_2, TweenInfo.new(.3, Enum.EasingStyle.Quad, Enum.EasingDirection.Out), {
                        ImageTransparency = 0
                    }):Play()
                    
