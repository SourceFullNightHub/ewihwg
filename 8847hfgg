R:CreateToggle({
    Name = "Auto Trials",
    Description = "Auto Completed Your Quest Race",
    Default = _G.S.AutoTrials,
    CallBack = function(Value)
        _G.S.AutoTrials = Value
        SaveSettings()
    end
})

task.spawn(function()
    while task.wait() do
        if _G.S.AutoTrials and Sea3 then
            pcall(function()
                if plr.Data.Race.Value == "Human" then
                    for i,v in pairs(WS.Enemies:GetDescendants()) do
                        if v:FindFirstChild("Humanoid") and v:FindFirstChild("HumanoidRootPart") and v.Humanoid.Health > 0 then
                            repeat wait(.1)
                                v.Humanoid.Health = 0
                                v.HumanoidRootPart.CanCollide = false
                                sethiddenproperty(plr, "SimulationRadius", math.huge)
                            until not _G.S.AutoTrials or v.Humanoid.Health <= 0
                        end
                    end
                elseif plr.Data.Race.Value == "Skypiea" then
                    for i,v in pairs(game:GetService("Workspace").Map.SkyTrial.Model:GetDescendants()) do
                        if v.Name ==  "snowisland_Cylinder.081" and (v.Position - game.Players.LocalPlayer.Character.HumanoidRootPart.Position).Magnitude <= 2550 then
                            Tween(v.CFrame * CFrame.new(0 ,0 ,0))
                        end
                    end
                elseif game:GetService("Workspace")["_WorldOrigin"].Locations:FindFirstChild("Trial of Water") then
                    for i,v in pairs(WS.SeaBeasts:GetChildren()) do
                        if v.Name == "SeaBeast1" and v:FindFirstChild("HumanoidRootPart") then
                            if v.Health.Value > 0 then
                                if (v.HumanoidRootPart.Position - plr.Character.HumanoidRootPart.Position).Magnitude <= 3000 then
                                    repeat task.wait()
                                        EnableBuso()
                                        Tween(v.HumanoidRootPart.CFrame * CFrame.new(0,400,0))
                                        AimPosition = v.HumanoidRootPart.Position
                                        SkillAim = true
                                        SpamSkill = true
                                    until not v.Parent or v.Health.Value <= 0 or not _G.S.AutoTrials
                                    CancelTween()
                                    SkillAim = false
                                    AimPosition = nil
                                    SpamSkill = false
                                end
                            end
                        end
                    end
                elseif plr.Data.Race.Value == "Cyborg" then
                    if (CFrame.new(-20021.8691, 10090.4893,-16.37994).Position - plr.Character.HumanoidRootPart.Position).Magnitude <= 2000 then
                        Tween(CFrame.new(-20021.8691, 10090.4893,-16.37994))
                    end
                elseif plr.Data.Race.Value == "Ghoul" then
                    if game:GetService("Workspace").Map:FindFirstChild("GhoulTrial") and game:GetService("Workspace")["_WorldOrigin"].Locations:FindFirstChild("Trial of Carnage") then
                        GhoulRoom = game:GetService("Workspace")["_WorldOrigin"].Locations["Trial of Carnage"].CFrame
                        if (game.Players.LocalPlayer.Character.HumanoidRootPart.Position - GhoulhPart.Position).Magnitude <= 1000 then
                            BringMoNearest()
                            for i,v in pairs(WS.Enemies:GetDescendants()) do
                                if v:FindFirstChild("Humanoid") and v:FindFirstChild("HumanoidRootPart") and v.Humanoid.Health > 0 then
                                    repeat wait(.1)
                                        v.Humanoid.Health = 0
                                        v.HumanoidRootPart.CanCollide = false
                                        sethiddenproperty(plr, "SimulationRadius", math.huge)
                                    until not _G.S.AutoTrials or v.Humanoid.Health <= 0
                                end
                            end
                            if (CFrame.new(-11706.6777, 10011.5615, 11.6579161).Position - game.Players.LocalPlayer.Character.HumanoidRootPart.Position).Magnitude <= 500 then
                            Tweento(CFrame.new(-11706.6777, 10011.5615, 11.6579161) * CFrame.new(0, math.random(-15, 30), 0))
                        end
                    end
                end
                elseif plr.Data.Race.Value == "Mink" then
                    if (game:GetService("Workspace").StartPoint.Position - plr.Character.HumanoidRootPart.Position).Magnitude <= 500 then
                        Tween(game:GetService("Workspace").StartPoint.CFrame)
                        for nah, niga in pairs(game:GetDescendants()) do
                            if niga:IsA("TouchInterest") or niga.Name == "TouchInterest" then
                                if (niga.Position - plr.Character.HumanoidRootPart.Position).Magnitude <= 50 then
                                    firetouchinterest(niga)
                                end
                            end
                        end
                    end
                end
            end)
        end
    end
end)

R:CreateToggle({
    Name = "Auto Kill Players After Trials",
    Description = "Auto Kill Players After Trials I Ready Upgrade!!!",
    Default =  _G.S.KillTrialBeo,
    CallBack = function(Value)
        _G.S.KillTrialBeo = Value
        SaveSettings()
    end
})

spawn(function()
    while task.wait() do
        if _G.S.KillTrialBeo and Sea3 then
            for i, v in pairs(Chaster:GetChildren()) do
                if v.Name ~= plr.Name and v.Humanoid.Health > 0 and v.Parent and v:FindFirstChild("HumanoidRootPart") then
                    if plr.PlayerGui.Main.Timer.Visible == true and (CFrame.new(28718.88671875, 14907.6630859375, -56.46955871582031).Position - plr.Character.HumanoidRootPart.Position).Magnitude <= 240 then
                        if (v.HumanoidRootPart.Position - plr.Character.HumanoidRootPart.Position).Magnitude <= 500 then
                            repeat task.wait()
                                v.HumanoidRootPart.Size = Vector3.new(30, 30, 30)
                                Tweento(v.HumanoidRootPart.CFrame * CFrame.new(0, 0, 2))
                                Click()
                                EquipWeapon()
                                EnableBuso()
                                nhieufunctionlam = true
                            until not v.Parent or v.Humanoid.Health <= 0 or not v:FindFirstChild("HumanoidRootPart") or plr.PlayerGui.Main.Timer.Visible == false
                            nhieufunctionlam = false
                        end
                    end
                end
            end
        end
    end
end)

spawn(function()
    while task.wait() do
        if _G.S.KillTrialBeo then
            if nhieufunctionlam then
                if _G.S.Z then
                    SendKeyEvents("Z")
                end
                if _G.S.X then
                    SendKeyEvents("X")
                end
                if _G.S.C then
                    SendKeyEvents("C")
                end
                if _G.S.KenHaki then
                    Rc.Remotes.CommE:FireServer("Ken",true)
                end
            end
        end
    end
end)

P:CreateSection({ Text = "Melee" })

P:CreateDropdown({
    Name = "Select Melee",
    Description = "Select Melee Your Need To Buy",
    Options = {"Black Leg", "Electro", "Fishman Karate", "Dragon Claw", "Superhuman", "Death Step", "Sharkman Karate", "Electric Claw", "Dragon Talon", "Godhuman", "Sanguine Art"},
    Default = "",
    CallBack = function(Value)
        Muamelee = Value
        if Muamelee == "Black Leg" then
            game:GetService("ReplicatedStorage").Remotes.CommF_:InvokeServer("BuyBlackLeg")            
        elseif Muamelee == "Electro" then
            game:GetService("ReplicatedStorage").Remotes.CommF_:InvokeServer("BuyElectro")
        elseif Muamelee == "Fishman Karate" then
            game:GetService("ReplicatedStorage").Remotes.CommF_:InvokeServer("BuyFishmanKarate")
        elseif Muamelee == "Dragon Claw" then
            game:GetService("ReplicatedStorage").Remotes.CommF_:InvokeServer("BlackbeardReward", "DragonClaw", "2")
        elseif Muamelee == "Superhuman" then
            game:GetService("ReplicatedStorage").Remotes.CommF_:InvokeServer("BuySuperhuman")
        elseif Muamelee == "Death Step" then
            game:GetService("ReplicatedStorage").Remotes.CommF_:InvokeServer("BuyDeathStep")
        elseif Muamelee == "Sharkman Karate" then
            game:GetService("ReplicatedStorage").Remotes.CommF_:InvokeServer("BuySharkmanKarate",true)
            game:GetService("ReplicatedStorage").Remotes.CommF_:InvokeServer("BuySharkmanKarate")
        elseif Muamelee == "Electric Claw" then
            game:GetService("ReplicatedStorage").Remotes.CommF_:InvokeServer("BuyElectricClaw")
        elseif Muamelee == "Dragon Talon" then
            game:GetService("ReplicatedStorage").Remotes.CommF_:InvokeServer("BuyDragonTalon")
        elseif Muamelee == "Godhuman" then
            game:GetService("ReplicatedStorage").Remotes.CommF_:InvokeServer("BuyGodhuman")
        elseif Muamelee == "Sanguine Art" then
            game:GetService("ReplicatedStorage").Remotes.CommF_:InvokeServer("BuySanguineArt")
        end 
    end    
})

