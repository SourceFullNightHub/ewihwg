I:CreateToggle({
    Name = "Auto Summon Rip Indra",
    Description = "Beta",
    Default = _G.S.SummonRipIndra,
    CallBack = function(Value)
        _G.S.SummonRipIndra = Value
        SaveSettings()
    end
})

spawn(function()
    while task.wait() do
        if _G.S.SummonRipIndra and Sea3 then
            if CheckColorRipIndra() and not CheckEnemies("rip_indra") and not CheckEnemies("rip_indra True Form") and not CheckReplicatedStorage("rip_indra") and not CheckReplicatedStorage("rip_indra True Form") then
                if plr.Backpack:FindFirstChild("God's Chalice") or plr.Character:FindFirstChild("God's Chalice") then
                    AutoActiveColorRip_Indra()
                else
                    notify("Script Warning", "Your Not Have God's Chalice", 5)
                end 
            elseif not CheckColorRipIndra() then
                if not CheckEnemies("rip_indra") and not CheckEnemies("rip_indra True Form") then
                    if plr.Backpack:FindFirstChild("God's Chalice") or plr.Character:FindFirstChild("God's Chalice") then
                        EquipTool("God's Chalice")
                        wait(0.5)
                        Tween(CFrame.new(-5560.12549, 313.915466, -2664.00708, -0.430556327, -3.33702346e-08, 0.902563691, -4.32060006e-08, 1, 1.63618576e-08, -0.902563691, -3.1951469e-08, -0.430556327))
                    end
                elseif CheckEnemies("rip_indra") or CheckEnemies("rip_indra True Form") then
                    KillMob("rip_indra")
                    KillMob("rip_indra True Form")
                else
                    Tween(CFrame.new(-5560.12549, 313.915466, -2664.00708))
                end
            end
        end
    end
end)

I:CreateToggle({
    Name = "Auto Dough King",
    Description = "Auto Trade God's Chalice to Sweet Chalice, Teleport To Cake Island Kill 500 Mob Summon Dough King and Kill Dough King",
    Default = _G.S.AutoDoughKing,
    CallBack = function(Value)
        _G.S.AutoDoughKing = Value
        SaveSettings()
    end
})

spawn(function()
    while task.wait() do
        pcall(function()
            if _G.S.AutoDoughKing and Sea3 then
                if CheckEnemies("Dough King") and not CheckGlassCake() then
                    KillMob("Dough King")
                else
                    if game.ReplicatedStorage:FindFirstChild("Dough King") then
                        Tween(CFrame.new(-2125.64282, 70.3262863, -12400.6777))
                    else
                        if CheckGlassCake() and not CheckEnemies("Dough King") then
                            if plr.Backpack:FindFirstChild("Sweet Chalice") and plr.Character:FindFirstChild("Sweet Chalice") then
                                if CheckEnemies("Baking Staff") or CheckEnemies("Cookie Crafter") or CheckEnemies("Cake Guard") or CheckEnemies("Head Baker") then
                                    KillMob("Baking Staff", _G.S.AutoDoughKing)
                                    KillMob("Cookie Crafter", _G.S.AutoDoughKing)
                                    KillMob("Cake Guard", _G.S.AutoDoughKing)
                                    KillMob("Head Baker", _G.S.AutoDoughKing)
                                else
                                    Tween(CFrame.new(-2091.911865234375, 70.00884246826172, -12142.8359375))
                                end
                            elseif not plr.Backpack:FindFirstChild("Sweet Chalice") and not plr.Character:FindFirstChild("Sweet Chalice") then
                                if plr.Backpack:FindFirstChild("God's Chalice") or plr.Character:FindFirstChild("God's Chalice") then
                                    if game.ReplicatedStorage.Remotes.CommF_:InvokeServer("SweetChaliceNpc") == "Where are the items?" then
                                        game.ReplicatedStorage.Remotes.CommF_:InvokeServer("SweetChaliceNpc")
                                    end
                                end
                            end
                        end
                    end
                end
            end
        end)
    end
end)

