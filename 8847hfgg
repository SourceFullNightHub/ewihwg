local Sea2toggle = I:CreateToggle({
    Name = "Auto Seconds Sea",
    Description = "Auto Seconds Sea(function Only Sea 1)",
    Default = _G.S.BucacSea2,
    CallBack = function(Value)
        _G.S.BucacSea2 = Value
        SaveSettings()
    end
})

spawn(function()
    while wait() do
        if _G.S.BucacSea2 and plr.Data.Level.Value >= 700 then
            if game.ReplicatedStorage.Remotes.CommF_:InvokeServer("DressrosaQuestProgress", "Dressrosa") ~= 0 then
                if game.Workspace.Map.Ice.Door.CanCollide then
                    if not plr.Backpack:FindFirstChild("Key") and not plr.Character:FindFirstChild("Key") then
                        if CheckDistancebehon(CFrame.new(4852.2895507813, 5.651451587677, 718.53070068359), 5) then
                            game.ReplicatedStorage.Remotes["CommF_"]:InvokeServer("DressrosaQuestProgress", "Detective")
                            Library:MakeNotify({
                                Name = "Night Hub",
                                Content = "Ask To NPC In Prison",
                                Time = 1,
                            })
                            wait(0.1)
                            EquipTool("Key")
                        else
                            Tween(CFrame.new(4852.2895507813, 5.651451587677, 718.53070068359))
                        end
                    else
                        EquipTool("Key")
                        if plr.Character:FindFirstChild("Key") then
                            Tween(game.Workspace.Map.Ice.Door.CFrame)
                            Library:MakeNotify({
                                Name = "Night Hub",
                                Content = "Teleport Ice Door",
                                Time = 1,
                            })
                        end
                    end
                elseif not game.Workspace.Map.Ice.Door.CanCollide then
                    if CheckBoss("Ice Admiral") then
                        KillBoss("Ice Admiral", _G.S.BucacSea2)
                        Library:MakeNotify({
                            Name = "Night Hub",
                            Content = "Kill Ice Admiral",
                            Time = 1,
                        })
                    else
                        Tween(game.Workspace.Map.Ice.Door.CFrame)
                    end
                end
            elseif game.ReplicatedStorage.Remotes.CommF_:InvokeServer("DressrosaQuestProgress", "Dressrosa") == 0 then
                game:GetService("ReplicatedStorage").Remotes.CommF_:InvokeServer("TravelDressrosa")
                Library:MakeNotify({
                    Name = "Night Hub",
                    Content = "Teleport To Seconds Sea",
                    Time = 1,
                })
                wait(0.5)
                Sea2toggle:Set(false)
            end
        end
    end
end)

I:CreateSection({ Text = "Sea 2 Item & Quest" })

local BartiloToggle = I:CreateToggle({
    Name = "Auto Bartilo(Quest)",
    Description = "Auto bartilo Quest(function Only Sea 2)",
    Default = _G.S.ChichBartilo,
    CallBack = function(Value)
        _G.S.ChichBartilo = Value
        SaveSettings()
    end
})

spawn(function()
    while wait() do
        if _G.S.ChichBartilo and CheckSea(2) then
            if plr.Data.Level.Value >= 800 and game:GetService("ReplicatedStorage").Remotes.CommF_:InvokeServer("BartiloQuestProgress","Bartilo") == 0 then
                if CheckQuestVisible() and CheckTextQuest("Swan Pirates") and CheckTextQuest("50") then
                    if CheckEnemies("Swan Pirates") then
                        KillMob("Swan Pirates", _G.S.ChichBartilo)
                    end
                else
                    repeat
                        Tween(CFrame.new(932.624451, 156.106079, 1180.27466))
                    until CheckDistancebehon(CFrame.new(932.624451, 156.106079, 1180.27466), 5)
                    game:GetService("ReplicatedStorage").Remotes.CommF_:InvokeServer("StartQuest","BartiloQuest",1)
                end
            elseif plr.Data.Level.Value >= 800 and game:GetService("ReplicatedStorage").Remotes.CommF_:InvokeServer("BartiloQuestProgress","Bartilo") == 1 then
                if CheckBoss("Jeremy") then
                    KillBoss("Jeremy", _G.S.ChichBartilo)
                end
            elseif plr.Data.Level.Value >= 800 and game:GetService("ReplicatedStorage").Remotes.CommF_:InvokeServer("BartiloQuestProgress","Bartilo") == 2 then
                repeat 
                    Tween(CFrame.new(-1850.49329, 13.1789551, 1750.89685)) 
                    wait() 
                until not _G.S.ChichBartilo or (plr.Character.HumanoidRootPart.Position-Vector3.new(-1850.49329, 13.1789551, 1750.89685)).Magnitude <= 10
                wait(1)
                repeat 
                    Tween(CFrame.new(-1858.87305, 19.3777466, 1712.01807)) 
                    wait() 
                until not _G.S.ChichBartilo or (plr.Character.HumanoidRootPart.Position-Vector3.new(-1858.87305, 19.3777466, 1712.01807)).Magnitude <= 10
                wait(1)
                repeat 
                    Tween(CFrame.new(-1803.94324, 16.5789185, 1750.89685)) 
                    wait() 
                until not _G.S.ChichBartilo or (plr.Character.HumanoidRootPart.Position-Vector3.new(-1803.94324, 16.5789185, 1750.89685)).Magnitude <= 10
                wait(1)
                repeat 
                    Tween(CFrame.new(-1858.55835, 16.8604317, 1724.79541)) 
                    wait() 
                until not _G.S.ChichBartilo or (plr.Character.HumanoidRootPart.Position-Vector3.new(-1858.55835, 16.8604317, 1724.79541)).Magnitude <= 10
                wait(1)
                repeat 
                    Tween(CFrame.new(-1869.54224, 15.987854, 1681.00659)) 
                    wait() 
                until not _G.S.ChichBartilo or (plr.Character.HumanoidRootPart.Position-Vector3.new(-1869.54224, 15.987854, 1681.00659)).Magnitude <= 10
                wait(1)
                repeat 
                    Tween(CFrame.new(-1800.0979, 16.4978027, 1684.52368)) 
                    wait() 
                until not _G.S.ChichBartilo or (plr.Character.HumanoidRootPart.Position-Vector3.new(-1800.0979, 16.4978027, 1684.52368)).Magnitude <= 10
                wait(1)
                repeat 
                    Tween(CFrame.new(-1819.26343, 14.795166, 1717.90625)) 
                    wait() 
                until not _G.S.ChichBartilo or (plr.Character.HumanoidRootPart.Position-Vector3.new(-1819.26343, 14.795166, 1717.90625)).Magnitude <= 10
                wait(1)
                repeat 
                    Tween(CFrame.new(-1813.51843, 14.8604736, 1724.79541)) 
                wait() 
                until not _G.S.ChichBartilo or (plr.Character.HumanoidRootPart.Position-Vector3.new(-1813.51843, 14.8604736, 1724.79541)).Magnitude <= 10
            elseif game:GetService("ReplicatedStorage").Remotes.CommF_:InvokeServer("BartiloQuestProgress","Bartilo") == 3 then
                BartiloToggle:Set(false)
                Library:MakeNotify({
                    Name = "Night Hub",
                    Content = "Auto Bartilo Quest Is Done!!!",
                    Time = 1,
                })
            end
        end
    end
end)

local GhoulRaceToggle = I:CreateToggle({
    Name = "Auto Ghoul Race",
    Description = "Auto Kill Cursed Captain To Get Torch Trade With NPC In CursedShip to get Race Ghoul(function Only Sea 2)",
    Default = _G.S.DitGhoul,
    CallBack = function(Value)
        _G.S.DitGhoul = Value
        SaveSettings()
    end
})

I:CreateToggle({
    Name = "Auto Ghoul Race(Hop)",
    Description = "If Not Cursed Captian then Hop Servers(function Only Sea 2)",
    Default = _G.S.HopB,
    CallBack = function(Value)
        _G.S.HopB = Value
        SaveSettings()
    end
})

spawn(function()
    while task.wait() do
        if _G.S.DitGhoul and Sea2 then
            if plr.Data.Race.Value ~= "Ghoul" then
                if not plr.Backpack:FindFirstChild("Hellfire Torch") and not plr.Character:FindFirstChild("Hellfire Torch") then
                    if CheckBoss("Cursed Captain") then
                        Library:MakeNotify({
                            Name = "Night Hub",
                            Content = "Kill Cursed Captain",
                            Time = 1,
                        })
                        KillBoss("Cursed Captain", _G.S.DitGhoul)
                        wait(_G.S.DelayHop)
                    else
                        if _G.S.HopB == true then
                            Library:MakeNotify({
                                Name = "Night Hub",
                                Content = "Not Have Boss, Hop Servers",
                                Time = 1,
                            })
                            HopServer()
                            wait(_G.S.DelayHop)
                        end
                    end
                elseif plr.Backpack:FindFirstChild("Hellfire Torch") or plr.Character:FindFirstChild("Hellfire Torch") then
                    if CheckDistancebehon(CFrame.new(923.748596, 125.529785, 33454.4883), 5) then
                        game:GetService("ReplicatedStorage").Remotes.CommF_:InvokeServer("Ectoplasm", "Change")
                        Library:MakeNotify({
                            Name = "Night Hub",
                            Content = "Change Race To Ghoul",
                            Time = 1,
                        })
                        wait(5)
                    else
                        Tween(CFrame.new(923.748596, 125.529785, 33454.4883))
                        Library:MakeNotify({
                            Name = "Night Hub",
                            Content = "Teleport To NPC In Cursed Ship",
                            Time = 1,
                        })
                        wait(5)
                    end
                end
            elseif plr.Data.Race.Value == "Ghoul" then
                GhoulRaceToggle:Set(false)
                Library:MakeNotify({
                    Name = "Night Hub",
                    Content = "Your Race Is Ghoul!!!",
                    Time = 1,
                })
                wait(5)
            end
        end
    end
end)

