                        NAMEISLANDESP.Font = "GothamBold"
                        NAMEISLANDESP.FontSize = "Size14"
                        NAMEISLANDESP.TextWrapped = true
                        NAMEISLANDESP.Size = UDim2.new(1,0,1,0)
                        NAMEISLANDESP.TextYAlignment = 'Top'
                        NAMEISLANDESP.BackgroundTransparency = 1
                        NAMEISLANDESP.TextStrokeTransparency = 0.5
                        NAMEISLANDESP.TextColor3 = Color3.fromRGB(27, 42, 53)
                    else
                        v['Hitle chinh phuc chau Au'].TextLabel.Text = (v.Name ..'   \n'.. round((game:GetService('Players').LocalPlayer.Character.Head.Position - v.Position).Magnitude/3) ..' Distance')
                    end
                end
            else
                if v:FindFirstChild('Hitle chinh phuc chau Au') then
                    v:FindFirstChild('Hitle chinh phuc chau Au'):Destroy()
                end
            end
        end)
    end
