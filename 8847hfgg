})

spawn(function()
    while task.wait() do
        if _G.S.AutoThirdSeaLord and Sea2 then
            if plr.Data.Level.Value >= 1500 then
                if not CheckEnemies("rip_indra") then
                    if (CFrame.new(-1926.3221435547, 12.819851875305, 1738.3092041016).Position - plr.Character.HumanoidRootPart.Position).Magnitude <= 10 then
                        game:GetService("ReplicatedStorage").Remotes.CommF_:InvokeServer("ZQuestProgress","Begin")
                    else
                        Tween(CFrame.new(-1926.3221435547, 12.819851875305, 1738.3092041016))
                    end
                elseif CheckEnemies("rip_indra") then
                    v = CheckEnemies("rip_indra")
                    repeat task.wait()
                        KillMob("rip_indra")
                    until not v.Parent or v.Humanoid.Health <= 0 or not v:FindFirstChild("HumanoidRootPart") or not _G.S.AutoThirdSeaLord
                    game:GetService("ReplicatedStorage").Remotes.CommF_:InvokeServer("TravelZou")
                end
            end
        end
    end
end)


