            if CheckInventory("Pole (1st Form)") and plr.Data.Level.Value >= 200 then
                Library:MakeNotify({
                    Name = "Night Hub",
                    Content = "Your Are Have Pole",
                    Time = 1,
                })
                PoleTakenToggle:Set(false)
            elseif not CheckInventory("Pole (1st Form)") and plr.Data.Level.Value >= 200 then
                if CheckEnemies("Thunder God") then
                    KillMob("Thunder God", _G.S.TakenPole)
                end
            end
        end
    end
end)

