T:CreateSection({ Text = "Settings FastAttack & Bring Mob & Bypass" })
print('[Loaded Complete FastAttack No Cooldown]')

T:CreateToggle({
    Name = "Enable FastAttack",
    Description = "Attack Fast And No CoolDown",
    Default = _G.S.FastAttack,
    CallBack = function(Value)
        _G.S.FastAttack = Value
        SaveSettings()
    end
})

local TableAttack = {'100', '90', '80', '70', '60', '50', '40', '30', '20', '10', '5', '1'}
T:CreateDropdown({
    Name = "FastAttack Cooldown",
    Description = "The lower the Delay number, the lower the cooldown of fastattack < for example 100 = 0.175 >",
    Options = TableAttack,
    Default = _G.S.FastAttackDelay,
    CallBack = function(Value)
        _G.S.FastAttackDelay = Value
        SaveSettings()
    end
})

spawn(function()
    while task.wait() do
        if _G.S.FastAttackDelay then
            if _G.S.FastAttackDelay == "100" then
                DelayAttack = 0.175
            elseif _G.S.FastAttackDelay == "90" then
                DelayAttack = 0.15
            elseif _G.S.FastAttackDelay == "80" then
                DelayAttack = 0.1
            elseif _G.S.FastAttackDelay == "70" then
                DelayAttack = 0.075
            elseif _G.S.FastAttackDelay == "60" then
                DelayAttack = 0.05
            elseif _G.S.FastAttackDelay == "50" then
                DelayAttack = 0.025
            elseif _G.S.FastAttackDelay == "40" then
                DelayAttack = 0.01
            elseif _G.S.FastAttackDelay == "30" then
                DelayAttack = 0.0075
            elseif _G.S.FastAttackDelay == "20" then
                DelayAttack = 0.005
            elseif _G.S.FastAttackDelay == "10" then
                DelayAttack = 0.001
            elseif _G.S.FastAttackDelay == "5" then
                DelayAttack = 0.0005
            elseif _G.S.FastAttackDelay == "0" then
                DelayAttack = 0 
            end
        end
    end
end)
spawn(function()
    while task.wait(.000000000000000025) do
        if _G.S.FastAttack then
            pcall(function()
                repeat task.wait(DelayAttack)
                    AttackByNightHub()
                until not _G.S.FastAttack
            end)
        end
    end
end)
