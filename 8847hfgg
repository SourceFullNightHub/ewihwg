local Lib = Library:CreateWindow({Title = "Night Hub", Creator = "By !Nightx"})

local A = Lib:CreateTab({Name = "About", Icon = "rbxassetid://17325442535"})
local H = Lib:CreateTab({Name = "Home", Icon = "rbxassetid://17184558565"})
local I = Lib:CreateTab({Name = "Item", Icon = "rbxassetid://17184405349"})
local S = Lib:CreateTab({Name = "Stats", Icon = "rbxassetid://17184559957"})
local E = Lib:CreateTab({Name = "Sea Event", Icon = "rbxassetid://17184412007"})
local U = Lib:CreateTab({Name = "Upgrated Race", Icon = "rbxassetid://17184413595"})
local V = Lib:CreateTab({Name = "Status Servers", Icon = "rbxassetid://17184408884"})
local M = Lib:CreateTab({Name = "Misc", Icon = "rbxassetid://17195489739"})
local T = Lib:CreateTab({Name = "Settings", Icon = "rbxassetid://17184407740"})

H:CreateDropdown({
    Name = "Select Tool",
    Description = "Select Tool To Farm",
    List = {"Melee", "Sword"},
    Default = _G.Settings.SelectTool,
    CallBack = function(Value)
        _G.Settings.SelectTool = Value
        SaveSettings()
    end
})

while wait() do
    if _G.Settings.SelectTool == "Melee" then
        for i, v in pairs(plr.Backpack:GetChildren()) do
            if v.ToolTip == "Melee" and plr.Backpack:FindFirstChild(tostring(v.Name)) then
                Tool = v.Name
            end
        end
    elseif _G.Settings.SelectTool == "Sword" then
        for i, v in pairs(plr.Backpack:GetChildren()) do
            if v.ToolTip == "Sword" and plr.Backpack:FindFirstChild(tostring(v.Name)) then
                Tool = v.Name
            end
        end
    end
end

local SelectMthodSection = H:CreateSections({Text = "Select Method Farm"})

H:CreateDropdown({
    Name = "Select Method Farm",
    Description = "Select Method Farm | EX : Cake Prince or Level or Bone",
    List = {"Level", "Cake Prince", "Bone"},
    Default = _G.Settings.SelectMethodFarm,
    CallBack = function(Value)
        _G.Settings.SelectMethodFarm = Value
        SaveSettings()
    end
})

H:CreateToggle({
    Name = "Start Farm",
    Description = "Start Farm Method Your Selected",
    Default = _G.Settings.StFarm,
    CallBack = function(Value)
        _G.Settings.StFarm = Value
        SaveSettings()
    end
})

while wait() do
    if _G.Settings.StFarm then
        if _G.Settings.SelectMethodFarm == "Level" then
            GetQuest()
            if not CheckQuestVisible() or not CheckTextQuest(NameMob) then
                TakeQuest(CFrameQuest, NameQuest, NumberQuest)
            elseif CheckQuestVisible() and CheckTextQuest(NameMob) then
                if CheckEnemies(Mob) then
                    KillMob(Mob)
                else
                    Tween(CFrameMob)
                end
            end
        end
    end
end
