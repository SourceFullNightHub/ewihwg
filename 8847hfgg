                                else
                                    Tween(CFrame.new(-3795.6423339844, 105.88877105713, -11421.307617188))
                                    if CheckDistance(CFrame.new(-3795.6423339844, 105.88877105713, -11421.307617188)) <= 300 and not CheckEnemies("Tide Keeper") then
                                        notify("Script", "Hop Server Find Boss 'Tide Keeper'", 5)
                                        wait(_G.S.HopDelay)
                                        HopServer()
                                    end
                                end
                            elseif CheckBackpack("Water Key") then
                                repeat task.wait()
                                    Tween(CFrame.new(-2604.6958, 239.432526, -10315.1982, 0.0425701365, 0, -0.999093413, 0, 1, 0, 0.999093413, 0, 0.0425701365))
                                until CheckDistancebehon(CFrame.new(-2604.6958, 239.432526, -10315.1982, 0.0425701365, 0, -0.999093413, 0, 1, 0, 0.999093413, 0, 0.0425701365), 10)
                                game:GetService("ReplicatedStorage").Remotes.CommF_:InvokeServer("BuySharkmanKarate")
                            end
                        elseif plr.Backpack:FindFirstChild("Fishman Karate").Level.Value < 400 or plr.Character:FindFirstChild("Fishman Karate").Level.Value < 400 then
                            StartFarmToggle:Set(true)
                        end
                    end
                elseif game:GetService("ReplicatedStorage").Remotes.CommF_:InvokeServer("BuySharkmanKarate", true) == 1 then
                    game:GetService("ReplicatedStorage").Remotes.CommF_:InvokeServer("BuySharkmanKarate")
                    AutoSharkmanToggle:Set(false)
                end
            end)
        end
    end
end)

