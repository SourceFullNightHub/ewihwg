local GhoulRaceToggle = I:CreateToggle({
    Name = "Auto Ghoul Race",
    Description = "Auto Kill Cursed Captain To Get Torch Trade With NPC In CursedShip to get Race Ghoul(function Only Sea 2)",
    Default = _G.S.DitGhoul,
    CallBack = function(Value)
        _G.S.DitGhoul = Value
        SaveSettings()
    end
})

I:CreateToggle({
    Name = "Auto Ghoul Race(Hop)",
    Description = "If Not Cursed Captian then Hop Servers(function Only Sea 2)",
    Default = _G.S.HopB,
    CallBack = function(Value)
        _G.S.HopB = Value
        SaveSettings()
    end
})

spawn(function()
    while task.wait() do
        if _G.S.DitGhoul and Sea2 then
            if plr.Data.Race.Value ~= "Ghoul" then
                if not plr.Backpack:FindFirstChild("Hellfire Torch") and not plr.Character:FindFirstChild("Hellfire Torch") then
                    if CheckEnemies("Cursed Captain") then
                        KillMob("Cursed Captain", _G.S.DitGhoul)
                    else
                        Tween(CFrame.new(916.928589, 181.092773, 33422))
                        if CheckDistance(CFrame.new(916.928589, 181.092773, 33422)) <= 300 then
                            if _G.S.HopB == true then
                                Library:Notify({
                                    Title = "Night Hub",
                                    Content = "Not Have Boss, Hop Servers",
                                    Time = 1,
                                })
                                wait(_G.S.HopDelay)
                                HopServer()
                            end
                        end
                    end
                elseif plr.Backpack:FindFirstChild("Hellfire Torch") or plr.Character:FindFirstChild("Hellfire Torch") then
                    if CheckDistancebehon(CFrame.new(923.748596, 125.529785, 33454.4883), 5) then
                        game:GetService("ReplicatedStorage").Remotes.CommF_:InvokeServer("Ectoplasm", "Change")
                        Library:Notify({
                            Title = "Night Hub",
                            Content = "Change Race To Ghoul",
                            Time = 1,
                        })
                        wait(5)
                    else
                        Tween(CFrame.new(923.748596, 125.529785, 33454.4883))
                        Library:Notify({
                            Title = "Night Hub",
                            Content = "Teleport To NPC In Cursed Ship",
                            Time = 1,
                        })
                        wait(5)
                    end
                end
            elseif plr.Data.Race.Value == "Ghoul" then
                GhoulRaceToggle:Set(false)
                Library:Notify({
                    Title = "Night Hub",
                    Content = "Your Race Is Ghoul!!!",
                    Time = 1,
                })
                wait(5)
            end
        end
    end
end)

