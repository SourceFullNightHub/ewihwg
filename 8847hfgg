function WaitHRP(q0) 
    if not q0 then return end
    return q0.Character:WaitForChild("HumanoidRootPart", 9) 
end 
function Tween(CFg)
    if game.Players.LocalPlayer.Character.Humanoid.Health > 0 and game.Players.LocalPlayer.Character:FindFirstChild("HumanoidRootPart") then
        Distance = (CFg.Position - game.Players.LocalPlayer.Character.HumanoidRootPart.Position).Magnitude
        game.Players.LocalPlayer.Character:WaitForChild("HumanoidRootPart", 9)
        game.Players.LocalPlayer.Character:WaitForChild("Head", 9)
        if not CFg then return end
        if not plr.Character.HumanoidRootPart:FindFirstChild("NClip") then
            local NClip = Instance.new("BodyVelocity", plr.Character.HumanoidRootPart)
            NClip.Name = "NClip"
            NClip.MaxForce = Vector3.new(math.huge, math.huge, math.huge)
            NClip.Velocity = Vector3.new(0, 0, 0)
        end
        if not plr.Character:FindFirstChild("TweenSmooth") then
            local TweenSmooth = Instance.new("Part", plr.Character) -- Create part
            TweenSmooth.Size = Vector3.new(10,1,10)
            TweenSmooth.Name = "TweenSmooth"
            TweenSmooth.Anchored = true
            TweenSmooth.Transparency = 1
            TweenSmooth.CanCollide = false
            TweenSmooth.CFrame = WaitHRP(plr).CFrame 
            TweenSmooth:GetPropertyChangedSignal("CFrame"):Connect(function()
                task.wait(0.01)
                WaitHRP(plr).CFrame = TweenSmooth.CFrame
            end)
        end
        tween = game:GetService("TweenService"):Create(game.Players.LocalPlayer.Character.TweenSmooth, TweenInfo.new(Distance / 180, Enum.EasingStyle.Linear),{CFrame = CFg})
        local NearestEntraceFunction = NearestEntrace(CFg)
		if NearestEntraceFunction then
            tween:Cancel()
			RETeleport(NearestEntraceFunction)
		end
        if Distance <= 250 then
            tween:Cancel()
            game.Players.LocalPlayer.Character.TweenSmooth.CFrame = CFg
        end
		tween:Play()
    end
end
Type = 1
spawn(function()
    while wait(.1) do
        if Type == 1 then
            PosFarm = CFrame.new(0, 20 ,15)
		elseif Type == 2 then
			PosFarm = CFrame.new(0, 12 ,-15)
		elseif Type == 3 then
			PosFarm = CFrame.new(15, 16 ,0)
		elseif Type == 4 then
			PosFarm = CFrame.new(0, 12 ,15)	
		elseif Type == 5 then
			PosFarm = CFrame.new(-15, 16 ,0)
		elseif Type == 6 then
			PosFarm = CFrame.new(15, 20 ,0)
        end
    end
end)
spawn(function()
    while wait(.1) do
        Type = 1
        wait(0.5)
        Type = 2
        wait(0.5)
        Type = 3
        wait(0.5)
        Type = 4
        wait(0.5)
        Type = 5
        wait(0.5)
    end
end)
function EnableBuso()
    if not plr:FindFirstChild("HasBuso") then
        game.ReplicatedStorage.Remotes.CommF_:InvokeServer("Buso")
    end
end
function Equip(Tool)
    if plr.BackPack:FindFirstChild(Tool) then
        local is = plr.BackPack:FindFirstChild(Tool)
        plr.Character.Humanoid:EquipTool(is)
    end
end
function CheckEnemies(Mob)
    v = game.Workspace.Enemies:FindFirstChild(Mob)
    if v then
        if v.Humanoid.Health > 0 and v.Parent and v:FindFirstChild("HumanoidRootPart") then
            return true
        end
    end
    return false
end
function CheckQuestVisible()
    if plr.PlayerGui.Main.Quest.Visible == true then
        return true
    end
    return false
end
function CheckTextQuest(Tit)
    if string.find(game:GetService("Players").LocalPlayer.PlayerGui.Main.Quest.Container.QuestTitle.Title.Text, Tit) then
        return true
    end
    return false
end
function KillMob(Trget)
    for i, v in pairs(game.Workspace.Enemies:GetChildren()) do
        if v.Name == Trget then
            if v.Humanoid.Health > 0 and v.Parent and v:FindFirstChild("HumanoidRootPart") then
                repeat task.wait()
                    v.Humanoid.WalkSpeed = 0
                    v.HumanoidRootPart.CFrame = v.HumanoidRootPart.CFrame
                    v.HumanoidRootPart.Size = Vector3.new(1, 1, 1)
                    v.Head.CanCollide = false
                    v.HumanoidRootPart.CanCollide = false
                    EnableBuso()

                    Tween(v.HumanoidRootPart.CFrame * PosFarm)
                until v.Humanoid.Health <= 0 or not v.Parent
            end
        end
    end
end
function TakeQuest(cfRg, NameQuet, NumbQuest)
    if _G.Settings.BypassTele then
        if (cfRg.Position - plr.Character.HumanoidRootPart.Position).Magnitude > 2500 then
            Bypass(cfRg)
        elseif (cfRg.Position - plr.Character.HumanoidRootPart.Position).Magnitude < 2500 then
            Tween(cfRg)
        end
    else
        Tween(cfRg)
    end
    if (plr.Character.HumanoidRootPart.Position - cfRg.Position).Magnitude <= 10 then
        game:GetService("ReplicatedStorage").Remotes.CommF_:InvokeServer("StartQuest", NameQuet, NumbQuest)
    end
end

