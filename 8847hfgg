spawn(function()
    while wait() do
        if _G.S.ChichBartilo and CheckSea(2) then
            if plr.Data.Level.Value >= 800 and game:GetService("ReplicatedStorage").Remotes.CommF_:InvokeServer("BartiloQuestProgress","Bartilo") == 0 then
                if CheckQuestVisible() and CheckTextQuest("Swan Pirates") and CheckTextQuest("50") then
                    if CheckEnemiesUpgrade("Swan Pirates") then
                        KillMob("Swan Pirates", _G.S.ChichBartilo)
                    end
                else
                    repeat
                        NoClip = true
                        Tween(CFrame.new(932.624451, 156.106079, 1180.27466))
                    until CheckDistancebehon(CFrame.new(932.624451, 156.106079, 1180.27466), 5)
                    game:GetService("ReplicatedStorage").Remotes.CommF_:InvokeServer("StartQuest","BartiloQuest",1)
                end
            elseif plr.Data.Level.Value >= 800 and game:GetService("ReplicatedStorage").Remotes.CommF_:InvokeServer("BartiloQuestProgress","Bartilo") == 1 then
                if CheckEnemiesUpgrade("Jeremy") then
                    KillMob("Jeremy", _G.S.ChichBartilo)
                end
            elseif plr.Data.Level.Value >= 800 and game:GetService("ReplicatedStorage").Remotes.CommF_:InvokeServer("BartiloQuestProgress","Bartilo") == 2 then
                repeat 
                    NoClip = true
                    Tween(CFrame.new(-1850.49329, 13.1789551, 1750.89685)) 
                    wait() 
                until not _G.S.ChichBartilo or (plr.Character.HumanoidRootPart.Position-Vector3.new(-1850.49329, 13.1789551, 1750.89685)).Magnitude <= 10
                wait(1)
                repeat 
                    NoClip = true
                    Tween(CFrame.new(-1858.87305, 19.3777466, 1712.01807)) 
                    wait() 
                until not _G.S.ChichBartilo or (plr.Character.HumanoidRootPart.Position-Vector3.new(-1858.87305, 19.3777466, 1712.01807)).Magnitude <= 10
                wait(1)
                repeat 
                    NoClip = true
                    Tween(CFrame.new(-1803.94324, 16.5789185, 1750.89685)) 
                    wait() 
                until not _G.S.ChichBartilo or (plr.Character.HumanoidRootPart.Position-Vector3.new(-1803.94324, 16.5789185, 1750.89685)).Magnitude <= 10
                wait(1)
                repeat 
                    NoClip = true
                    Tween(CFrame.new(-1858.55835, 16.8604317, 1724.79541)) 
                    wait() 
                until not _G.S.ChichBartilo or (plr.Character.HumanoidRootPart.Position-Vector3.new(-1858.55835, 16.8604317, 1724.79541)).Magnitude <= 10
                wait(1)
                repeat 
                    NoClip = true
                    Tween(CFrame.new(-1869.54224, 15.987854, 1681.00659)) 
                    wait() 
                until not _G.S.ChichBartilo or (plr.Character.HumanoidRootPart.Position-Vector3.new(-1869.54224, 15.987854, 1681.00659)).Magnitude <= 10
                wait(1)
                repeat 
                    NoClip = true
                    Tween(CFrame.new(-1800.0979, 16.4978027, 1684.52368)) 
                    wait() 
                until not _G.S.ChichBartilo or (plr.Character.HumanoidRootPart.Position-Vector3.new(-1800.0979, 16.4978027, 1684.52368)).Magnitude <= 10
                wait(1)
