local Windows = Library:CreateWindow({ Title = "Night Hub", Creator = "! Nightx" })

function notify(N, a, h)
    if N == nil then
        N = "Night Hub"
    end
    if a == nil then
        a = "Content"
    end
    if h == nil then
        h = 5
    end
    Library:MakeNotify({Name = N, Content = a, Time = h,})
end

