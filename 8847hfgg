local CastlePos = CFrame.new(-5496.17432, 313.768921, -2841.53027)
spawn(function()
    while wait() do
        if _G.Settings.AutoCastleRaid and Sea3 then
            if (CastlePos.Position - Your.Character.HumanoidRootPart.Position).Magnitude <= 500 then
                for i,v in pairs(game:GetService("Workspace").Enemies:GetChildren()) do
                    if _G.Settings.AutoCastleRaid and v:FindFirstChild("HumanoidRootPart") and v:FindFirstChild("Humanoid") and v.Humanoid.Health > 0 then
                        if (v.HumanoidRootPart.Position - Your.Character.HumanoidRootPart.Position).Magnitude < 2000 then
                            repeat wait()
                                EnableBuso()
                                EquipTool(ToolFarm)
                                v.Humanoid.WalkSpeed = 0
                                v.HumanoidRootPart.Size = Vector3.new(1, 1, 1)
                                v.HumanoidRootPart.CanCollide = false
                                v.Head.CanCollide = false
                                Tween(v.HumanoidRootPart.CFrame * CFrame.new(2, 20, 2))
                            until v.Humanoid.Health <= 0 or not v.Parent or not _G.Settings.AutoCastleRaid
                        end
                    end
                end
            else
                if _G.Settings.BypassTele then
                    if (CastlePos.Position - game.Players.LocalPlayer.Character.HumanoidRootPart.Position).Magnitude > 2000 then
                        game:GetService("ReplicatedStorage").Remotes.CommF_:InvokeServer("requestEntrance",Vector3.new(-5005.29834, 314.858734, -3025.00659))
                    elseif (CastlePos.Position - game.Players.LocalPlayer.Character.HumanoidRootPart.Position).Magnitude < 2000 then
                        Tween(CastlePos)
                    end
                else
                    Tween(CastlePos)
                end
            end
        end
    end
end)

