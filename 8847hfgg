    if tool then
        wait(0.5)
        game.Players.LocalPlayer.Character.Humanoid:EquipTool(tool)
    end
end

function CheckSea(yamatekudasai)
    if game.PlaceId == 2753915549 then
        if yamatekudasai == 1 then
            return true
        end
    elseif game.PlaceId == 4442272183 then
        if yamatekudasai == 2 then
            return true
        end
    elseif game.PlaceId == 7449423635 then
        if yamatekudasai == 3 then
            return true
        end
    end
    return false
end

