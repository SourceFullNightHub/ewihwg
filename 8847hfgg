E:CreateToggle({
    Name = "Reset If Your Boat Have Ben Destroy",
    Description = "Nah, i not think english",
    Default = _G.S.Rest,
    CallBack = function(Value)
        _G.S.Rest = Value
        SaveSettings()
    end
})

spawn(function()
    while wait() do
        if _G.S.StartSea and GetYourShip() then
            if game.Players.LocalPlayer.Character:WaitForChild("Humanoid").Sit == false then
                if not CheckEnemies("Shark") and not CheckEnemies("Terrorshark") and not CheckEnemies("Fish Crew Member") and not CheckSeaBeast() and not GetShipTarget() then
                    table.foreach(GetYourShip():GetDescendants(), function(a, child)
                        if child:IsA("BasePart") or child:IsA("MeshPart") or child:IsA("Part") then
                            child.CanCollide = false
                        end
                    end)
                    TPP(GetYourShip():FindFirstChild("VehicleSeat").CFrame * CFrame.new(0, 1, 0))
                end
            end
        end
    end
end)

spawn(function()
    while wait() do
        if _G.S.StartSea then
            pcall(function()
                if not GetYourShip() then
                    if _G.S.Rest then
                        if CheckDistancelonhon(CFrame.new(-16927.451171875, 9.0863618850708, 433.8642883300781), 2500) then
                            Bypass(CFrame.new(-16927.451171875, 9.0863618850708, 433.8642883300781))
                        elseif CheckDistancebehon(CFrame.new(-16927.451171875, 9.0863618850708, 433.8642883300781), 2500) then
                            TPP(CFrame.new(-16927.451171875, 9.0863618850708, 433.8642883300781))
                        end
                    else
                        TPP(CFrame.new(-16927.451171875, 9.0863618850708, 433.8642883300781))
                    end
                    if (CFrame.new(-16927.451171875, 9.0863618850708, 433.8642883300781).Position - game.Players.LocalPlayer.Character.HumanoidRootPart.Position).magnitude <= 10 then
                        local args = {
                            [1] = "BuyBoat",
                            [2] = "PirateBrigade"
                        }
                        game:GetService("ReplicatedStorage").Remotes.CommF_:InvokeServer(unpack(args))
                    end
                end
            end)
        end
    end
end)

spawn(function()
    while wait() do
        if _G.S.StartSea then
            if GetYourShip() then
                if game.Players.LocalPlayer.Character:WaitForChild("Humanoid").Sit == true then
                    repeat wait()
                        if (CFrame.new(-17013.80078125, 10.962434768676758, 438.0169982910156).Position - game.Players.LocalPlayer.Character.HumanoidRootPart.Position).magnitude <= 10 then
                            TPB(CFrame.new(-44842.3945, 10.7790766, 16911.3477))
                        end
                    until GetShipTarget() or GetShipTarget() or not CheckSeaBeast() or CheckEnemies("Shark") or CheckEnemies("Terrorshark") or CheckEnemies("Piranha") or CheckEnemies("Fish Crew Member") or plr.Character.Humanoid.Sit == false or _G.S.StartSea == false
                end
            end
        end
    end
end)

spawn(function()
    while wait() do
        if _G.S.StartSea then
            if CheckEnemies("Shark") or CheckEnemies("Terrorshark") or CheckEnemies("Piranha") or CheckEnemies("Fish Crew Member") or GetShipTarget() or CheckSeaBeast() or GetShipTarget() then
                plr.Character.Humanoid.Sit = false
            end
        end
    end
end)

E:CreateToggle({
    Name = "Safe Farm",
    Description = "If Low Health tween to Y",
    Default = _G.S.Y,
    CallBack = function(Value)
        _G.S.Y = Value
        SaveSettings()
    end
})

spawn(function()
    while task.wait() do
        if _G.S.Y then
            if plr.Character.Humanoid.Health < 4000 then
                CFSafe = CFrame.new(20, 300, 20)
            elseif plr.Character.Humanoid.Health > 4000 then
                CFSafe = CFrame.new(2, 20, 2)
            end
        else
            CFSafe = CFrame.new(2, 20, 2)
        end
    end
end)

local FarmmingSea = E:CreateSection({Text = 'Farmming Sea Event'})

E:CreateToggle({
    Name = "Auto Shark",
    Description = "Auto Kill Shark",
    Default = _G.S.KillShark,
    CallBack = function(Value)
        _G.S.KillShark = Value
        SaveSettings()
    end
})

