                                v.HumanoidRootPart.CanCollide = false
                                v.Head.CanCollide = false
                                Tween(CFrame.new(448.46756, 199.356781, -441.389252)) -- AFK I TM!!!
                            until _G.Settings.AutoFactory == false or v.Humanoid.Health <= 0 or not v.Parent
                        end
                    end
                end
            else
                if _G.Settings.BypassTele then
                    if (CFrame.new(448.46756, 199.356781, -441.389252).Position - Your.Character.HumanoidRootPart.Position).Magnitude > 2000 then
                        Bypass(CFrame.new(448.46756, 199.356781, -441.389252))
                    elseif (CFrame.new(448.46756, 199.356781, -441.389252).Position - Your.Character.HumanoidRootPart.Position).Magnitude < 2000 then
                        Tween(CFrame.new(448.46756, 199.356781, -441.389252))
                    end
                else
                    Tween(CFrame.new(448.46756, 199.356781, -441.389252))
                end
            end        
        end
    end
end)

