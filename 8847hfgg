    pcall(function()
        game:GetService("RunService").Stepped:Connect(function()
            if _G.S.AutoChestInstance or _G.S.AutoFarmMastery or _G.S.SummonRipIndra or _G.S.AutoRaceV2V3 or _G.S.AutoRaceV2V3 or _G.S.AutoChestTween  or _G.S.PilotHelmet or _G.S.SerpentBow or _G.S.AutoLei or _G.S.AutoTwinHook or _G.S.AutoCavander or _G.S.AutoBuddySword or _G.S.AutoDoughKing or _G.S.KillTrialBeo or _G.S.AutoTrainRace or _G.S.AutoTrials or _G.S.AutoPullLever or _G.S.StFarm or _G.S.PiratesRaid or _G.S.ChichElite or _G.S.DitFactory or _G.S.TakenSaber or _G.S.TakenPole or _G.S.BopdaiCyborg or _G.S.BucacSea2 or _G.S.ChichBartilo or _G.S.DitGhoul or _G.S.Swanlacaichogi or _G.S.chimdenvschimtrang or _G.S.Thuyentruongbidien or _G.S.dodoclonv2 or _G.S.Bayyyyyyyyyy or _G.S.DonateForme or _G.S.TweentoIsland or TweenPly or _G.KillPlayer or _G.NearestKill or _G.S.StartSea or _G.S.AutoSeaBeast or _G.S.AutoGhoshShip or _G.S.AutoShip or _G.S.KillShark or _G.S.KillTerrorShark or _G.S.KillFishAndPirnha or NoClip == true then
                for i,v in pairs(game:GetService("Players").LocalPlayer.Character:GetDescendants()) do
