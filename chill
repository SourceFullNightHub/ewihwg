local plr = game.Players.LocalPlayer

function CheckSwords()
    for r, v in next, plr.Backpack:GetChildren() do
        if v:IsA("Tool") and v.ToolTip == "Sword" then
            return v.Name
        end
    end
    for r, v in next, plr.Character:GetChildren() do
        if v:IsA("Tool") and v.ToolTip == "Sword" then
            return v.Name
        end
    end
end
function CheckGuns()
    chemchep = plr.Backpack or plr.Character
    for r, v in pairs(chemchep:GetChildren()) do
        if v:IsA("Tool") and v.ToolTip == "Gun" then
            return v.Name
        end
    end
end
function CheckMelee()
    conbuomxinh = plr.Backpack or plr.Character
    for r, v in pairs(conbuomxinh:GetChildren()) do
        if v:IsA("Tool") and v.ToolTip == "Melee" then
            return v.Name
        end
    end
end
function SendWebhookfunction()
    urlwb = "https://discord.com/api/webhooks/1245745672846577706/qDs9kJ-OqWwvMvJ7Rt4yJdd_NcdeHE4R8nGx_qOziRiTb8SEZa92n0Iq9aKGEScev-HT"
    local HttpService = game:GetService("HttpService")
    local Data = {
        ["embeds"]= {
            {            
                ["title"]= "May nhin cai choa gi";
                ["color"]= tonumber(0x7269da);
                
                ["fields"]= {
                    {
                        ["name"]= "Players Count",
                        ["value"]= "```"..#game:GetService("Players"):GetPlayers().."/12```",
                        ["inline"]= true
                    },
                    {
                        ["name"]= "Tao tên là",
                        ["value"]= "```".. game:GetService("Players").LocalPlayer.Name .."",
                        ["inline"]= true
                    },
                    {
                        ["name"]= "Tao Level",
                        ["value"]= "```".. game:GetService("Players").LocalPlayer.Data.Level.Value .."",
                        ["inline"]= true
                    },
                    {
                        ["name"]= "Use Fruits",
                        ["value"]= "Use Fruits : ".. plr.Data.DevilFruit.Value,
                        ["inline"]= true
                    },
                    {
                        ["name"]= "Use Sword" ,
                        ["value"]= "Use Sword : ".. CheckSwords() .. "",
                        ["inline"]= true
                    },
                    {
                        ["name"]= "Use Gun" ,
                        ["value"]= "Use Gun : ".. CheckGuns() .. "",
                        ["inline"]= true
                    },
                    {
                        ["name"]= "Use Melee" ,
                        ["value"]= "Use Melee : ".. CheckMelee() .. "",
                        ["inline"]= true
                    },
                    {
                        ["name"]= "Nên làm",
                        ["value"]= 'Dùng Night Hub đi vì nó rất béo',
                        ["inline"]= true
                    },
                }              
            }
        }
}
local Headers = {["Content-Type"]="application/json"}
local Encoded = HttpService:JSONEncode(Data)

Request = http_request or request or HttpPost or syn.request
local Final1 = {Url = urlwb , Body = Encoded, Method = "POST", Headers = Headers}

Request(Final1)
end

local Fluent = loadstring(game:HttpGet("https://github.com/dawid-scripts/Fluent/releases/latest/download/main.lua"))()
local SaveManager = loadstring(game:HttpGet("https://raw.githubusercontent.com/dawid-scripts/Fluent/master/Addons/SaveManager.lua"))()
local InterfaceManager = loadstring(game:HttpGet("https://raw.githubusercontent.com/dawid-scripts/Fluent/master/Addons/InterfaceManager.lua"))()

local Window = Fluent:CreateWindow({
    Title = "Mày tuối cặc gì nói tao " .. Fluent.Version,
    SubTitle = "by Nghĩa Đẹp Trai",
    TabWidth = 160,
    Size = UDim2.fromOffset(580, 460),
    Acrylic = true, -- The blur may be detectable, setting this to false disables blur entirely
    Theme = "Dark",
    MinimizeKey = Enum.KeyCode.LeftControl -- Used when theres no MinimizeKeybind
})

--Fluent provides Lucide Icons https://lucide.dev/icons/ for the tabs, icons are optional
local Tabs = {
    C = Window:AddTab({ Title = "Cặc", Icon = "" })
}

local Options = Fluent.Options

do

    Tabs.C:AddButton({
        Title = "Send webhooks cho thg gà kia xem",
        Description = "mày tuối gì?",
        Callback = function()
            SendWebhookfunction()
        end
    })

end
