spawn(function()
    while wait() do
    if _G.AutoBountyNightHub then
        if not game:GetService("Players").LocalPlayer.Character:FindFirstChild("HasBuso") then
            game:GetService("ReplicatedStorage").Remotes.CommF_:InvokeServer("Buso")
            end
        end
    end
end)
local Myhealth = game.Players.LocalPlayer.Character.Humanoid.Health
spawn(function()
    while wait() do
    if _G.AutoBountyNightHub then
        for i,v in pairs(game:GetService("Workspace").Characters:GetChildren()) do
            if v.Name ~= game.Players.LocalPlayer.Name then
                if v.Humanoid.Health > 0 and (game.Players.LocalPlayer.Character.HumanoidRootPart.Position - v.HumanoidRootPart.Position).Magnitude <= 7000 then
                    plyselecthunthelpold = v.Humanoid.Health
                    TargetSelectHunt = v.Humanoid
                    repeat task.wait()
                        Converted["_TextLabel1"].Text = "Target Name : "..v.Name..""
                        Converted["_TextLabel2"].Text = "Target Bounty : " ..v.leaderstats["Bounty/Honor"].Value..""
                        NameTarget = v.Name
                        v.HumanoidRootPart.Size = Vector3.new(100, 100, 100)
                        v.HumanoidRootPart.CanCollide = false
                        topos(v.HumanoidRootPart.CFrame * CFrame.new(5,12,5))
                        Useskill = true
                    until _G.AutoBountyNightHub == false or v.Humanoid.Health <= 0 or not v.Parent
                        Useskill = false
                    end
                end
            end
        end
    end
end)

spawn(function()
    while wait() do
    if _G.AutoBountyNightHub then
        if Distance <= 50 then
            if Useskill then
                autoskill()
                end
            end
        end
    end
end)

spawn(function()
    pcall(function()
        while wait() do
            if _G.AutoBountyNightHub then
                if TargetSelectHunt ~= nil then
                    if StartCheckTarget then 
                        wait(.1)
                            if TargetSelectHunt.Health == TargetSelectHunt.MaxHealth or TargetSelectHunt.Health >= plyselecthunthelpold then
                                NextPlayers = true
                                TargetSelectHunt = nil
                            end
                        end
                    end
                end
            end
        end)
    end)
































